{'id_' : 'me|m11|d06_en_US_dedes|meInsert2', 'text' : 'whose memory we observe;'}
{'id_' : 'me|m11|d06_en_US_dedes|meNote|text', 'text' : ''}
{'id_' : 'me|m11|d06_en_US_dedes|meLE|commemoration1', 'text' : 'Our Father among the Saints Paul the Confessor, Archbishop of Constantinople.'}
{'id_' : 'me|m11|d06_en_US_dedes|meLE|commemoration2', 'text' : ''}
