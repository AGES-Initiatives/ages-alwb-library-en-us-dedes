{'id_' : 'me|m01|d22_en_US_dedes|meDate|text', 'text' : 'January 22'}
{'id_' : 'me|m01|d22_en_US_dedes|meCommemoration|text', 'text' : 'Memory of the Holy Apostle Timothy, and the Holy Devout Martyr Anastasios the Persian'}
{'id_' : 'me|m01|d22_en_US_dedes|meShortCommemoration1|text', 'text' : ''}
{'id_' : 'me|m01|d22_en_US_dedes|meCom1|title', 'text' : 'For St. Timothy.'}
{'id_' : 'me|m01|d22_en_US_dedes|meCom2|title', 'text' : 'For St. Anastasios.'}
{'id_' : 'me|m01|d22_en_US_dedes|meInsert1', 'text' : 'the holy and glorious Apostle Timothy, and the holy devout martyr Anastasios the Persian,'}
{'id_' : 'me|m01|d22_en_US_dedes|meInsert2', 'text' : 'whose memory we observe;'}
{'id_' : 'me|m01|d22_en_US_dedes|meNote|text', 'text' : ''}
{'id_' : 'me|m01|d22_en_US_dedes|meLE|commemoration1', 'text' : 'The Holy Apostle Timothy. The Venerable Martyr Anastasios the Persian.'}
{'id_' : 'me|m01|d22_en_US_dedes|meLE|commemoration2', 'text' : ''}
