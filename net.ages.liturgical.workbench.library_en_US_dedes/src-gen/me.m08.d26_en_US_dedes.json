{'id_' : 'me|m08|d26_en_US_dedes|meMA|LaudsGlory|text', 'text' : ''}
{'id_' : 'me|m08|d26_en_US_dedes|meMA|LaudsTheotokion|mode', 'text' : 'Mode 2.'}
{'id_' : 'me|m08|d26_en_US_dedes|meMA|LaudsTheotokion|melody', 'text' : ''}
{'id_' : 'me|m08|d26_en_US_dedes|meMA|LaudsTheotokion|text', 'text' : 'O Mother of God, I have committed my every hope wholly unto you. Keep me under your shelter.'}
