{'id_' : 'me|m07|d22_en_US_dedes|alt1|meCommemoration|text', 'text' : 'Memory of the Holy and Glorious Virgin Martyr Markella of Chios'}
{'id_' : 'me|m07|d22_en_US_dedes|alt1|meCommemoration2|text', 'text' : ''}
{'id_' : 'me|m07|d22_en_US_dedes|alt1|meShortCommemoration1|text', 'text' : 'St. Markella of Chios'}
{'id_' : 'me|m07|d22_en_US_dedes|alt1|meCom1|title', 'text' : ''}
