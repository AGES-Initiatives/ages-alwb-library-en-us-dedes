{'id_' : 'me|m01|d24_en_US_dedes|meNote|text', 'text' : ''}
{'id_' : 'me|m01|d24_en_US_dedes|meLE|commemoration1', 'text' : 'Our Venerable Mother Xenia.'}
{'id_' : 'me|m01|d24_en_US_dedes|meLE|commemoration2', 'text' : ''}
