{'id_' : 'me|m01|d24_en_US_dedes|meCommemoration|text', 'text' : 'Memory of our Devout Mother Xenia'}
{'id_' : 'me|m01|d24_en_US_dedes|meShortCommemoration1|text', 'text' : 'Memory of our Devout Mother Xenia'}
{'id_' : 'me|m01|d24_en_US_dedes|meCom1|title', 'text' : 'For St. Xenia.'}
{'id_' : 'me|m01|d24_en_US_dedes|meCom2|title', 'text' : ''}
