{'id_' : 'oc|m3|d1_en_US_dedes|ocMA|C3Acrostic|text', 'text' : ''}
{'id_' : 'oc|m3|d1_en_US_dedes|ocMA|OdeS1C1H|mode', 'text' : 'Mode 3.'}
