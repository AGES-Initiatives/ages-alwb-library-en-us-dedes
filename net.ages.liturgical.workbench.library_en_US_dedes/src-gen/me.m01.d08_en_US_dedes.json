{'id_' : 'me|m01|d08_en_US_dedes|meVE|Aposticha2|text', 'text' : ''}
{'id_' : 'me|m01|d08_en_US_dedes|meVE|Aposticha3|mode', 'text' : 'Mode pl. 2.'}
