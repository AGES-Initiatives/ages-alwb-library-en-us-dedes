{'id_' : 'me|m04|d25_en_US_dedes|meInsert1', 'text' : 'the holy and glorious Apostle and Evangelist Mark,'}
{'id_' : 'me|m04|d25_en_US_dedes|meInsert2', 'text' : 'whose memory we celebrate, '}
{'id_' : 'me|m04|d25_en_US_dedes|meNote|text', 'text' : ''}
{'id_' : 'me|m04|d25_en_US_dedes|meLE|commemoration1', 'text' : 'The Holy Apostle and Evangelist Mark.'}
{'id_' : 'me|m04|d25_en_US_dedes|meLE|commemoration2', 'text' : ''}
