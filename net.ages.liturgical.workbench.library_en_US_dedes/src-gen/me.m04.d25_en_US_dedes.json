{'id_' : 'me|m04|d25_en_US_dedes|meVE|Stichera06|mode', 'text' : 'Mode 1.'}
{'id_' : 'me|m04|d25_en_US_dedes|meVE|Stichera06|melody', 'text' : 'O Martyrs, extolled. '}
{'id_' : 'me|m04|d25_en_US_dedes|meVE|Stichera06|text', 'text' : ''}
