{'id_' : 'oc|m7|d2_en_US_dedes|ocMA|OdeW8C14|mode', 'text' : 'Grave Mode.'}
{'id_' : 'oc|m7|d2_en_US_dedes|ocMA|OdeW8C14|ode', 'text' : 'Ode viii.'}
