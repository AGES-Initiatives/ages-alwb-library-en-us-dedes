{'id_' : 'me|m02|d03_en_US_dedes|meLE|commemoration1', 'text' : 'The Holy and Just Symeon who Received God.'}
{'id_' : 'me|m02|d03_en_US_dedes|meLE|commemoration2', 'text' : ''}
{'id_' : 'me|m02|d03_en_US_dedes|meHO|number', 'text' : ''}
