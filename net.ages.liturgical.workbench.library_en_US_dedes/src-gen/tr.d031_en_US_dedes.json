{'id_' : 'tr|d031_en_US_dedes|trMA|AposIdiomelon1|mode', 'text' : 'Mode pl. 1.'}
{'id_' : 'tr|d031_en_US_dedes|trMA|AposIdiomelon1|melody', 'text' : 'Idiomelon.'}
