{'id_' : 'he|h|m3_en_US_dedes|OEkMiOnton|notmetered', 'text' : 'N/M (From non-existence.)'}
{'id_' : 'he|h|m3_en_US_dedes|OEkMiOnton|name', 'text' : 'From non-existence.'}
{'id_' : 'he|h|m3_en_US_dedes|OEkMiOnton|mode', 'text' : 'Mode 3.'}
