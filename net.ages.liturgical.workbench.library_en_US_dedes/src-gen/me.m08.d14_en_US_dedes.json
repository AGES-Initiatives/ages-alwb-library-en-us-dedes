{'id_' : 'me|m08|d14_en_US_dedes|meMA|Ode7C21|text', 'text' : ''}
{'id_' : 'me|m08|d14_en_US_dedes|meMA|Ode7C22|ode', 'text' : 'Ode vii.'}
