{'id_' : 'he|h|m8_en_US_dedes|TonEkNyktosAgnosias|text', 'text' : ''}
{'id_' : 'he|h|m8_en_US_dedes|TonEkPatrosProAionon|notmetered', 'text' : 'N/M ()'}
