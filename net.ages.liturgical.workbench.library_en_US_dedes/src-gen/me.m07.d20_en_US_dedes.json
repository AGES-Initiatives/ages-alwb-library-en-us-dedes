{'id_' : 'me|m07|d20_en_US_dedes|meCommemoration|text', 'text' : 'Memory of the Holy and Glorious Prophet Elias (Elijah) the Tishbite.'}
{'id_' : 'me|m07|d20_en_US_dedes|meShortCommemoration1|text', 'text' : 'Memory of the Holy Prophet Elias'}
{'id_' : 'me|m07|d20_en_US_dedes|meCom1|title', 'text' : ''}
