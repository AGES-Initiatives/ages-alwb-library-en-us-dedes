{'id_' : 'me|m07|d20_en_US_dedes|meMA|Ode5C1T|ode', 'text' : 'Ode v.'}
{'id_' : 'me|m07|d20_en_US_dedes|meMA|Ode5C1T|mode', 'text' : 'Mode 2.'}
