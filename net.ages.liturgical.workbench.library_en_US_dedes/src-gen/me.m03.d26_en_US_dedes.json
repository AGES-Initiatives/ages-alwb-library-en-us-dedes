{'id_' : 'me|m03|d26_en_US_dedes|meCommemoration|text', 'text' : 'The Synaxis in Honor of the Chief Commander Gabriel.'}
{'id_' : 'me|m03|d26_en_US_dedes|meShortCommemoration1|text', 'text' : 'Synaxis in Honor of the Chief Commander Gabriel'}
{'id_' : 'me|m03|d26_en_US_dedes|meShortCommemoration2|text', 'text' : 'Leave-taking of Annunciation'}
