{'id_' : 'me|m05|d08_en_US_dedes|meVE|Lity1|text', 'text' : ''}
{'id_' : 'me|m05|d08_en_US_dedes|meVE|Lity2|poet', 'text' : 'By Germanos.'}
{'id_' : 'me|m05|d08_en_US_dedes|meVE|Lity2|mode', 'text' : 'Mode 1.'}
