{'id_' : 'me|m12|d26_en_US_dedes|meNote|text', 'text' : ''}
{'id_' : 'me|m12|d26_en_US_dedes|meLE|commemoration1', 'text' : 'The Synaxis in of our All-Holy Lady Theotokos and Ever-Virgin Mary.'}
{'id_' : 'me|m12|d26_en_US_dedes|meLE|commemoration2', 'text' : ''}
