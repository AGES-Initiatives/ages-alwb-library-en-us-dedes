{'id_' : 'pe|d113_en_US_dedes|peVE|LityGlory|mode', 'text' : 'Mode 3.'}
{'id_' : 'pe|d113_en_US_dedes|peVE|LityGlory|melody', 'text' : 'Idiomelon.'}
