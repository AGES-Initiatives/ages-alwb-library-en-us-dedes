{'id_' : 'me|m01|d15_en_US_dedes|meDate|text', 'text' : 'January 15'}
{'id_' : 'me|m01|d15_en_US_dedes|meCommemoration|text', 'text' : 'Memory of our Devout Father Paul of Thebes and Saint John the Hut-Dweller.'}
{'id_' : 'me|m01|d15_en_US_dedes|meShortCommemoration1|text', 'text' : ''}
{'id_' : 'me|m01|d15_en_US_dedes|meCom1|title', 'text' : 'For St. Paul.'}
{'id_' : 'me|m01|d15_en_US_dedes|meCom2|title', 'text' : 'For St. John.'}
{'id_' : 'me|m01|d15_en_US_dedes|meInsert1', 'text' : 'our devout and God-bearing father John the Hut-Dweller,'}
{'id_' : 'me|m01|d15_en_US_dedes|meInsert2', 'text' : 'whose memory we observe;'}
{'id_' : 'me|m01|d15_en_US_dedes|meNote|text', 'text' : ''}
{'id_' : 'me|m01|d15_en_US_dedes|meLE|commemoration1', 'text' : 'Our Devout Fathers Paul of Thebes and John the Hut-Dweller.'}
{'id_' : 'me|m01|d15_en_US_dedes|meLE|commemoration2', 'text' : ''}
