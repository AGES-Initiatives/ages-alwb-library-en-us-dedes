{'id_' : 'me|m02|d14_en_US_dedes|meCommemoration|text', 'text' : 'Memory of our Devout Father Afxentios'}
{'id_' : 'me|m02|d14_en_US_dedes|meShortCommemoration1|text', 'text' : 'Memory of our Devout Father Afxentios'}
{'id_' : 'me|m02|d14_en_US_dedes|meCom1|title', 'text' : ''}
{'id_' : 'me|m02|d14_en_US_dedes|meCom2|title', 'text' : ''}
{'id_' : 'me|m02|d14_en_US_dedes|meInsert1', 'text' : 'our devout Father Afxentios,'}
{'id_' : 'me|m02|d14_en_US_dedes|meInsert2', 'text' : 'whose memory we observe,'}
{'id_' : 'me|m02|d14_en_US_dedes|meNote|text', 'text' : ''}
{'id_' : 'me|m02|d14_en_US_dedes|meLE|commemoration1', 'text' : ''}
