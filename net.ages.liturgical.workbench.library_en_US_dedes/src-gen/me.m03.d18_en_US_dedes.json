{'id_' : 'me|m03|d18_en_US_dedes|meMA|Ode1C1H|ode', 'text' : 'Ode i.'}
{'id_' : 'me|m03|d18_en_US_dedes|meMA|Ode1C1H|mode', 'text' : 'Mode 4.'}
