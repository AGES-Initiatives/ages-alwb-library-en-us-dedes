{'id_' : 'me|m02|d19_en_US_dedes|alt1|meShortCommemoration1|text', 'text' : 'Memory of St. Philothea the Athenian'}
{'id_' : 'me|m02|d19_en_US_dedes|alt1|meCom1|title', 'text' : ''}
{'id_' : 'me|m02|d19_en_US_dedes|alt1|meCom2|title', 'text' : ''}
{'id_' : 'me|m02|d19_en_US_dedes|alt1|meInsert1', 'text' : 'the glorious devout martyr Philothea the Athenian,'}
{'id_' : 'me|m02|d19_en_US_dedes|alt1|meInsert2', 'text' : 'whose memory we celebrate;'}
{'id_' : 'me|m02|d19_en_US_dedes|alt1|meNote|text', 'text' : ''}
{'id_' : 'me|m02|d19_en_US_dedes|alt1|meLE|commemoration1', 'text' : 'Memory of the Devout Martyr Philothea the Athenian'}
{'id_' : 'me|m02|d19_en_US_dedes|alt1|meLE|commemoration2', 'text' : ''}
{'id_' : 'me|m02|d19_en_US_dedes|alt1|meHO|number', 'text' : ''}
