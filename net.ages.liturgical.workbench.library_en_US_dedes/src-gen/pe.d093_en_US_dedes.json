{'id_' : 'pe|d093_en_US_dedes|peVE|Aposticha2|mode', 'text' : 'Mode 3.'}
{'id_' : 'pe|d093_en_US_dedes|peVE|Aposticha2|melody', 'text' : ''}
{'id_' : 'pe|d093_en_US_dedes|peVE|Aposticha2|text', 'text' : 'We offer You, O Christ, an evening hymn with incense and spiritual songs. O Savior, have mercy on our souls.'}
{'id_' : 'pe|d093_en_US_dedes|peVE|Aposticha3|mode', 'text' : 'Mode 3.'}
{'id_' : 'pe|d093_en_US_dedes|peVE|Aposticha3|melody', 'text' : ''}
