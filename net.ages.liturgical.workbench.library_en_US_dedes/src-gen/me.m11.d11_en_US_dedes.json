{'id_' : 'me|m11|d11_en_US_dedes|meVE|SticGlory|poet', 'text' : 'By Byzantios.'}
{'id_' : 'me|m11|d11_en_US_dedes|meVE|SticGlory|mode', 'text' : 'Mode pl. 2.'}
{'id_' : 'me|m11|d11_en_US_dedes|meVE|SticGlory|melody', 'text' : ''}
