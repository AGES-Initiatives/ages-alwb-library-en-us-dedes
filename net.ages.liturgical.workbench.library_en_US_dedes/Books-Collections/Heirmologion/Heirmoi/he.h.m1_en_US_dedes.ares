OEnTiVatoOftheis.notmetered = "N/M (When You appeared.)"
OEnTiVatoOftheis.name = "When You appeared."
OEnTiVatoOftheis.mode = misc_en_US_dedes.Mode1
OEnTiVatoOftheis.ode = misc_en_US_dedes.Ode7
OEnTiVatoOftheis.text = "When You appeared in the burning bush to the law-giver Moses, * You prefigured there the childbirth of the Ever-Virgin. * Blessed are You, O Lord, the God of our fathers."

OFotisasTiEllampsei.notmetered = "N/M ()"
OFotisasTiEllampsei.name = ""
OFotisasTiEllampsei.mode = misc_en_US_dedes.Mode1
