SynToThavmasioStas.text = ""

ThavmatosYperfyous.notmetered = "N/M (Babylon's bedewing furnace.)"
ThavmatosYperfyous.name = "Babylon's bedewing furnace."
ThavmatosYperfyous.mode = misc_en_US_dedes.Mode1
ThavmatosYperfyous.ode = misc_en_US_dedes.Ode8
ThavmatosYperfyous.text = "Babylon's bedewing furnace bore the image * of an extraordinary wonder. * For it did not burn the youths it accepted, * nor did the fire of Divinity consume the Virgin's womb wherein it went. * So let us melodiously chant in praise: * Let all creation bless and extol the Lord, * and let it exalt Him supremely to the ages."

TheosOnEirinis.notmetered = "N/M (God of peace.)"
TheosOnEirinis.name = "God of peace."
TheosOnEirinis.mode = misc_en_US_dedes.Mode1
TheosOnEirinis.ode = misc_en_US_dedes.Ode5
TheosOnEirinis.text = "God of peace and Father of mercies, Your Son * You have sent unto us as Your messenger, * the Angel of great counsel who is granting peace. * Therefore having been guided to the light of godly knowledge, * waking from the night to dawn, we sing Your glory, O Lover of man."

TinFotoforonNefelin.notmetered = "N/M (The cloud that radiates.)"
TinFotoforonNefelin.name = "The cloud that radiates."
TinFotoforonNefelin.mode = misc_en_US_dedes.Mode1
TinFotoforonNefelin.ode = misc_en_US_dedes.Ode9
TinFotoforonNefelin.text = "The cloud that radiates brightly * and into which, as the rain fell * upon the fleece, the Lord of all * from the heavens descended * and was incarnated for us * becoming a man, though Unoriginate, * the immaculate Mother * of our God do we all magnify in song."

TinSinEirinin.notmetered = "N/M ()"
TinSinEirinin.name = ""
TinSinEirinin.mode = misc_en_US_dedes.Mode1
TinSinEirinin.ode = misc_en_US_dedes.Ode5
TinSinEirinin.text = ""

TinZoodochonPigin.notmetered = "N/M (A spring, abundantly flowing.)"
TinZoodochonPigin.name = "A spring, abundantly flowing."
TinZoodochonPigin.mode = misc_en_US_dedes.Mode1
TinZoodochonPigin.ode = misc_en_US_dedes.Ode9
TinZoodochonPigin.text = "A spring, abundantly flowing, that hosted life, * a golden lamp-stand that bore the unwaning Light, * living Temple, and all pure * tabernacle symbolize * the Theotokos, who is said to be wider * than earth and heaven. With hymns let us honor her."

ToFaeinonIminExanateilon.notmetered = "N/M (Let Your radiant.)"
ToFaeinonIminExanateilon.name = "Let Your radiant."
ToFaeinonIminExanateilon.mode = misc_en_US_dedes.Mode1
ToFaeinonIminExanateilon.ode = misc_en_US_dedes.Ode5
ToFaeinonIminExanateilon.text = "Let Your radiant and unapproachable * light rise like dawn on us, * who rise early in the morning to observe * the judgments of Your commands, O Christ our God * our Master who loves mankind."

ToPantanaktos.notmetered = "N/M (As the three young men.)"
ToPantanaktos.name = "As the three young men."
ToPantanaktos.mode = misc_en_US_dedes.Mode1
ToPantanaktos.ode = misc_en_US_dedes.Ode7
ToPantanaktos.text = "As the three young men were ensnared by the longing * for the King of all, hence they disdained the tyrant's * impious chatter, though he was very angry. * The infinite fire yielded to them, who uttered, * Blessed are You, O Master, unto the ages."
ToPantanaktos1.text = "As the three young men were ensnared by the longing <br>for the King of all, hence they disdained the tyrant's <br>impious chatter, though he was very angry. <br>The infinite fire yielded to them, who uttered, <br>Blessed are You, O Master, unto the ages."
ToPantanaktos2.text = ""
ToPantanaktos3.text = ""
ToPantanaktos4.text = ""
ToPantanaktos5.text = ""

ToProTonAionon.notmetered = "N/M (To the Son.)"
ToProTonAionon.name = "To the Son."
ToProTonAionon.mode = misc_en_US_dedes.Mode1
ToProTonAionon.ode = misc_en_US_dedes.Ode3
ToProTonAionon.text = "To the Son, begotten * without flux, of the Father, before the ages, * and who was lately made incarnate * of the Virgin without seed; * to Christ God now let us cry aloud: * You have exalted the horn of our strength. Only You are holy, O Lord."

ToTheionKaiArriton.notmetered = "N/M (O Christ, I shall tell.)"
ToTheionKaiArriton.name = "O Christ, I shall tell."
ToTheionKaiArriton.mode = misc_en_US_dedes.Mode1
ToTheionKaiArriton.ode = misc_en_US_dedes.Ode5
ToTheionKaiArriton.text = "O Christ, I shall tell of Your virtues * and of their beauty, divine and ineffable: * As coeternal and hypostatic radiance * you shone from glory eternal; and coming forth * incarnate from the Virgin's womb, * You have risen as a Sun * on those in darkness and in the shadow of death."

ToVoithisantiTheo.notmetered = "N/M ()"
ToVoithisantiTheo.name = "(N/M)"
ToVoithisantiTheo.mode = misc_en_US_dedes.Mode1
ToVoithisantiTheo.ode = misc_en_US_dedes.Ode1
ToVoithisantiTheo.text = "To God who helped Moses in Egypt, and through him drowned Pharaoh with his entire army, let us sing a song of victory, for He is glorified."

