TonEnSofiaTousOuranous.text = ""

TonProfitinDiesosas.notmetered = "N/M (As You saved Prophet Jonah.)"
TonProfitinDiesosas.name = "As You saved Prophet Jonah."
TonProfitinDiesosas.mode = misc_en_US_dedes.Mode1
TonProfitinDiesosas.ode = misc_en_US_dedes.Ode6
TonProfitinDiesosas.text = "As You saved Prophet Jonah from the belly of the whale of old, * I pray raise me up from the sea of my sins, in Your love for man."

TonProfitinIonan.notmetered = "N/M (Imitating the inspired Prophet.)"
TonProfitinIonan.name = "Imitating the inspired Prophet."
TonProfitinIonan.mode = misc_en_US_dedes.Mode1
TonProfitinIonan.ode = misc_en_US_dedes.Ode6
TonProfitinIonan.text = "Imitating the inspired Prophet Jonah in the whale, * I cry out to You, O Lord, * \"From destruction free my life * and save me, I pray * to You, O Savior of the world. Glory to You!\""

TonTousYmnologousEnKamino.notmetered = "N/M ()"
TonTousYmnologousEnKamino.name = ""
TonTousYmnologousEnKamino.mode = misc_en_US_dedes.Mode1
TonTousYmnologousEnKamino.ode = misc_en_US_dedes.Ode8
TonTousYmnologousEnKamino.text = ""

TonTyponTouTheiouStavrou.notmetered = "N/M (When Jonah was in.)"
TonTyponTouTheiouStavrou.name = "When Jonah was in."
TonTyponTouTheiouStavrou.mode = misc_en_US_dedes.Mode1
TonTyponTouTheiouStavrou.ode = misc_en_US_dedes.Ode6
TonTyponTouTheiouStavrou.text = "When Jonah was in the belly of the whale, * he extended his arms and thus foreshadowed the figure of Your holy Cross, O Lord. * Then he bounded forth * from the monster, being saved by Your power, O Logos."

TousEnKaminoPaidas.notmetered = "N/M ()"
TousEnKaminoPaidas.name = "N/M"
TousEnKaminoPaidas.mode = misc_en_US_dedes.Mode1
TousEnKaminoPaidas.ode = misc_en_US_dedes.Ode7
TousEnKaminoPaidas.text = "O Savior, the fire did not touch nor affect Your Servants who were in the furnace. Then the three, as if from one mouth, extolled and blessed You, saying: \"O God of our Fathers, blessed are You!\""

TyponTisAgnis.notmetered = "N/M (Burning, yet.)"
TyponTisAgnis.name = "Burning, yet remaining."
TyponTisAgnis.mode = misc_en_US_dedes.Mode1
TyponTisAgnis.ode = misc_en_US_dedes.Ode9
TyponTisAgnis.text = "Burning, yet remaining unconsumed, * the bush disclosed a type of your virgin childbirth. * And now we entreat that you extinguish the furnace of * fierce temptations, for it is besetting us; * that we, O Theotokos, * may praise you ceaselessly and magnify."

YmnonSoiProsferomen.notmetered = "N/M ()"
YmnonSoiProsferomen.name = "N/M"
YmnonSoiProsferomen.mode = misc_en_US_dedes.Mode1
YmnonSoiProsferomen.ode = misc_en_US_dedes.Ode8
YmnonSoiProsferomen.text = "We offer You a hymn of the Bodiless hosts, as did the Servants in the furnace. And we extol You and say: Bless the Lord, O all you works of the Lord."

YpilthonOsNymfona.notmetered = "N/M ()"
YpilthonOsNymfona.name = ""
YpilthonOsNymfona.mode = misc_en_US_dedes.Mode1
YpilthonOsNymfona.ode = misc_en_US_dedes.Ode7
YpilthonOsNymfona.text = ""

