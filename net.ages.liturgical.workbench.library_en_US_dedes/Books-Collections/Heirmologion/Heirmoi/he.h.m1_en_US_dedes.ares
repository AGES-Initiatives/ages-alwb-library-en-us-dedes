SeNoitin.name = ""
SeNoitin.mode = misc_en_US_dedes.Mode1
SeNoitin.ode = misc_en_US_dedes.Ode7
SeNoitin.text = ""

SeTinFaeinin.notmetered = "N/M (You, the brightly shining.)"
SeTinFaeinin.name = "You, the brightly shining."
SeTinFaeinin.mode = misc_en_US_dedes.Mode1
SeTinFaeinin.ode = misc_en_US_dedes.Ode9
SeTinFaeinin.text = "You, the brightly shining lantern and the Mother of our God,* the conspicuous glory,* you are superior to all creation.* We hymn you and magnify."

SeTinOratheisan.notmetered = "N/M ()"
SeTinOratheisan.name = "N/M"
SeTinOratheisan.mode = misc_en_US_dedes.Mode1
SeTinOratheisan.ode = misc_en_US_dedes.Ode9
SeTinOratheisan.text = "O Virgin Theotokos, you are the gate that the Prophet Ezekiel saw and through which no one went except God alone. We honor you with hymns."

SouITropaiouchos.notmetered = "N/M (As befits Divinity.)"
SouITropaiouchos.name = "As befits Divinity."
SouITropaiouchos.mode = misc_en_US_dedes.Mode1
SouITropaiouchos.ode = misc_en_US_dedes.Ode1
SouITropaiouchos.text = "As befits Divinity, O Lord, * so Your victorious right hand was glorified. * For it shattered adversaries, as it is all-powerful, Immortal One, * opening a new way across the deep for the Israelites."

SplanchnonIonan.notmetered = "N/M (Such as it received.)"
SplanchnonIonan.name = "Such as it received."
SplanchnonIonan.mode = misc_en_US_dedes.Mode1
SplanchnonIonan.ode = misc_en_US_dedes.Ode6
SplanchnonIonan.text = "Such as it received * Jonah as an embryo, the sea beast disgorged him from its bowels intact. * With the Virgin, though, when the Logos had dwelt in her taking on flesh, * He came forth from her preserving her yet incorrupt. * For from her no fluxion suffered He, * and He kept her unaltered in childbirth."

StereomaMouGenou.notmetered = "N/M (Become my stable support.)"
StereomaMouGenou.name = "Become my stable support."
StereomaMouGenou.mode = misc_en_US_dedes.Mode1
StereomaMouGenou.ode = misc_en_US_dedes.Ode3
StereomaMouGenou.text = "Become my stable support, * O Lord who established the earth of old on the waters, * for no one is holy except for You."

StereosonDespotaChriste.notmetered = "N/M (O Christ, set me firmly.)"
StereosonDespotaChriste.name = "O Christ, set me firmly."
StereosonDespotaChriste.mode = misc_en_US_dedes.Mode1
StereosonDespotaChriste.ode = misc_en_US_dedes.Ode3
StereosonDespotaChriste.text = "O Christ, set me firmly on the rock of the Faith, by the power of Your all-holy Cross, * so that my mind will not move when tempted by the enemy's assaults. * Only You are holy, O Lord."

StereosonKyrie.notmetered = "N/M ()"
StereosonKyrie.name = ""
StereosonKyrie.mode = misc_en_US_dedes.Mode1
StereosonKyrie.ode = misc_en_US_dedes.Ode3
StereosonKyrie.text = ""

StereosonMeChriste.notmetered = "N/M ()"
StereosonMeChriste.name = ""
StereosonMeChriste.mode = misc_en_US_dedes.Mode1
StereosonMeChriste.ode = misc_en_US_dedes.Ode3
StereosonMeChriste.text = ""

StereothitoIKardiaMou.notmetered = "N/M (Firmly established be.)"
StereothitoIKardiaMou.name = "Firmly established be my heart."
StereothitoIKardiaMou.mode = misc_en_US_dedes.Mode1
StereothitoIKardiaMou.ode = misc_en_US_dedes.Ode3
StereothitoIKardiaMou.text = "Firmly established be my heart, O Lord, * in Your will, O Savior and Christ my God, * You who established upon the waters the second firmament, * and made firm on the waters * the earth, O Almighty One."

StergeinMenImas.notmetered = "N/M (Easier for us.)"
StergeinMenImas.name = "Easier for us."
StergeinMenImas.mode = misc_en_US_dedes.Mode1
StergeinMenImas.ode = misc_en_US_dedes.Ode9
StergeinMenImas.text = "Easier for us is from fear to love silence, * free of any peril as it is, O Virgin; * whereas to weave stirring hymns out of longing * in earnest, is difficult. Therefore, O Mother, * give us strength to match our volition, we pray you."
StergeinMenImas1.text = "Easier for us is from fear to love silence, <br>free of any peril as it is, O Virgin; <br>whereas to weave stirring hymns out of longing <br>in earnest, is difficult. Therefore, O Mother, <br>give us strength to match our volition, we pray you."
StergeinMenImas2.text = "free of any peril as it is, O Virgin;"
StergeinMenImas3.text = "whereas to weave stirring hymns out of longing"
StergeinMenImas4.text = "in earnest, is difficult. Therefore, O Mother,"
StergeinMenImas5.text = "give us strength to match our volition, we pray you."

