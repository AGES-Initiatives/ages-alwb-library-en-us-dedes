tyLIin.antiphon2.title = "Antiphon 2"
tyLIin.antiphon2.sunday.text = "Save us, O Son of God, risen from the dead..."
tyLIin.antiphon2.weekday.text = "Σῶσον ἡμᾶς Υἱὲ Θεοῦ, ὁ ἐν ἁγίοις θαυμαστός..."
tyLIin.antiphon2.jan01.text = "Σῶσον ἡμᾶς Υἱὲ Θεοῦ, ὁ σαρκὶ περιτμηθείς..."
