tyKOin.Kontakion.title = "Kontakion"
tyKOin.default.text = "O Protection..."

//Menaion
tyKOin.Jan01.Kontakion.text = "Ὁ τῶν ὅλων Κύριος..."
tyKOin.Jan02.Kontakion.text = "Ἐν τοῖς ῥείθροις σήμερον..."
tyKOin.Jan06.Kontakion.text = "Ἐπεφάνης σήμερον..."
tyKOin.Feb01.Kontakion.text = "Ὡς ἀγκάλας σήμερον..."
tyKOin.Feb02.Kontakion.text = "Ὁ μήτραν παρθενικὴν..."
tyKOin.Mar24.Kontakion.text = "Ἐπελεύσει Πνεύματος..."
tyKOin.Mar25.Kontakion.text = "Τῇ ὑπερμάχῳ..."
tyKOin.Jul02.Kontakion.text = "Περιβολὴν πᾶσι πιστοῖς..."
tyKOin.Jul25.Kontakion.text = "Προγόνων Χριστοῦ..."
tyKOin.Aug05.Kontakion.text = "Ἐν τῇ θείᾳ σήμερον Μεταμορφώσει..."
tyKOin.Aug06.Kontakion.text = "Ἐπὶ τοῦ ὄρους μετεμορφώθης..."
tyKOin.Aug14.Kontakion.text = "Τῇ ἐνδόξῳ μνήμῃ σου..."
tyKOin.Aug15.Kontakion.text = "Τὴν ἐν πρεσβείαις..."
tyKOin.Aug31.Kontakion.text = "Τὴν θεοδόχον γαστέρα σου..."
tyKOin.Sep01.Kontakion.text = "Ὁ τῶν αἰώνων Ποιητὴς..."
tyKOin.Sep07.Kontakion.text = "Τὴν στολισθεῖσαν..."
tyKOin.Sep08.Kontakion.text = "Ἰωακεὶμ καὶ Ἄννα..."
tyKOin.Sep13.Kontakion.text = "Οὐρανὸς πολύφωτος..."
tyKOin.Sep14.Kontakion.text = "Ὁ ὑψωθεὶς ἐν τῷ Σταυρῷ..."
tyKOin.Oct28.Kontakion.text = "Ὥσπερ νεφέλη ἀγλαῶς..."
tyKOin.Nov20.Kontakion.text = "Εὐφροσύνης σήμερον..."
tyKOin.Nov21.Kontakion.text = "Ὁ καθαρώτατος ναὸς..."
tyKOin.Dec09.Kontakion.text = "Ἑορτάζει σήμερον..."
tyKOin.Dec20.Kontakion.text = "Ἡ Παρθένος... τὸν προαιώνιον..."
tyKOin.Dec25.Kontakion.text = "Ἡ Παρθένος... τὸν ὑπερούσιον..."

//Moveable
tyKOin.tr001.Kontakion.text = "Φαρισαίου φύγωμεν..."
tyKOin.tr008.Kontakion.text = "Τῆς πατρῴας, δόξης σου..."
tyKOin.tr014.Kontakion.text = "Μετὰ τῶν Ἁγίων..."
tyKOin.tr015.Kontakion.text = "Ὅταν ἔλθῃς ὁ Θεός..."
tyKOin.tr022.Kontakion.text = "Τῆς σοφίας ὁδηγέ..."
tyKOin.tr063.Kontakion.text = "Ἡ πάντων χαρά..."
tyKOin.tr064.Kontakion.text = "Τῷ θρόνῳ ἐν οὐρανῷ..."
tyKOin.pe00P.Kontakion.text = "Εἰ καὶ ἐν τάφῳ..."
tyKOin.pe078.Kontakion.text = "Τῇ φιλοπράγμονι δεξιᾷ..."
tyKOin.pe085.Kontakion.text = "Τὸ Χαῖρε ταῖς Μυροφόροις..."
tyKOin.pe095.Kontakion.text = "Τῆς ἑορτῆς τῆς νομικῆς..."
tyKOin.pe110.Kontakion.text = "Τὴν ὑπὲρ ἡμῶν..."
tyKOin.pe120.Kontakion.text = "Ὅτε καταβὰς..."
tyKOin.pe127.Kontakion.text = "Ὡς ἀπαρχὰς τῆς φύσεως..."


