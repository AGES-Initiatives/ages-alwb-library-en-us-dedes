hoCA.couplet1.title = titles_en_US_dedes.ti.Couplet
hoCA.couplet1.verse1 = "O Barbara, pray to the Lord, to forgive me,"
hoCA.couplet1.verse2 = "the angelonymous sinner, who dared to translate this."
