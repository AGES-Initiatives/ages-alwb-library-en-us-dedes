hoCA.Troparion62.melody = ""
hoCA.Troparion62.incipit = ho.ho23_en_US_dedes.hoCA.Troparion62.incipit
hoCA.Troparion62.text = ho.ho23_en_US_dedes.hoCA.Troparion62.text

hoCA.Kontakion.mode = miscellanea_en_US_dedes.misc.Mode3
hoCA.Kontakion.melody = he.a.m3_en_US_dedes.heAU.IParthenosSimeron.name
hoCA.Kontakion.incipit = me.m12.d06_en_US_dedes.meMA.Kontakion1.incipit
hoCA.Kontakion.text = me.m12.d06_en_US_dedes.meMA.Kontakion1.text

hoCA.PostGospelGlory.mode = miscellanea_en_US_dedes.misc.Mode2
hoCA.PostGospelGlory.melody = ""
hoCA.PostGospelGlory.incipit = ho.ho03_en_US_dedes.hoMA.PostGospelHierarch.incipit
hoCA.PostGospelGlory.text = ho.ho03_en_US_dedes.hoMA.PostGospelHierarch.text

hoCA.PostGospelBoth.mode = miscellanea_en_US_dedes.misc.Mode2
hoCA.PostGospelBoth.melody = ""
hoCA.PostGospelBoth.incipit = ho.ho03_en_US_dedes.hoMA.PostGospelTheotokos.incipit
hoCA.PostGospelBoth.text = ho.ho03_en_US_dedes.hoMA.PostGospelTheotokos.text

hoCA.PostGospelHymn1.mode = miscellanea_en_US_dedes.misc.Mode6
hoCA.PostGospelHymn1.melody = he.a.m6_en_US_dedes.heAU.OlinApothemenoi.name
hoCA.PostGospelHymn1.incipit = "I have led my life…"
hoCA.PostGospelHymn1.text = "I have led my life till now in carefree ease and indifference. * I am near the end of it, and I find myself devoid of spiritual fruit. * All I have is the load of my sinful actions. * O benevolent and gracious Lord, * take my unbearable burdens in an act of mercy to me. * And grant compunction and sincere repentance to me that I might be saved, * by the intercessions of Nicholas, which you gladly accept * when I invoke them, and by the prayers * of Your holy Mother, O God."

hoCA.Ode71.ode = miscellanea_en_US_dedes.misc.Ode7
hoCA.Ode71.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode71.melody = he.h.m8_en_US_dedes.heHE.PaidesEvraion.name
hoCA.Ode71.incipit = "Nicholas, you are known for…"
hoCA.Ode71.text = "Nicholas, you are known for saving * those who sail the seas, and for defending widows, * subsidizing the poor, and looking after orphans. * Now by your intercessions for us, * keep us safe from any danger."

hoCA.Ode72.ode = miscellanea_en_US_dedes.misc.Ode7
hoCA.Ode72.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode72.melody = he.h.m8_en_US_dedes.heHE.PaidesEvraion.name
hoCA.Ode72.incipit = "You work extraordinary wonders, *…"
hoCA.Ode72.text = "You work extraordinary wonders, * far away at sea and everywhere on dry land. * When invoked you are there for passengers and patients, * O Nicholas, delivering them * from their illnesses and perils."

hoCA.Ode73.ode = miscellanea_en_US_dedes.misc.Ode7
hoCA.Ode73.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode73.melody = he.h.m8_en_US_dedes.heHE.PaidesEvraion.name
hoCA.Ode73.incipit = "Nicholas, you are a physician…"
hoCA.Ode73.text = "Nicholas, you are a physician, * for you cure every life-threatening condition. * Now I pray you to heal my spiritual illness, * and make my soul be healthy and strong, * by your godly intercessions."

hoCA.Ode7T.ode = miscellanea_en_US_dedes.misc.Ode7
hoCA.Ode7T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode7T.melody = he.h.m8_en_US_dedes.heHE.PaidesEvraion.name
hoCA.Ode7T.incipit = "Hands lifted, pray for us…"
hoCA.Ode7T.text = "Hands lifted, pray for us, O Virgin, * to our God and King, whose mercy is unending, * and deliver us from diseases and disasters, * from all attacks and threats on our lives, * by your mighty intercessions."

hoCA.Ode81.ode = miscellanea_en_US_dedes.misc.Ode8
hoCA.Ode81.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode81.melody = he.h.m8_en_US_dedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode81.incipit = "By your entreaties * to…"
hoCA.Ode81.text = "By your entreaties * to God, O Nicholas, save us * who are trapped in a hole of temptations. * Earnestly we pray you to rescue us, O Father."

hoCA.Ode82.ode = miscellanea_en_US_dedes.misc.Ode8
hoCA.Ode82.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode82.melody = he.h.m8_en_US_dedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode82.incipit = "O blessed Father, * as…"
hoCA.Ode82.text = "O blessed Father, * as you are living in comfort, * and enjoying the glory of heaven, * by your intercessions, protect us who extol you."

hoCA.Ode83.ode = miscellanea_en_US_dedes.misc.Ode8
hoCA.Ode83.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode83.melody = he.h.m8_en_US_dedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode83.incipit = "You were illumined * by…"
hoCA.Ode83.text = "You were illumined * by uncreated divine light. * Now you brighten the souls of the troubled, * dispelling the gloom of temptations, holy Father."

hoCA.Ode8T.ode = miscellanea_en_US_dedes.misc.Ode8
hoCA.Ode8T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode8T.melody = he.h.m8_en_US_dedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode8T.incipit = "I now rely on *…"
hoCA.Ode8T.text = "I now rely on * the mighty help you can give me, * holy Virgin, who gave birth to the Savior * of the world, the Lord, who is merciful and mighty."

hoCA.Ode91.ode = miscellanea_en_US_dedes.misc.Ode9
hoCA.Ode91.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode91.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.name
hoCA.Ode91.incipit = "O Nicholas, thrice-blessed, * you…"
hoCA.Ode91.text = "O Nicholas, thrice-blessed, * you are very famous * throughout the world for your virtues and miracles. * We all rejoice and proclaim you to be our Patron Saint."

hoCA.Ode92.ode = miscellanea_en_US_dedes.misc.Ode9
hoCA.Ode92.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode92.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.name
hoCA.Ode92.incipit = "When we may be in…"
hoCA.Ode92.text = "When we may be in danger, * we pray you to save us, * who glorify you with faith, O praiseworthy Saint, * who imitated the Savior, all-blessed Nicholas."

hoCA.Ode93.ode = miscellanea_en_US_dedes.misc.Ode9
hoCA.Ode93.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode93.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.name
hoCA.Ode93.incipit = "Delighting in celestial * flashes…"
hoCA.Ode93.text = "Delighting in celestial * flashes of divine light, * O Father Nicholas, you have been blessed by God. * May we find safety and shelter in your protective care."

hoCA.Ode9T.ode = miscellanea_en_US_dedes.misc.Ode9
hoCA.Ode9T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode9T.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.name
hoCA.Ode9T.incipit = "My soul is sick and…"
hoCA.Ode9T.text = "My soul is sick and hurting, * suffering from sinning. * O Lady, bless me that I may experience * eternal blessings in heaven, * O Mary Mother of God."

hoCA.Meg00.incipit = ho.ho03_en_US_dedes.hoMA.Ode9.Meg00.incipit
hoCA.Meg00.text = ho.ho03_en_US_dedes.hoMA.Ode9.Meg00.text
hoCA.Meg01.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.melody
hoCA.Meg01.incipit = he.h.m6_en_US_dedes.heHE.TinTimioteran.incipit
hoCA.Meg01.text = he.h.m6_en_US_dedes.heHE.TinTimioteran.text
hoCA.Meg02.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg02.incipit = "Blessed Father Nicholas, come to…"
hoCA.Meg02.text = "Blessed Father Nicholas, come to us * now and when we need you to protect us and keep us safe. * Rescue us from perils on land and sea, and save us * who fervently invoke you, and who extol your name."
hoCA.Meg03.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg03.incipit = "You are the consistently good…"
hoCA.Meg03.text = "You are the consistently good repute * of the holy fathers, and the Trinity's pure abode. * You defend the faithful, you comfort the afflicted; * and therefore we salute you, O Father Nicholas."
hoCA.Meg04.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg04.incipit = "You are our physician when…"
hoCA.Meg04.text = "You are our physician when we are ill; * you take care of orphans; you give rich blessings to the poor. * Rescuer and and savior of travelers in danger, * you hearten those in trouble, wise Father Nicholas!"
hoCA.Meg05.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg05.incipit = "We are proud to call…"
hoCA.Meg05.text = "We are proud to call you our Patron Saint, * for you rescue captives and you save travelers at sea, * you support the poor, you defend widows and orphans, * and also feed the hungry, all-blessed Nicholas."
hoCA.Meg0F.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg0F.incipit = ho.ho23_en_US_dedes.hoCA.Meg0F.incipit
hoCA.Meg0F.text = ho.ho23_en_US_dedes.hoCA.Meg0F.text

