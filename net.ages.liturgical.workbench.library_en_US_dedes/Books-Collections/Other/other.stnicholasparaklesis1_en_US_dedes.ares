A_Resource_Whose_Name = other.stnicholasparaklesis1_en_US_dedes

hoCA.title = "Supplicatory Canon"
hoCA.title2 = "to Saint Nicholas"
hoCA.title3 = "Archbishop of Myra in Lycia"
hoCA.title4 = "the Wonderworker"
hoCA.poet = ""
hoCA.translator = "Fr. Seraphim Dedes"
hoCA.Insert1 = "our Father among the Saints Nicholas the Wonder-worker, Archbishop of Myra in Lycia,"

hoCA.media.text = ""

hoCA.Apolytikion1.mode = miscellanea_en_US_dedes.misc.Mode4
hoCA.Apolytikion1.melody = he.a.m4_en_US_dedes.heAU.OYpsotheisEnToStavro.name
hoCA.Apolytikion1.incipit = "Let us now venerate the…"
hoCA.Apolytikion1.text = "Let us now venerate the all-holy Icon * of Bishop Nicholas, our Saint, O believers, * and all of us who wallow in so many sins, and pray, * \"Quickly come, O Hierarch of the Lord, and protect us; * make us safe from terror and the threat of destruction, * and from infectious diseases keep us free, * by your entreaties to God who is benevolent.\""

hoCA.acrostic = "Acrostic of the Theotokia: \"Seraphim.\""
hoCA.verse = prayers_en_US_dedes.pr.verse_SaintM.text

hoCA.Ode11.ode = miscellanea_en_US_dedes.misc.Ode1
hoCA.Ode11.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode11.melody = he.h.m8_en_US_dedes.heHE.ArmatilatinFarao.name
hoCA.Ode11.incipit = "By your entreaties, as you…"
hoCA.Ode11.text = "By your entreaties, as you pray to God for all, O sacred Nicholas, * drive away the dark cloud of despair that covers me, * for you are filled abundantly with divine joy and gladness * because you stand in the presence of * Christ the King of all, O all-blessed Saint."

hoCA.Ode12.ode = miscellanea_en_US_dedes.misc.Ode1
hoCA.Ode12.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode12.melody = he.h.m8_en_US_dedes.heHE.ArmatilatinFarao.name
hoCA.Ode12.incipit = "The sinful passions, like a…"
hoCA.Ode12.text = "The sinful passions, like a perfect storm, on every side encompass me, * and my soul is rattled by the tempest of my thoughts. * I pray you steer me to the calm port of doing what Christ wills, * by your entreaties, O Nicholas, * so that I may glorify you, O Saint."

hoCA.Ode13.ode = miscellanea_en_US_dedes.misc.Ode1
hoCA.Ode13.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode13.melody = he.h.m8_en_US_dedes.heHE.ArmatilatinFarao.name
hoCA.Ode13.incipit = "All-blessed Nicholas, you truly dwell…"
hoCA.Ode13.text = "All-blessed Nicholas, you truly dwell together with Apostles and Saints. * You are always filled with brilliant light that comes from God. * Grant us who venerate today your sublime holy Icon * to be partakers of that divine * uncreated light, by your holy prayers."

hoCA.Ode1T.ode = miscellanea_en_US_dedes.misc.Ode1
hoCA.Ode1T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode1T.melody = he.h.m8_en_US_dedes.heHE.ArmatilatinFarao.name
hoCA.Ode1T.incipit = "Save me, O Lady, from…"
hoCA.Ode1T.text = "Save me, O Lady, from eternal fire of hell, I now implore you with faith, * immaculate Virgin, who received within your womb * the unbearable fire of God. By your welcome entreaties, * free me from imminent punishment, * which is well deserved for my many sins."

hoCA.Ode31.ode = miscellanea_en_US_dedes.misc.Ode3
hoCA.Ode31.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode31.melody = he.h.m8_en_US_dedes.heHE.SyEiToStereoma.name
hoCA.Ode31.incipit = "Blessed father Nicholas, * by…"
hoCA.Ode31.text = "Blessed father Nicholas, * by your entreaties, may we receive the peace of God, * so that we may duly * glorify you and honor you."

hoCA.Ode32.ode = miscellanea_en_US_dedes.misc.Ode3
hoCA.Ode32.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode32.melody = he.h.m8_en_US_dedes.heHE.SyEiToStereoma.name
hoCA.Ode32.incipit = "Grant that we may find…"
hoCA.Ode32.text = "Grant that we may find relief * and even cures for the illnesses afflicting us, * by your intercessions, we entreat you, St. Nicholas."

hoCA.Ode33.ode = miscellanea_en_US_dedes.misc.Ode3
hoCA.Ode33.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode33.melody = he.h.m8_en_US_dedes.heHE.SyEiToStereoma.name
hoCA.Ode33.incipit = "Many dangers threaten me, *…"
hoCA.Ode33.text = "Many dangers threaten me, * and passions tempt me on every side. Deliver me, * by your intercessions; O St. Nicholas, rescue me."

hoCA.Ode3T.ode = miscellanea_en_US_dedes.misc.Ode3
hoCA.Ode3T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode3T.melody = he.h.m8_en_US_dedes.heHE.SyEiToStereoma.name
hoCA.Ode3T.incipit = "Even when the evils of…"
hoCA.Ode3T.text = "Even when the evils of * this life befall me, I do not fear, for I have you, * O all-spotless Virgin, * to protect me and shelter me."

hoCA.Troparion31.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Troparion31.melody = ""
hoCA.Troparion31.incipit = "Look graciously * on us…"
hoCA.Troparion31.text = "Look graciously * on us, all-blessed St. Nicholas, we beseech you. * By your intercessions, expel the causes of all distress * from the souls of us, who with faith supplicate you."

hoCA.Troparion32.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Troparion32.melody = ""
hoCA.Troparion32.incipit = ho.ho23_en_US_dedes.hoCA.Troparion32.incipit
hoCA.Troparion32.text = ho.ho23_en_US_dedes.hoCA.Troparion32.text

hoCA.Kathisma11.mode = miscellanea_en_US_dedes.misc.Mode2
hoCA.Kathisma11.melody = he.a.m2_en_US_dedes.heAU.ToisTonAimatonSou.name
hoCA.Kathisma11.incipit = "With dazzling rays of your…"
hoCA.Kathisma11.text = "With dazzling rays of your wonderful miracles, * you have illumined and cheered the entire earth, * dispelling the gloom of calamity, * and repelling attacks that endanger us, * O fervent protector, St. Nicholas."

hoCA.Ode41.ode = miscellanea_en_US_dedes.misc.Ode4
hoCA.Ode41.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode41.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.name
hoCA.Ode41.incipit = "I have fallen into a…"
hoCA.Ode41.text = "I have fallen into a host * of sins and transgressions, and I am very sick. * O St. Nicholas, deliver me, * by your supplications to our Lord and God."

hoCA.Ode42.ode = miscellanea_en_US_dedes.misc.Ode4
hoCA.Ode42.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode42.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.name
hoCA.Ode42.incipit = "You once rescued from imminent…"
hoCA.Ode42.text = "You once rescued from imminent * execution three men, for they were innocent. * I entreat you now to rescue me * from eternal punishment, O Nicholas."

hoCA.Ode43.ode = miscellanea_en_US_dedes.misc.Ode4
hoCA.Ode43.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode43.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.name
hoCA.Ode43.incipit = "I have lived my life…"
hoCA.Ode43.text = "I have lived my life recklessly. * It is right that I, the delinquent, be condemned. * Pray I find my way to godliness, * through sincere repentance, O St. Nicholas."

hoCA.Ode4T.ode = miscellanea_en_US_dedes.misc.Ode4
hoCA.Ode4T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode4T.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.name
hoCA.Ode4T.incipit = "Resurrect me, since I am…"
hoCA.Ode4T.text = "Resurrect me, since I am dead, * Lady Theotokos, for you gave birth to Him * who revived the dead, O Virgin pure. * I entreat you, cleanse me of impurity."

hoCA.Ode51.ode = miscellanea_en_US_dedes.misc.Ode5
hoCA.Ode51.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode51.melody = he.h.m8_en_US_dedes.heHE.InaTiMeAposo.name
hoCA.Ode51.incipit = "Your attention was focused *…"
hoCA.Ode51.text = "Your attention was focused * on the Lord in heaven, to whom you did ascend, * and who had equipped you * with His grace to do marvelous miracles. * Therefore we entreat you, O blessed Nicholas, to save us * from disasters and serious illnesses."

hoCA.Ode52.ode = miscellanea_en_US_dedes.misc.Ode5
hoCA.Ode52.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode52.melody = he.h.m8_en_US_dedes.heHE.InaTiMeAposo.name
hoCA.Ode52.incipit = "All the clergy and people…"
hoCA.Ode52.text = "All the clergy and people * celebrate your memory, O father Nicholas. * They receive your blessings, * and with joy they delight in your miracles; * and, when it is proper, they join the choirs and extol you, * and they call you their fabulous Patron Saint."

hoCA.Ode53.ode = miscellanea_en_US_dedes.misc.Ode5
hoCA.Ode53.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode53.melody = he.h.m8_en_US_dedes.heHE.InaTiMeAposo.name
hoCA.Ode53.incipit = "Once your heart was illumined…"
hoCA.Ode53.text = "Once your heart was illumined * by the light of God, it was indeed like Paradise, * for you had within it * Christ the Lord, who is truly the tree of life. * Pray to Him, O Father, that we your servants may be granted * the delight and the glory of Paradise."

hoCA.Ode5T.ode = miscellanea_en_US_dedes.misc.Ode5
hoCA.Ode5T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode5T.melody = he.h.m8_en_US_dedes.heHE.InaTiMeAposo.name
hoCA.Ode5T.incipit = "All-immaculate Virgin, * I have…"
hoCA.Ode5T.text = "All-immaculate Virgin, * I have put my every hope of being saved in you. * Now as I am drowning * in a sea of misfortune, I pray to you. * Please do not ignore me, rather extend your hand and save me, * as your Son did to Peter, and rescue me."

hoCA.Ode61.ode = miscellanea_en_US_dedes.misc.Ode6
hoCA.Ode61.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode61.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.name
hoCA.Ode61.incipit = "The risks of life, like…"
hoCA.Ode61.text = "The risks of life, like a storm on the high seas, * cannot sink my ship, O all-blessed Father. * I am so glad that I have you as pilot, * to steer me safely to calm havens and repose. * O Bishop Nicholas, through you * I shall make it to heaven forevermore."

hoCA.Ode62.ode = miscellanea_en_US_dedes.misc.Ode6
hoCA.Ode62.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode62.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.name
hoCA.Ode62.incipit = "All-blessed Saint, sympathize with your…"
hoCA.Ode62.text = "All-blessed Saint, sympathize with your servants, * and with brotherly affection assist us, * and give us good health, since you are good-natured. * O godly-minded St. Nicholas, pray for us, * and by your holy prayers to God, * from all hardships and troubles deliver us."

hoCA.Ode63.ode = miscellanea_en_US_dedes.misc.Ode6
hoCA.Ode63.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode63.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.name
hoCA.Ode63.incipit = "Of old you saved those…"
hoCA.Ode63.text = "Of old you saved those three innocent generals, * by appearing to the Emperor, O Father. * Likewise now save us from every disaster * and from diseases, O all-holy Nicholas, * by your entreaties to the Lord, * and with love we shall honor your memory."

hoCA.Ode6T.ode = miscellanea_en_US_dedes.misc.Ode6
hoCA.Ode6T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode6T.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.name
hoCA.Ode6T.incipit = "Pray fervently for your servant…"
hoCA.Ode6T.text = "Pray fervently for your servant, O Lady. * My indifference to matters of the spirit * has made me drowsy. So rouse me, and let not * my eyes succumb to the sleep and the death of sin. * For I have chosen you to be * my protectress to guide me throughout my life."

hoCA.Troparion61.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Troparion61.melody = ""
hoCA.Troparion61.incipit = hoCA.Troparion31.incipit
hoCA.Troparion61.text = hoCA.Troparion31.text

