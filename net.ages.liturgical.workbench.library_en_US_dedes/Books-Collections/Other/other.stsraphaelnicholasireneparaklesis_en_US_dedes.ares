A_Resource_Whose_Name = other.stsraphaelnicholasireneparaklesis_en_US_dedes

hoCA.title = "Supplication Service"
hoCA.title2 = "to the Holy Devout Martyrs Raphael and Nicholas, and Irene the Virgin Martyr"
hoCA.poet = poets_en_US_dedes.po.GerasimouMonachouMikragiannanitou
hoCA.translator = "Translated by Fr. Seraphim Dedes"
hoCA.Insert1 = "the holy and glorious Devout Martyrs Raphael and Nicholas, and Irene the Virgin Martyr,"

hoCA.media.text = "" // for complete music

hoCA.Apolytikion1.mode = miscellanea_en_US_dedes.misc.Mode4
hoCA.Apolytikion1.melody = he.a.m4_en_US_dedes.heAU.OYpsotheisEnToStavro.name
hoCA.Apolytikion1.incipit = "Let us in earnest seek…"
hoCA.Apolytikion1.text = "Let us in earnest seek the awesome protection * of the devout and holy Martyrs of Thermi, * who recently appeared, and let us shout to them: * “Blessed Fr. Raphael, and divine deacon Nicholas, * and most noble St. Irene, we entreat you to save us * from every harm, necessity and threat, * for we depend on your intercession on our behalf.”"

hoCA.Apolytikion2.mode = ho.ho23_en_US_dedes.hoCA.Apolytikion2.mode
hoCA.Apolytikion2.melody = ho.ho23_en_US_dedes.hoCA.Apolytikion2.melody
hoCA.Apolytikion2.incipit = ho.ho23_en_US_dedes.hoCA.Apolytikion2.incipit
hoCA.Apolytikion2.text = ho.ho23_en_US_dedes.hoCA.Apolytikion2.text

hoCA.acrostic = ""

hoCA.Ode11.ode = miscellanea_en_US_dedes.misc.Ode1
hoCA.Ode11.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode11.melody = he.h.m8_en_US_dedes.heHE.YgranDiodefsas.name
hoCA.Ode11.incipit = "We pray you, all-blessed St…"
hoCA.Ode11.text = "We pray you, all-blessed St. Raphael, * Nicholas the deacon, and Irene, now with the divine * hosts of holy Martyrs, to entreat God, * that from distress of all kinds He deliver us."

hoCA.Ode12.ode = miscellanea_en_US_dedes.misc.Ode1
hoCA.Ode12.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode12.melody = he.h.m8_en_US_dedes.heHE.YgranDiodefsas.name
hoCA.Ode12.incipit = "Unearthed from its hiding place…"
hoCA.Ode12.text = "Unearthed from its hiding place, your divine * body sanctifies now pious Christians throughout the world. * O St. Raphael the wonder-worker, * sanctification give us and deliverance."

hoCA.Ode13.ode = miscellanea_en_US_dedes.misc.Ode1
hoCA.Ode13.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode13.melody = he.h.m8_en_US_dedes.heHE.YgranDiodefsas.name
hoCA.Ode13.incipit = "Your relics, O Nicholas and…"
hoCA.Ode13.text = "Your relics, O Nicholas and Irene, * appeared as noetic fragrant flowers that blossomed forth. * The mystical grace of God within them * dispels the unpleasant smell of anxieties."

hoCA.Ode1T.ode = miscellanea_en_US_dedes.misc.Ode1
hoCA.Ode1T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode1T.melody = he.h.m8_en_US_dedes.heHE.YgranDiodefsas.name
hoCA.Ode1T.incipit = "O Virgin, the well-spring of…"
hoCA.Ode1T.text = "O Virgin, the well-spring of the divine * compassion, pour out salvation’s sweetness into my soul, * and counter the bitterness within me, * which countless passions of mine have been oozing there."

hoCA.Ode31.ode = miscellanea_en_US_dedes.misc.Ode3
hoCA.Ode31.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode31.melody = he.h.m8_en_US_dedes.heHE.OuraniasApsidos.name
hoCA.Ode31.incipit = "The abundant divine grace that…"
hoCA.Ode31.text = "The abundant divine grace that you, O Saint, have from God * actively dispenses great blessings to all through miracles. * Therefore we pray you grant healing of soul and of body * unto us who run to you for aid, O Raphael."

hoCA.Ode32.ode = miscellanea_en_US_dedes.misc.Ode3
hoCA.Ode32.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode32.melody = he.h.m8_en_US_dedes.heHE.OuraniasApsidos.name
hoCA.Ode32.incipit = "We entreat you, O Martyrs…"
hoCA.Ode32.text = "We entreat you, O Martyrs of Christ, who with Nicholas * and Irene for His sake competed according to the rules: * Save us from injuries by our unruly opponent, * through your earnest prayers to our tender-loving Lord."

hoCA.Ode33.ode = miscellanea_en_US_dedes.misc.Ode3
hoCA.Ode33.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode33.melody = he.h.m8_en_US_dedes.heHE.OuraniasApsidos.name
hoCA.Ode33.incipit = "It was truly fulfillment of…"
hoCA.Ode33.text = "It was truly fulfillment of some divine mysteries * when you were revealed to the world and your relics came to light. * We pray you grant to us the fear of God as a virtue, * sacred Martyrs Raphael, Nicholas and Irene."

hoCA.Ode3T.ode = miscellanea_en_US_dedes.misc.Ode3
hoCA.Ode3T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode3T.melody = he.h.m8_en_US_dedes.heHE.OuraniasApsidos.name
hoCA.Ode3T.incipit = "As the radiant dwelling and…"
hoCA.Ode3T.text = "As the radiant dwelling and as the glorious throne * of the King of glory almighty and Ruler over all, * all-blameless Virgin pure, make of my soul a bright vessel * of a life of purity, so that I may be saved."

hoCA.Troparion31.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Troparion31.melody = ""
hoCA.Troparion31.incipit = "Deliver us, * who run…"
hoCA.Troparion31.text = "Deliver us, * who run with faith to your aid, holy father Raphael, * and Sts. Nicholas and Irene, and preserve us from * all kinds of evil and perils and all diseases."

hoCA.Troparion32.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Troparion32.melody = ""
hoCA.Troparion32.incipit = ho.ho23_en_US_dedes.hoCA.Troparion32.incipit
hoCA.Troparion32.text = ho.ho23_en_US_dedes.hoCA.Troparion32.text

hoCA.Kathisma11.mode = miscellanea_en_US_dedes.misc.Mode2
hoCA.Kathisma11.melody = he.a.m2_en_US_dedes.heAU.TaAnoZiton.name
hoCA.Kathisma11.incipit = "O wise Raphael and Nicholas…"
hoCA.Kathisma11.text = "O wise Raphael and Nicholas, we sing your praise, * for you have become our protectors in recent years. * Holy Martyrs, every day * you fulfill the requests of the believers all, * who have seen you in dreams and awake * when you make your miraculous appearances."

hoCA.Ode41.ode = miscellanea_en_US_dedes.misc.Ode4
hoCA.Ode41.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode41.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.name
hoCA.Ode41.incipit = "We who call on your…"
hoCA.Ode41.text = "We who call on your sacred name * Father Raphael, pray that you will keep us free * from all illnesses and suffering, * knowing you watch over us invisibly."

hoCA.Ode42.ode = miscellanea_en_US_dedes.misc.Ode4
hoCA.Ode42.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode42.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.name
hoCA.Ode42.incipit = "Martyrs Nicholas and Irene, *…"
hoCA.Ode42.text = "Martyrs Nicholas and Irene, * it widely known that you rescue those who run * with sincerity to you for help * from the unseen enemy’s assaults on them."

hoCA.Ode43.ode = miscellanea_en_US_dedes.misc.Ode4
hoCA.Ode43.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode43.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.name
hoCA.Ode43.incipit = "We entreat you, O Martyr…"
hoCA.Ode43.text = "We entreat you, O Martyr Saints * Raphael and Nicholas and Irene, to pray * that salvation of body and soul * be obtained by all who run to you with faith."

hoCA.Ode4T.ode = miscellanea_en_US_dedes.misc.Ode4
hoCA.Ode4T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode4T.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.name
hoCA.Ode4T.incipit = "By your kindness, O Virgin…"
hoCA.Ode4T.text = "By your kindness, O Virgin pure, * heal the wounds and trauma I carry in my soul. * For my twofold health entreat the Lord, * and for the forgiveness of my trespasses."

hoCA.Ode51.ode = miscellanea_en_US_dedes.misc.Ode5
hoCA.Ode51.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode51.melody = he.h.m8_en_US_dedes.heHE.FotisonImas.name
hoCA.Ode51.incipit = "Father Raphael, * many saw…"
hoCA.Ode51.text = "Father Raphael, * many saw you and were filled with joy. * So we pray you to bring joy into our life, * for the bitter tribulations make it sorrowful."

hoCA.Ode52.ode = miscellanea_en_US_dedes.misc.Ode5
hoCA.Ode52.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode52.melody = he.h.m8_en_US_dedes.heHE.FotisonImas.name
hoCA.Ode52.incipit = "Hear our voice, divine *…"
hoCA.Ode52.text = "Hear our voice, divine * deacon Nicholas, and you Irene * who are wondrous and whose name in Greek means peace; * we entreat you, give our life divine untroubled peace."

hoCA.Ode53.ode = miscellanea_en_US_dedes.misc.Ode5
hoCA.Ode53.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode53.melody = he.h.m8_en_US_dedes.heHE.FotisonImas.name
hoCA.Ode53.incipit = "People on the Greek *…"
hoCA.Ode53.text = "People on the Greek * isle of Lesbos have you as their joy. * Free us also from the sorrows we confront, * holy Martyrs Raphael, Nicholas and Irene."

hoCA.Ode5T.ode = miscellanea_en_US_dedes.misc.Ode5
hoCA.Ode5T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode5T.melody = he.h.m8_en_US_dedes.heHE.FotisonImas.name
hoCA.Ode5T.incipit = "Rescue us, we pray, *…"
hoCA.Ode5T.text = "Rescue us, we pray, * from the mad assaults the enemy * like a killer lion launches on our lives * every day, O Theotokos ever-virgin Maid."

hoCA.Ode61.ode = miscellanea_en_US_dedes.misc.Ode6
hoCA.Ode61.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode61.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.name
hoCA.Ode61.incipit = "O Raphael, the devout blessed…"
hoCA.Ode61.text = "O Raphael, the devout blessed Martyr, * by the faithful you were seen through your many * appearances and in visions, revealing * knowledge of things that were previously unknown. * Now will you mystically appear, * and to us grant our earnest requests, we pray."

hoCA.Ode62.ode = miscellanea_en_US_dedes.misc.Ode6
hoCA.Ode62.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode62.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.name
hoCA.Ode62.incipit = "O Nicholas, and Irene pious…"
hoCA.Ode62.text = "O Nicholas, and Irene pious maiden, * you were hidden underground for a long time. * As from a sanctum emerging, O Martyrs, * you now appear to the world, and you pray for us, * and thus you readily dispense * to all people the wealth of the love of God."

hoCA.Ode63.ode = miscellanea_en_US_dedes.misc.Ode6
hoCA.Ode63.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode63.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.name
hoCA.Ode63.incipit = "Deliver us from our life’s…"
hoCA.Ode63.text = "Deliver us from our life’s many troubles, * and watch over us, we pray you, O Martyrs, * the Church’s new shining stars, who competed * of old in contests of martyrdom and excelled, * O God-inspired Raphael * and Irene and divine deacon Nicholas."

hoCA.Ode6T.ode = miscellanea_en_US_dedes.misc.Ode6
hoCA.Ode6T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode6T.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.name
hoCA.Ode6T.incipit = "O Virgin pure, grant that…"
hoCA.Ode6T.text = "O Virgin pure, grant that I be delivered * from the vileness of my passions, and save me * from every sinister plot of the devil, * by interceding for me with your Son and God. * And ask the Lord to give me tears * of repentance to save me, O blessed Maid."

hoCA.Troparion61.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Troparion61.melody = ""
hoCA.Troparion61.incipit = hoCA.Troparion31.incipit
hoCA.Troparion61.text = hoCA.Troparion31.text

hoCA.Troparion62.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Troparion62.melody = ""
hoCA.Troparion62.incipit = ho.ho23_en_US_dedes.hoCA.Troparion62.incipit
hoCA.Troparion62.text = ho.ho23_en_US_dedes.hoCA.Troparion62.text

hoCA.Kontakion.mode = miscellanea_en_US_dedes.misc.Mode2
hoCA.Kontakion.melody = he.a.m2_en_US_dedes.heAU.ToisTonAimatonSou.name
hoCA.Kontakion.incipit = "As ardent helpers of all…"
hoCA.Kontakion.text = "As ardent helpers of all, Martyr Raphael * and godly-minded St. Nicholas and Irene, * you always give help to the suffering, * and by your speedy assistance you alleviate * the pain and distress of our illnesses."

hoCA.PostGospelGlory.mode = miscellanea_en_US_dedes.misc.Mode2
hoCA.PostGospelGlory.melody = ""
hoCA.PostGospelGlory.incipit = ho.ho03_en_US_dedes.hoMA.PostGospelMartyrs.incipit
hoCA.PostGospelGlory.text = ho.ho03_en_US_dedes.hoMA.PostGospelMartyrs.text

hoCA.PostGospelBoth.mode = miscellanea_en_US_dedes.misc.Mode2
hoCA.PostGospelBoth.melody = ""
hoCA.PostGospelBoth.incipit = ho.ho03_en_US_dedes.hoMA.PostGospelTheotokos.incipit
hoCA.PostGospelBoth.text = ho.ho03_en_US_dedes.hoMA.PostGospelTheotokos.text

hoCA.PostGospelHymn1.mode = miscellanea_en_US_dedes.misc.Mode6
hoCA.PostGospelHymn1.melody = he.a.m6_en_US_dedes.heAU.OlinApothemenoi.name
hoCA.PostGospelHymn1.incipit = "In the Church of Christ…"
hoCA.PostGospelHymn1.text = "In the Church of Christ you are like stars that recently lit up, * rising from the Island of * Lesbos, and you shine on all * by your miracles, * holy great Raphael, Nicholas the deacon, * and the beautiful virgin Irene. * Full of the sympathy * and the grace of God, you appear to us * believers all, and you indeed * drive away the gloom of our suffering. * Therefore we entreat you, O wise and holy Martyrs: Never cease * rescuing us and protecting us * from all harm throughout our life."

hoCA.Ode71.ode = miscellanea_en_US_dedes.misc.Ode7
hoCA.Ode71.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode71.melody = he.h.m8_en_US_dedes.heHE.OiEkTisIoudaias.name
hoCA.Ode71.incipit = "Blessed God-bearing father, * many…"
hoCA.Ode71.text = "Blessed God-bearing father, * many people have seen the celestial sight of you; * and they received a blessing: * the light of godly gladness. * So to me grant the saving gift * of your protection and help, * O Raphael, I pray you."

hoCA.Ode72.ode = miscellanea_en_US_dedes.misc.Ode7
hoCA.Ode72.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode72.melody = he.h.m8_en_US_dedes.heHE.OiEkTisIoudaias.name
hoCA.Ode72.incipit = "We entreat you to cease…"
hoCA.Ode72.text = "We entreat you to cease not * from providing the cures for the passions that afflict * our ailing souls and bodies, * O Nicholas thrice-blessed, and all-decorous St. Irene. * And every day free us from * distressing situations."

hoCA.Ode73.ode = miscellanea_en_US_dedes.misc.Ode7
hoCA.Ode73.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode73.melody = he.h.m8_en_US_dedes.heHE.OiEkTisIoudaias.name
hoCA.Ode73.incipit = "You were known by the…"
hoCA.Ode73.text = "You were known by the Master * from of old. Now, O Sts. Raphael and Nicholas, * the world has come to know you * as famous wonder-workers. * For when people request your help * with ardent faith, you are there * providing aid and shelter."

hoCA.Ode7T.ode = miscellanea_en_US_dedes.misc.Ode7
hoCA.Ode7T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode7T.melody = he.h.m8_en_US_dedes.heHE.OiEkTisIoudaias.name
hoCA.Ode7T.incipit = "Being born as a human…"
hoCA.Ode7T.text = "Being born as a human * from your all-holy body, the Lord and God of all * made you, all-blameless Virgin, * protectress of the whole world. * Therefore rightly we honor you, * and pray that you keep us free * from all distress and danger."

hoCA.Ode81.ode = miscellanea_en_US_dedes.misc.Ode8
hoCA.Ode81.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode81.melody = he.h.m8_en_US_dedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode81.incipit = "Grant us well-being * and…"
hoCA.Ode81.text = "Grant us well-being * and health of both soul and body * when we come and venerate your holy relics * and as we extol you, O Raphael all-blessed."

hoCA.Ode82.ode = miscellanea_en_US_dedes.misc.Ode8
hoCA.Ode82.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode82.melody = he.h.m8_en_US_dedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode82.incipit = "By your entreaties * may…"
hoCA.Ode82.text = "By your entreaties * may we be truly successful * and with faith live out our lives without sorrow, * godly-minded Martyr St. Nicholas, we pray you."

hoCA.Ode83.ode = miscellanea_en_US_dedes.misc.Ode8
hoCA.Ode83.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode83.melody = he.h.m8_en_US_dedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode83.incipit = "When circumstances * of life…"
hoCA.Ode83.text = "When circumstances * of life are hard grant me courage; * when I suffer from depression grant me comfort, * recently revealed holy trinity of Martyrs."

hoCA.Ode8T.ode = miscellanea_en_US_dedes.misc.Ode8
hoCA.Ode8T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode8T.melody = he.h.m8_en_US_dedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode8T.incipit = "O Virgin Maiden, * in…"
hoCA.Ode8T.text = "O Virgin Maiden, * in your most caring compassion, * heal my troubled soul, I earnestly entreat you. * Alleviate my pain and my physical discomfort."

hoCA.Ode91.ode = miscellanea_en_US_dedes.misc.Ode9
hoCA.Ode91.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode91.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.name
hoCA.Ode91.incipit = "O Raphael most sacred, *…"
hoCA.Ode91.text = "O Raphael most sacred, * you were given great gifts * of grace divine by our Lord God and Savior Christ. * You have become our supporter and great protector, O Saint."

hoCA.Ode92.ode = miscellanea_en_US_dedes.misc.Ode9
hoCA.Ode92.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode92.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.name
hoCA.Ode92.incipit = "And you, O holy Martyrs…"
hoCA.Ode92.text = "And you, O holy Martyrs, * glorified together, * O bride of God St. Irene and St. Nicholas, * from the inglorious passions deliver us, we pray."

hoCA.Ode93.ode = miscellanea_en_US_dedes.misc.Ode9
hoCA.Ode93.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode93.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.name
hoCA.Ode93.incipit = "Invisibly from heaven * never…"
hoCA.Ode93.text = "Invisibly from heaven * never cease to guard us, * O holy priest Raphael and St. Nicholas * and St. Irene; and protect us who now extol you with faith."

hoCA.Ode9T.ode = miscellanea_en_US_dedes.misc.Ode9
hoCA.Ode9T.mode = miscellanea_en_US_dedes.misc.Mode8
hoCA.Ode9T.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.name
hoCA.Ode9T.incipit = "We praise your grace and…"
hoCA.Ode9T.text = "We praise your grace and favor, * highly favored Maiden, * for everyday you deliver and rescue us, * and you unerringly guide us to do the will of God."

hoCA.Meg00.incipit = ho.ho03_en_US_dedes.hoMA.Ode9.Meg00.incipit
hoCA.Meg00.text = ho.ho03_en_US_dedes.hoMA.Ode9.Meg00.text
hoCA.Meg01.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.melody
hoCA.Meg01.incipit = he.h.m6_en_US_dedes.heHE.TinTimioteran.incipit
hoCA.Meg01.text = he.h.m6_en_US_dedes.heHE.TinTimioteran.text
hoCA.Meg02.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg02.incipit = "Ithaca’s divine son, O Raphael…"
hoCA.Meg02.text = "Ithaca’s divine son, O Raphael, * patron saint of Souli, we salute you and say: Rejoice! * Rejoice, O illustrious Nicholas the deacon! * Rejoice, Irene the virgin, fragrant bouquet for Christ!"
hoCA.Meg03.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg03.incipit = "You competed mightily in the…"
hoCA.Meg03.text = "You competed mightily in the past; * recently in Souli you are famous and known to all. * You shine in the world by performing many wonders * to glorify the Savior, * thrice-blessed martyr Saints."
hoCA.Meg04.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg04.incipit = "Luminous with glory that is…"
hoCA.Meg04.text = "Luminous with glory that is divine, * as devout and holy divine Martyrs of Christ the Lord, * by many appearances you have for the believers * clearly uncovered knowledge of things had been unknown."
hoCA.Meg05.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg05.incipit = "Pride and joy of Souli…"
hoCA.Meg05.text = "Pride and joy of Souli, and patron Saints * of so many churches that were named after you, rejoice, * Raphael all-blessed and Nicholas the deacon, * and St. Irene the virgin, beacons of light divine!"
hoCA.Meg06.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg06.incipit = "Godly-minded St. Raphael, rejoice, *…"
hoCA.Meg06.text = "Godly-minded St. Raphael, rejoice, * along with St. Nicholas, the treasures we have obtained. * You enrich the faithful with gifts of cures and healings, * and sanctify the churches bearing your holy names."
hoCA.Meg07.melody = he.h.m6_en_US_dedes.heHE.TinTimioteran.name
hoCA.Meg07.incipit = "Raphael and Nicholas and Irene…"
hoCA.Meg07.text = "Raphael and Nicholas and Irene, * deliver from every danger all those who reverently * seek refuge in you trusting in your intercession. * And pray the Lord to grant us forgiveness of our sins."
