trSY.commemoration.text = "On this day, we remember the expulsion of the first-formed Adam from the Garden of Eden."
trSY.commemoration.couplet1.title = titles_en_US_dedes.Stichoi
trSY.commemoration.couplet1.verse1 = "Let the world now bitterly mourn with Eve and Adam,"
trSY.commemoration.couplet1.verse2 = "for it fell together with them who fell by sweet eating."
trSY.commemoration.narration.p01 = "[paragraph 01]"
trSY.commemoration.narration.p02 = "[paragraph 02]"
trSY.commemoration.narration.p03 = "[paragraph 03]"
trSY.commemoration.narration.p04 = "[paragraph 04]"
trSY.commemoration.narration.p05 = "[paragraph 05]"
trSY.commemoration.narration.p06 = "[paragraph 06]"
trSY.commemoration.narration.p07 = "[paragraph 07]"
trSY.lastline = "By Your ineffable compassion, O Christ our God, make us worthy of the delights of Paradise and have mercy, as You alone love mankind. Amen."

