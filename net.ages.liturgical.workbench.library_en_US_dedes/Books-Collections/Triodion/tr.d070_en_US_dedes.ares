trSY.commemoration.text = "On great and holy Saturday, we celebrate the burial of the divine Body and the descent into Hades of our Lord and Savior Jesus Christ, through which He recalled our human race from corruption and passed it over into life eternal."
trSY.commemoration.couplet1.title = titles_en_US_dedes.ti.Verses
trSY.commemoration.couplet1.verse1 = "In vain you guard the grave, O guards."
trSY.commemoration.couplet1.verse2 = "No tomb can hold Him who is life itself."
trSY.commemoration.narration.p01 = "[paragraph 01]"
trSY.commemoration.narration.p02 = "[paragraph 02]"
trSY.commemoration.narration.p03 = "[paragraph 03]"
trSY.commemoration.narration.p04 = "[paragraph 04]"
trSY.commemoration.narration.p05 = "[paragraph 05]"
trSY.commemoration.narration.p06 = "[paragraph 06]"
trSY.commemoration.narration.p07 = "[paragraph 07]"
trSY.lastline = "By Your ineffable condescension, O Christ our God, have mercy on us. Amen."

