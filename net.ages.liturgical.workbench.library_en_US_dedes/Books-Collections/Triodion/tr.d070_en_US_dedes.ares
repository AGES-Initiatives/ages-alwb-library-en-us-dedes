trMA.LaudsTheotokion.text = oc.m2.d1_en_US_dedes.ocMA.Kathisma23.text

// Final Troparia from beginning of Matins

trTE.Troparion.mode = miscellanea_en_US_dedes.misc.Mode2
trTE.Troparion.melody = ""
trTE.Troparion.incipit = "O Christ, who hold the…"
trTE.Troparion.text = "O Christ, who hold the whole world in Your hand, You accepted to be held in the sepulcher, so that You might rescue humanity, which was swallowed by Hades, and, as immortal God, give us life and immortality."
