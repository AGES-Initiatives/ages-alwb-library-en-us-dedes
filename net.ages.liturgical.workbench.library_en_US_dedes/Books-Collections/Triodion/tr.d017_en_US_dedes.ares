trMA.Ode2C1H.incipit = he.h.m3_en_US_dedes.OsOmvrosEpAgrostin.incipit
trMA.Ode2C1H.text = he.h.m3_en_US_dedes.OsOmvrosEpAgrostin.text

trMA.Ode2C11.ode = misc_en_US_dedes.Ode2
trMA.Ode2C11.mode = misc_en_US_dedes.Mode3
trMA.Ode2C11.melody = he.h.m3_en_US_dedes.OsOmvrosEpAgrostin.notmetered
trMA.Ode2C11.incipit = "This week of preparation for…"
trMA.Ode2C11.text = "This week of preparation for fasting like the morning brightly shines on us the beams of repentance."

trMA.Ode2C12.ode = misc_en_US_dedes.Ode2
trMA.Ode2C12.mode = misc_en_US_dedes.Mode3
trMA.Ode2C12.melody = he.h.m3_en_US_dedes.OsOmvrosEpAgrostin.notmetered
trMA.Ode2C12.incipit = "O Christ, free me from…"
trMA.Ode2C12.text = "O Christ, free me from the outer darkness, in the abundance of Your mercy, as the only merciful Lord."

trMA.Ode2C13.ode = misc_en_US_dedes.Ode2
trMA.Ode2C13.mode = misc_en_US_dedes.Mode3
trMA.Ode2C13.melody = he.h.m3_en_US_dedes.OsOmvrosEpAgrostin.notmetered
trMA.Ode2C13.incipit = "O Savior, snatch me from…"
trMA.Ode2C13.text = "O Savior, snatch me from the fire and the from the worm that never sleeps, as I cleanse myself through focusing on repentance."

trMA.Ode2C1T.ode = misc_en_US_dedes.Ode2
trMA.Ode2C1T.mode = misc_en_US_dedes.Mode3
trMA.Ode2C1T.melody = he.h.m3_en_US_dedes.OsOmvrosEpAgrostin.notmetered
trMA.Ode2C1T.incipit = "All-lauded Virgin, be for us…"
trMA.Ode2C1T.text = "All-lauded Virgin, be for us the way of repentance, and guide us to the gates of salvation."

trMA.Ode2C21.ode = misc_en_US_dedes.Ode2
trMA.Ode2C21.mode = misc_en_US_dedes.Mode2
trMA.Ode2C21.melody = he.h.m2_en_US_dedes.ProsecheteLaosMou.notmetered
trMA.Ode2C21.incipit = "The season of repentance is…"
trMA.Ode2C21.text = "The season of repentance is here. O lovers of Christ, let us diligently wash away all of our transgressions, and appear to the Master as people who are blessed."

trMA.Ode2C22.ode = misc_en_US_dedes.Ode2
trMA.Ode2C22.mode = misc_en_US_dedes.Mode2
trMA.Ode2C22.melody = he.h.m2_en_US_dedes.ProsecheteLaosMou.notmetered
trMA.Ode2C22.incipit = "Let us diligently keep a…"
trMA.Ode2C22.text = "Let us diligently keep a constant fast that is pure and sinless, that so we may obtain forgiveness of sins."

trMA.Ode2C23.ode = misc_en_US_dedes.Ode2
trMA.Ode2C23.mode = misc_en_US_dedes.Mode2
trMA.Ode2C23.melody = he.h.m2_en_US_dedes.ProsecheteLaosMou.notmetered
trMA.Ode2C23.incipit = "O Christ, You are very…"
trMA.Ode2C23.text = "O Christ, You are very merciful and good, and You always accept everyone in repentance. Therefore we all continually glorify You."

trMA.Ode2C2Tr.ode = misc_en_US_dedes.Ode2
trMA.Ode2C2Tr.mode = misc_en_US_dedes.Mode2
trMA.Ode2C2Tr.melody = he.h.m2_en_US_dedes.ProsecheteLaosMou.notmetered
trMA.Ode2C2Tr.incipit = "Let us glorify God, the…"
trMA.Ode2C2Tr.text = "Let us glorify God, the Trinity in Monad, inseparable, unconfused, undivided in substance and distinct in persons."

trMA.Ode2C2T.ode = misc_en_US_dedes.Ode2
trMA.Ode2C2T.mode = misc_en_US_dedes.Mode2
trMA.Ode2C2T.melody = he.h.m2_en_US_dedes.ProsecheteLaosMou.name
trMA.Ode2C2T.incipit = "O pure Virgin Mother, He…"
trMA.Ode2C2T.text = "O pure Virgin Mother, He who was incarnate from you is the Logos and human. We pray you implore Him to save us your servants from danger."

trMA.Ode2C2H.ode = misc_en_US_dedes.Ode2
trMA.Ode2C2H.mode = misc_en_US_dedes.Mode2
trMA.Ode2C2H.melody = he.h.m2_en_US_dedes.ProsecheteLaosMou.melody
trMA.Ode2C2H.incipit = he.h.m2_en_US_dedes.ProsecheteLaosMou.incipit
trMA.Ode2C2H.text = he.h.m2_en_US_dedes.ProsecheteLaosMou.text

trMA.Ode8C11.ode = misc_en_US_dedes.Ode8
trMA.Ode8C11.mode = misc_en_US_dedes.Mode3
trMA.Ode8C11.melody = he.h.m3_en_US_dedes.TonExAnarchou.notmetered
trMA.Ode8C11.incipit = "Now is the time to…"
trMA.Ode8C11.text = "Now is the time to change your mind and to demonstrate worthy fruits of repentance. O my soul, be quick to rouse yourself, and in fasting and prayers fervently sing to God, \"Sing a hymn to Him, bless Him, and exalt Him beyond measure, unto all the ages.\""

trMA.Ode8C12.ode = misc_en_US_dedes.Ode8
trMA.Ode8C12.mode = misc_en_US_dedes.Mode3
trMA.Ode8C12.melody = he.h.m3_en_US_dedes.TonExAnarchou.notmetered
trMA.Ode8C12.incipit = "O brethren, let us cleanse…"
trMA.Ode8C12.text = "O brethren, let us cleanse the senses of our souls in advance, and let us cleave to the Lord through acts of contrition. And let us sincerely cry aloud, in genuine repentance, \"Sing a hymn to Him, bless Him, and exalt Him beyond measure, unto all the ages.\""

trMA.Ode8C13.ode = misc_en_US_dedes.Ode8
trMA.Ode8C13.mode = misc_en_US_dedes.Mode3
trMA.Ode8C13.melody = he.h.m3_en_US_dedes.TonExAnarchou.notmetered
trMA.Ode8C13.incipit = "By persistent prayer let us…"
trMA.Ode8C13.text = "By persistent prayer let us diligently put to death the foe who lies in wait for us with recollections and curiosity, and who enchants our senses with uncontrolled desire for fleeting pleasure. And let us sing a hymn to Christ, and bless Him, and exalt Him beyond measure, unto all the ages."

trMA.Ode8C1T.ode = misc_en_US_dedes.Ode8
trMA.Ode8C1T.mode = misc_en_US_dedes.Mode3
trMA.Ode8C1T.melody = he.h.m3_en_US_dedes.TonExAnarchou.notmetered
trMA.Ode8C1T.incipit = "O Virgin, you made our…"
trMA.Ode8C1T.text = "O Virgin, you made our earthly nature heavenly. Rain of your tender mercies on my soul, that has become earthen and spoiled, and make it bring forth the fruits of virtues, and it will sing to Christ and exalt Him beyond measure, unto all the ages."

trMA.Ode8C21.ode = misc_en_US_dedes.Ode8
trMA.Ode8C21.mode = misc_en_US_dedes.Mode2
trMA.Ode8C21.melody = he.h.m2_en_US_dedes.TonEnTiVatoMosi.notmetered
trMA.Ode8C21.incipit = "O believers, now let us…"
trMA.Ode8C21.text = "O believers, now let us cleanse ourselves in advance, not just in terms of food, but also in our actions. Let us commence with mental fervor, that we may appear to our Creator bright and cheerful, unto the ages."

trMA.Ode8C22.ode = misc_en_US_dedes.Ode8
trMA.Ode8C22.mode = misc_en_US_dedes.Mode2
trMA.Ode8C22.melody = he.h.m2_en_US_dedes.TonEnTiVatoMosi.notmetered
trMA.Ode8C22.incipit = "This is how we should…"
trMA.Ode8C22.text = "This is how we should fast, not with enmity and conflict, nor with envy and strife, nor with vainglory and hidden deceit, but as Christ did, with humility."

trMA.Ode8C23.ode = misc_en_US_dedes.Ode8
trMA.Ode8C23.mode = misc_en_US_dedes.Mode2
trMA.Ode8C23.melody = he.h.m2_en_US_dedes.TonEnTiVatoMosi.notmetered
trMA.Ode8C23.incipit = "As it says, those who…"
trMA.Ode8C23.text = "As it says, those who give alms to the poor are actually lending their money wisely to the Savior. Such unimaginable joy! For He grants rich rewards for good deeds, unto all the ages."

trMA.Ode8C2Tr.ode = misc_en_US_dedes.Ode8
trMA.Ode8C2Tr.mode = misc_en_US_dedes.Mode2
trMA.Ode8C2Tr.melody = he.h.m2_en_US_dedes.TonEnTiVatoMosi.notmetered
trMA.Ode8C2Tr.incipit = "Glory to the Father and…"
trMA.Ode8C2Tr.text = "Glory to the Father and the Son and the Holy Spirit! For the three are one God. All-holy Trinity, we mortals join the Angels and unceasingly extol You, unto all the ages."

trMA.Ode8C2T.ode = misc_en_US_dedes.Ode8
trMA.Ode8C2T.mode = misc_en_US_dedes.Mode2
trMA.Ode8C2T.melody = he.h.m2_en_US_dedes.TonEnTiVatoMosi.notmetered
trMA.Ode8C2T.incipit = "Rejoice, pure Virgin Mary Theotokos…"
trMA.Ode8C2T.text = "Rejoice, pure Virgin Mary Theotokos, the mercy-seat of the world, the strength of Orthodox Christians! We pray you earnestly entreat that we be rescued from the unquenchable fire, as we glorify you, unto all the ages."

trMA.Ode8C2H.ode = misc_en_US_dedes.Ode8
trMA.Ode8C2H.mode = misc_en_US_dedes.Mode2
trMA.Ode8C2H.melody = he.h.m2_en_US_dedes.TonEnTiVatoMosi.melody
trMA.Ode8C2H.incipit = he.h.m2_en_US_dedes.TonEnTiVatoMosi.incipit
trMA.Ode8C2H.text = he.h.m2_en_US_dedes.TonEnTiVatoMosi.text

trMA.Ode9C11.ode = misc_en_US_dedes.Ode9
trMA.Ode9C11.mode = misc_en_US_dedes.Mode3
trMA.Ode9C11.melody = he.h.m3_en_US_dedes.SeTinAkataflekton.notmetered
trMA.Ode9C11.incipit = "Only very merciful Savior, You…"
trMA.Ode9C11.text = "Only very merciful Savior, You have given us mortals repentance for salvation. We pray You open to us Your immaculate heart of mercy."

trMA.Ode9C12.ode = misc_en_US_dedes.Ode9
trMA.Ode9C12.mode = misc_en_US_dedes.Mode3
trMA.Ode9C12.melody = he.h.m3_en_US_dedes.SeTinAkataflekton.notmetered
trMA.Ode9C12.incipit = "Woe is me! How frightful…"
trMA.Ode9C12.text = "Woe is me! How frightful is that unbiased Tribunal, where we shall all stand naked, to give an account of all the wicked things we did in our life!"

trMA.Ode9C13.ode = misc_en_US_dedes.Ode9
trMA.Ode9C13.mode = misc_en_US_dedes.Mode3
trMA.Ode9C13.melody = he.h.m3_en_US_dedes.SeTinAkataflekton.notmetered
trMA.Ode9C13.incipit = "O Benefactor, rain on us…"
trMA.Ode9C13.text = "O Benefactor, rain on us a shower of compassion, to wash away all our filth; and give us opportunities to repent."

trMA.Ode9C14.ode = misc_en_US_dedes.Ode9
trMA.Ode9C14.mode = misc_en_US_dedes.Mode3
trMA.Ode9C14.melody = he.h.m3_en_US_dedes.SeTinAkataflekton.notmetered
trMA.Ode9C14.incipit = "Let us all cheerfully enter…"
trMA.Ode9C14.text = "Let us all cheerfully enter the front gates of holy Lent with an eager heart, and let us offer hymns of gratitude to Christ."

trMA.Ode9C1T.ode = misc_en_US_dedes.Ode9
trMA.Ode9C1T.mode = misc_en_US_dedes.Mode3
trMA.Ode9C1T.melody = he.h.m3_en_US_dedes.SeTinAkataflekton.notmetered
trMA.Ode9C1T.incipit = "Believing correctly, we extol you…"
trMA.Ode9C1T.text = "Believing correctly, we extol you, O Virgin, as our strong protection and sure hope. We pray you implore your Son to save us."

trMA.Ode9C21.ode = misc_en_US_dedes.Ode9
trMA.Ode9C21.mode = misc_en_US_dedes.Mode2
trMA.Ode9C21.melody = he.h.m2_en_US_dedes.TinParthenianSou.notmetered
trMA.Ode9C21.incipit = "Now let us begin to…"
trMA.Ode9C21.text = "Now let us begin to cleanse ourselves, through tears and fasting and great feats of virtue, so that we may obtain great mercy."

trMA.Ode9C22.ode = misc_en_US_dedes.Ode9
trMA.Ode9C22.mode = misc_en_US_dedes.Mode2
trMA.Ode9C22.melody = he.h.m2_en_US_dedes.TinParthenianSou.notmetered
trMA.Ode9C22.incipit = "Now let us begin by…"
trMA.Ode9C22.text = "Now let us begin by following the example of the wise virgins, O lovers of Christ. Let us eager go out to meet Him with lamps burning bright."

trMA.Ode9C23.ode = misc_en_US_dedes.Ode9
trMA.Ode9C23.mode = misc_en_US_dedes.Mode2
trMA.Ode9C23.melody = he.h.m2_en_US_dedes.TinParthenianSou.notmetered
trMA.Ode9C23.incipit = "Now let us begin by…"
trMA.Ode9C23.text = "Now let us begin by following the example of Elijah, who fasted and fled from wicked Jezebel, so that we may likewise be taken up from earth."

trMA.Ode9C2Tr.ode = misc_en_US_dedes.Ode9
trMA.Ode9C2Tr.mode = misc_en_US_dedes.Mode2
trMA.Ode9C2Tr.melody = he.h.m2_en_US_dedes.TinParthenianSou.notmetered
trMA.Ode9C2Tr.incipit = "Holy, Holy, Holy, thrice-holy, one…"
trMA.Ode9C2Tr.text = "Holy, Holy, Holy, thrice-holy, one Godhead, Father ruler of all, consubstantial Son, and the Spirit also."

trMA.Ode9C2T.ode = misc_en_US_dedes.Ode9
trMA.Ode9C2T.mode = misc_en_US_dedes.Mode2
trMA.Ode9C2T.melody = he.h.m2_en_US_dedes.TinParthenianSou.notmetered
trMA.Ode9C2T.incipit = "O pure and holy ever-virgin…"
trMA.Ode9C2T.text = "O pure and holy ever-virgin Lady, we boast in you, we rely on you. We pray you rescue us from all from perdition."

trMA.Ode9C2H.ode = misc_en_US_dedes.Ode9
trMA.Ode9C2H.mode = misc_en_US_dedes.Mode2
trMA.Ode9C2H.melody = he.h.m2_en_US_dedes.TinParthenianSou.melody
trMA.Ode9C2H.incipit = he.h.m2_en_US_dedes.TinParthenianSou.incipit
trMA.Ode9C2H.text = he.h.m2_en_US_dedes.TinParthenianSou.text

trMA.AposIdiomelon1.mode = misc_en_US_dedes.Mode3
trMA.AposIdiomelon1.melody = misc_en_US_dedes.Idiomelon
trMA.AposIdiomelon1.incipit = "O people everywhere, let us…"
trMA.AposIdiomelon1.text = "O people everywhere, let us greet the coming of Lent with gladness. Once again, we are at the opening of the spiritual games. Let us put aside our physical comforts, that we may increase the gifts of grace in our souls. Let us suffer with Christ as His servants, and we shall be glorified with Him as children of God. And the Holy Spirit will dwell in us and illumine our souls."

