trMA.Ode8C11.ode = miscellanea_en_US_dedes.misc.Ode8
trMA.Ode8C11.mode = miscellanea_en_US_dedes.misc.Mode2
trMA.Ode8C11.greekmelody = he.h.m2_en_US_dedes.heHE.ToDogmatiToTyranniko.greekname
trMA.Ode8C11.melody = he.h.m2_en_US_dedes.heHE.ToDogmatiToTyranniko.name
trMA.Ode8C11.incipit = "Let us rid ourselves of…"
trMA.Ode8C11.text = "Let us rid ourselves of all laziness; * and with our lamps shining brightly, * let us go forth to meet the immortal Bridegroom Christ, * singing hymns and shouting, \"Bless the Lord, all you works of the Lord.\""

trMA.Ode8C12.ode = miscellanea_en_US_dedes.misc.Ode8
trMA.Ode8C12.mode = miscellanea_en_US_dedes.misc.Mode2
trMA.Ode8C12.melody = he.h.m2_en_US_dedes.heHE.ToDogmatiToTyranniko.name
trMA.Ode8C12.incipit = "Let the oil of distributing…"
trMA.Ode8C12.text = "Let the oil of distributing alms * be abundant in the vessels of our souls, * so that we will not be purchasing * when it comes time for the rewards. * Then we can sing, \"Bless the Lord, all you works of the Lord.\""

trMA.Ode8C13.ode = miscellanea_en_US_dedes.misc.Ode8
trMA.Ode8C13.mode = miscellanea_en_US_dedes.misc.Mode2
trMA.Ode8C13.melody = he.h.m2_en_US_dedes.heHE.ToDogmatiToTyranniko.name
trMA.Ode8C13.incipit = "As many of you as…"
trMA.Ode8C13.text = "As many of you as have received * the talent from God, labor to increase it, * and double the grace, with the aid * of Christ who gave it to you, * and with joy sing, \"Bless the Lord, all you works of the Lord.\""

