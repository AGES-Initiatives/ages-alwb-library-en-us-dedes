trMA.Ode2C1H.ode = misc_en_US_dedes.Ode2
trMA.Ode2C1H.mode = misc_en_US_dedes.Mode8
trMA.Ode2C1H.melody = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.melody
trMA.Ode2C1H.incipit = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.incipit
trMA.Ode2C1H.text = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.text

trMA.Ode2C11.ode = misc_en_US_dedes.Ode2
trMA.Ode2C11.mode = misc_en_US_dedes.Mode8
trMA.Ode2C11.melody = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.notmetered
trMA.Ode2C11.incipit = "Now see, I, even I…"
trMA.Ode2C11.text = "Now see, I, even I, am your God. I have established the terms of life in righteous judgment, and I receive, from corruption into incorruption, all who have reposed in the hope of eternal resurrection."

trMA.Ode2C12.ode = misc_en_US_dedes.Ode2
trMA.Ode2C12.mode = misc_en_US_dedes.Mode8
trMA.Ode2C12.melody = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.notmetered
trMA.Ode2C12.incipit = "O Lord, from the four…"
trMA.Ode2C12.text = "O Lord, from the four corners of the earth You have received believers when they die, be that on land or at sea, in rivers, wells, lakes, or springs, whether they were eaten by wild beasts, birds, or reptiles. We pray You grant repose to them all."

trMA.Ode2C13.ode = misc_en_US_dedes.Ode2
trMA.Ode2C13.mode = misc_en_US_dedes.Mode8
trMA.Ode2C13.melody = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.notmetered
trMA.Ode2C13.incipit = "O Lord, having previously analyzed…"
trMA.Ode2C13.text = "O Lord, having previously analyzed all things in Your hand, when You come again, reconstitute all those who have disintegrated into the four elements, and resurrect them, and forgive them all their offenses, committed in knowledge or in ignorance."

trMA.Ode2C14.ode = misc_en_US_dedes.Ode2
trMA.Ode2C14.mode = misc_en_US_dedes.Mode8
trMA.Ode2C14.melody = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.notmetered
trMA.Ode2C14.incipit = "O Lord, the prospect of…"
trMA.Ode2C14.text = "O Lord, the prospect of Your Second Coming is terrifying. For You will arrive on earth like lightning, and You will raise Your entire creation to be judged. Grant to those who lived for You in faith and who welcome You that they may with You forever."

trMA.Ode2C1Tr.ode = misc_en_US_dedes.Ode2
trMA.Ode2C1Tr.mode = misc_en_US_dedes.Mode8
trMA.Ode2C1Tr.melody = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.notmetered
trMA.Ode2C1Tr.incipit = "Super-perfect, super-divine Monad in three…"
trMA.Ode2C1Tr.text = "Super-perfect, super-divine Monad in three hypostases, unbegotten Father, only-begotten Son, and Spirit who proceeded from the Father and who was made manifest through the Son: one essence, nature, rule, and dominion, save us all!"

trMA.Ode2C1T.ode = misc_en_US_dedes.Ode2
trMA.Ode2C1T.mode = misc_en_US_dedes.Mode8
trMA.Ode2C1T.melody = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.notmetered
trMA.Ode2C1T.incipit = "O Virgin Mother, ineffable is…"
trMA.Ode2C1T.text = "O Virgin Mother, ineffable is the miracle of your conception! At once you are both pregnant and a virgin. You give birth to a child, without knowing a man. How can this be? The Logos of God knows, who in a new way, transcending nature, was born of you."

trMA.Ode2K.ode = misc_en_US_dedes.Ode2
trMA.Ode2K.mode = misc_en_US_dedes.Mode8
trMA.Ode2K.melody = misc_en_US_dedes.HeirmosNotMetered
trMA.Ode2K.incipit = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.incipit
trMA.Ode2K.text = he.h.m8_en_US_dedes.IdeteIdeteOProTonAionon.text

trMA.Ode3C1H.ode = misc_en_US_dedes.Ode3
trMA.Ode3C1H.mode = misc_en_US_dedes.Mode8
trMA.Ode3C1H.melody = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.melody
trMA.Ode3C1H.incipit = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.incipit
trMA.Ode3C1H.text = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.text

trMA.Ode3C11.ode = misc_en_US_dedes.Ode3
trMA.Ode3C11.mode = misc_en_US_dedes.Mode8
trMA.Ode3C11.melody = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.notmetered
trMA.Ode3C11.incipit = "O God, grant that those…"
trMA.Ode3C11.text = "O God, grant that those who have finished the race of this life in pious glory may wear a crown of righteousness, and that they may enjoy the eternal blessings."

trMA.Ode3C12.ode = misc_en_US_dedes.Ode3
trMA.Ode3C12.mode = misc_en_US_dedes.Mode8
trMA.Ode3C12.melody = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.notmetered
trMA.Ode3C12.incipit = "O God, when You will…"
trMA.Ode3C12.text = "O God, when You will test all thing with fire, grant repose to those who were suddenly taken, or incinerated by lightning, or who froze to death, or who died by any injury."

trMA.Ode3C13.ode = misc_en_US_dedes.Ode3
trMA.Ode3C13.mode = misc_en_US_dedes.Mode8
trMA.Ode3C13.melody = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.notmetered
trMA.Ode3C13.incipit = "O Christ, grant that those…"
trMA.Ode3C13.text = "O Christ, grant that those who sailed the ever turbulent sea of this life, and who were navigated by Orthodox living, may put in to port in the harbor of the incorruptible life that You give."

trMA.Ode3C14.ode = misc_en_US_dedes.Ode3
trMA.Ode3C14.mode = misc_en_US_dedes.Mode8
trMA.Ode3C14.melody = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.notmetered
trMA.Ode3C14.incipit = "O Christ our God, we…"
trMA.Ode3C14.text = "O Christ our God, we pray that in the last day You resurrect in glory all those who, by Your judgments, were devoured by any creatures or the sea or birds of heaven."

trMA.Ode3C1Tr.ode = misc_en_US_dedes.Ode3
trMA.Ode3C1Tr.mode = misc_en_US_dedes.Mode8
trMA.Ode3C1Tr.melody = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.notmetered
trMA.Ode3C1Tr.incipit = "Mentally I distinguish three Persons…"
trMA.Ode3C1Tr.text = "Mentally I distinguish three Persons in the simplicity of the divine Monad, and at the same time I join them together. For shining in threefold radiance with the swiftness of lightning, the Godhead is seen as Unity."

trMA.Ode3C1T.ode = misc_en_US_dedes.Ode3
trMA.Ode3C1T.mode = misc_en_US_dedes.Mode8
trMA.Ode3C1T.melody = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.notmetered
trMA.Ode3C1T.incipit = "O pure Maiden, your miracle…"
trMA.Ode3C1T.text = "O pure Maiden, your miracle is beyond comprehension. You became pregnant without the agency of a man, and after childbirth you preserved your virginity. That is why the host of Angels and the human race throughout the ages extol you."

trMA.Ode3K.ode = misc_en_US_dedes.Ode3
trMA.Ode3K.mode = misc_en_US_dedes.Mode8
trMA.Ode3K.melody = misc_en_US_dedes.HeirmosNotMetered
trMA.Ode3K.incipit = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.incipit
trMA.Ode3K.text = he.h.m8_en_US_dedes.OStereosasEnTiCheiriSou.text

trMA.Ode3Kathisma1.mode = misc_en_US_dedes.Mode5
trMA.Ode3Kathisma1.melody = he.a.m5_en_US_dedes.TonSynanarchonLogon.name
trMA.Ode3Kathisma1.incipit = "You endured crucifixion and died…"
trMA.Ode3Kathisma1.text = "You endured crucifixion and died on our behalf; * You put Hades to death and with yourself raised the dead. * Savior, grant repose to those whom You have taken from us, * as our God who loves humanity. O Giver of life, * with an abundance of mercy, * account them worthy of Your kingdom, * at Your fearful Second Coming, O Lord."

trMA.Ode3Kathisma2.mode = misc_en_US_dedes.Mode5
trMA.Ode3Kathisma2.melody = he.a.m5_en_US_dedes.TonSynanarchonLogon.name
trMA.Ode3Kathisma2.incipit = oc.m5.d3_en_US_dedes.ocVE.ApolTheotokionVM.incipit
trMA.Ode3Kathisma2.text = oc.m5.d3_en_US_dedes.ocVE.ApolTheotokionVM.text

trMA.Ode4C1H.ode = misc_en_US_dedes.Ode4
trMA.Ode4C1H.mode = misc_en_US_dedes.Mode8
trMA.Ode4C1H.melody = he.h.m8_en_US_dedes.ExOrousKataskiou.melody
trMA.Ode4C1H.incipit = he.h.m8_en_US_dedes.ExOrousKataskiou.incipit
trMA.Ode4C1H.text = he.h.m8_en_US_dedes.ExOrousKataskiou.text

trMA.Ode4C11.ode = misc_en_US_dedes.Ode4
trMA.Ode4C11.mode = misc_en_US_dedes.Mode8
trMA.Ode4C11.melody = he.h.m8_en_US_dedes.ExOrousKataskiou.notmetered
trMA.Ode4C11.incipit = "We pray You, our Savior…"
trMA.Ode4C11.text = "We pray You, our Savior, remember all who have died, from the beginning of time to the present, our fathers, ancestors, grandparents, and great-grandparents, who kept Your laws and correctly believed in You."

trMA.Ode4C12.ode = misc_en_US_dedes.Ode4
trMA.Ode4C12.mode = misc_en_US_dedes.Mode8
trMA.Ode4C12.melody = he.h.m8_en_US_dedes.ExOrousKataskiou.notmetered
trMA.Ode4C12.incipit = "O Christ, we pray for…"
trMA.Ode4C12.text = "O Christ, we pray for all monastics, ascetics, young and old, who reached the end life in faith, on a mountain, on the road, in tombs, and in the wilderness, and grant them to dwell among the Saint."

trMA.Ode4C13.ode = misc_en_US_dedes.Ode4
trMA.Ode4C13.mode = misc_en_US_dedes.Mode8
trMA.Ode4C13.melody = he.h.m8_en_US_dedes.ExOrousKataskiou.notmetered
trMA.Ode4C13.incipit = "We pray You, our Savior…"
trMA.Ode4C13.text = "We pray You, our Savior, to grant repose to all the faithful who who suddenly and unexpectedly left this life, in the midst of joy or sorrow, prosperity or misfortune."

trMA.Ode4C14.ode = misc_en_US_dedes.Ode4
trMA.Ode4C14.mode = misc_en_US_dedes.Mode8
trMA.Ode4C14.melody = he.h.m8_en_US_dedes.ExOrousKataskiou.notmetered
trMA.Ode4C14.incipit = "We pray You, Christ our…"
trMA.Ode4C14.text = "We pray You, Christ our Savior, to grant repose to those who froze to death, died in a traffic accident, or were killed by a tornado, blizzard, or hurricane, or were struck by a falling object, or were buried alive."

trMA.Ode4C1Tr.ode = misc_en_US_dedes.Ode4
trMA.Ode4C1Tr.mode = misc_en_US_dedes.Mode8
trMA.Ode4C1Tr.melody = he.h.m8_en_US_dedes.ExOrousKataskiou.notmetered
trMA.Ode4C1Tr.incipit = "Strange it is that the…"
trMA.Ode4C1Tr.text = "Strange it is that the Godhead is one and also three, fully present in each single person indivisibly. For the Father, the Son, and the Holy Spirit are the three who are worshiped as one God."

trMA.Ode4C1T.ode = misc_en_US_dedes.Ode4
trMA.Ode4C1T.mode = misc_en_US_dedes.Mode8
trMA.Ode4C1T.melody = he.h.m8_en_US_dedes.ExOrousKataskiou.notmetered
trMA.Ode4C1T.incipit = "We are battered by the…"
trMA.Ode4C1T.text = "We are battered by the storm of sin. O Mother of our Lord, guide us, by your prayers, and steer us to the shelter of salvation, and rescue us from every calamity."

trMA.Ode4K.ode = misc_en_US_dedes.Ode4
trMA.Ode4K.mode = misc_en_US_dedes.Mode8
trMA.Ode4K.melody = misc_en_US_dedes.Heirmos
