trMA.Ode6C11.mode = miscellanea_en_US_dedes.misc.Mode6
trMA.Ode6C11.greekmelody = he.h.m6_en_US_dedes.heHE.AvyssosEschati.greekname
trMA.Ode6C11.melody = he.h.m6_en_US_dedes.heHE.AvyssosEschati.name
trMA.Ode6C11.incipit = "Savior, You cried out, “O…"
trMA.Ode6C11.text = "Savior, You cried out, “O Disciples, you call me Lord and Teacher, and you are right, for that is what I am. So imitate the example, and do as you have seen me do.”"

trMA.Ode6C12.ode = miscellanea_en_US_dedes.misc.Ode6
trMA.Ode6C12.mode = miscellanea_en_US_dedes.misc.Mode6
trMA.Ode6C12.melody = he.h.m6_en_US_dedes.heHE.AvyssosEschati.name
trMA.Ode6C12.incipit = "He who is not dirty…"
trMA.Ode6C12.text = "He who is not dirty does not need washing, not even of his feet. And you are clean, O my Disciples, but not all; for one of you is inclined to undisciplined insanity."

