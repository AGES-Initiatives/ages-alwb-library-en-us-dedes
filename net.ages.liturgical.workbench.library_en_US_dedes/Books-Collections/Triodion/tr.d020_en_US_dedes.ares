trMA.Ode9C31.ode = misc_en_US_dedes.Ode9
trMA.Ode9C31.mode = misc_en_US_dedes.Mode8
trMA.Ode9C31.melody = he.h.m8_en_US_dedes.AllotrionTonMiteron.notmetered
trMA.Ode9C31.incipit = "O Giver of Life, we…"
trMA.Ode9C31.text = "O Giver of Life, we venerate Your stigmata, by which we were redeemed from the sinful passions, and we are recognized as being sealed by Your precious Cross, for salvation and help against all the enemy's assaults."

trMA.Ode9C32.ode = misc_en_US_dedes.Ode9
trMA.Ode9C32.mode = misc_en_US_dedes.Mode8
trMA.Ode9C32.melody = he.h.m8_en_US_dedes.AllotrionTonMiteron.notmetered
trMA.Ode9C32.incipit = "All-merciful Lord, who of Your…"
trMA.Ode9C32.text = "All-merciful Lord, who of Your own will were crucified for the salvation of all, shine the splendid rays of Your Cross on the souls of those who extol Your Passion, and guide and direct us on the path of life."

trMA.Ode9C3Tr.ode = misc_en_US_dedes.Ode9
trMA.Ode9C3Tr.mode = misc_en_US_dedes.Mode8
trMA.Ode9C3Tr.melody = he.h.m8_en_US_dedes.AllotrionTonMiteron.notmetered
trMA.Ode9C3Tr.incipit = "To lawless men it is…"
trMA.Ode9C3Tr.text = "To lawless men it is foreign to glorify God, the Father and the Son and the Holy Spirit, the Holy Trinity, the uncreated universal dominion, by which the whole world was created, and by the dominion of which it was established."

trMA.Ode9C3T.ode = misc_en_US_dedes.Ode9
trMA.Ode9C3T.mode = misc_en_US_dedes.Mode8
trMA.Ode9C3T.melody = he.h.m8_en_US_dedes.AllotrionTonMiteron.notmetered
trMA.Ode9C3T.incipit = "Compassionate King of glory, at…"
trMA.Ode9C3T.text = "Compassionate King of glory, at the intercessions of the Theotokos, be gracious to Your servants when You come to judge the universe, O just Judge. For into You have we all been baptized, and only You do we worship."

trMA.Ode9C3A1.ode = misc_en_US_dedes.Ode9
trMA.Ode9C3A1.mode = misc_en_US_dedes.Mode8
trMA.Ode9C3A1.melody = he.h.m8_en_US_dedes.AllotrionTonMiteron.notmetered
trMA.Ode9C3A1.incipit = "O loving Lord, we have…"
trMA.Ode9C3A1.text = "O loving Lord, we have all sinned and disobeyed Your commandments, and we have strayed far from the way of Your divine precepts, because of our indifference. We entreat You, O tender-lover Savior, to call our souls back from error."

