trMA.Ode3C1H.ode = misc_en_US_dedes.Ode3
trMA.Ode3C1H.mode = misc_en_US_dedes.Mode4
trMA.Ode3C1H.melody = he.h.m4_en_US_dedes.TousSousYmnologous.melody
trMA.Ode3C1H.incipit = he.h.m4_en_US_dedes.TousSousYmnologous.incipit
trMA.Ode3C1H.text = he.h.m4_en_US_dedes.TousSousYmnologous.text

trMA.Ode3C11.ode = misc_en_US_dedes.Ode3
trMA.Ode3C11.mode = misc_en_US_dedes.Mode4
trMA.Ode3C11.melody = he.h.m4_en_US_dedes.TousSousYmnologous.name
trMA.Ode3C11.incipit = "Rejoice, blessed Virgin! You have…"
trMA.Ode3C11.text = "Rejoice, blessed Virgin! You have borne Christ, * the Wheat that came forth from land unploughed. * The Table symbolizes you, * who held the Lord, the Bread of life. * Rejoice, O never empty well * of living water, O Bride of God!"

trMA.Ode3C12.ode = misc_en_US_dedes.Ode3
trMA.Ode3C12.mode = misc_en_US_dedes.Mode4
trMA.Ode3C12.melody = he.h.m4_en_US_dedes.TousSousYmnologous.name
trMA.Ode3C12.incipit = "For us the believers you…"
trMA.Ode3C12.text = "For us the believers you have mothered * the unblemished Bull, the Lamb of God, * who takes away the sins of the world; * we therefore call you Heifer and Ewe. * Rejoice! You are the Mercy Seat, * from which forgiveness has come to us."

trMA.Ode3C13.ode = misc_en_US_dedes.Ode3
trMA.Ode3C13.mode = misc_en_US_dedes.Mode4
trMA.Ode3C13.melody = he.h.m4_en_US_dedes.TousSousYmnologous.name
trMA.Ode3C13.incipit = "Christ the Sun of Righteousness…"
trMA.Ode3C13.text = "Christ the Sun of Righteousness has risen * from you, Bride of God and shining Dawn. * Rejoice, for as the dwelling place * of Him who is the Light of the world, * you drove away the night of sin, * and darkest demons now fade away."

trMA.Ode3C14.ode = misc_en_US_dedes.Ode3
trMA.Ode3C14.mode = misc_en_US_dedes.Mode4
trMA.Ode3C14.melody = he.h.m4_en_US_dedes.TousSousYmnologous.name
trMA.Ode3C14.incipit = "The East Gate, through which…"
trMA.Ode3C14.text = "The East Gate, through which the Prince alone went, * prefigured your body; for the Lord * alone came forth from you. Rejoice! * Your Son has shattered Hades' gates. * Rejoice, all-lauded and divine * gateway of those who are being saved!"

trMA.Ode3K.ode = misc_en_US_dedes.Ode3
trMA.Ode3K.mode = misc_en_US_dedes.Mode4
trMA.Ode3K.melody = misc_en_US_dedes.Heirmos
