trMA.Ode9C1H.mode = misc_en_US_dedes.Mode4
trMA.Ode9C1H.melody = he.h.m4_en_US_dedes.ApasGigenis_Akathist.melody
trMA.Ode9C1H.incipit = he.h.m4_en_US_dedes.ApasGigenis_Akathist.incipit
