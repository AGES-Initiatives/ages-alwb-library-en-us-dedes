trMA.Ode6C1H.incipit = he.h.m4_en_US_dedes.TinTheianTaftin.incipit
trMA.Ode6C1H.text = he.h.m4_en_US_dedes.TinTheianTaftin.text

trMA.Ode6C11.ode = misc_en_US_dedes.Ode6
trMA.Ode6C11.mode = misc_en_US_dedes.Mode4
trMA.Ode6C11.melody = he.h.m4_en_US_dedes.TinTheianTaftin.name
trMA.Ode6C11.incipit = "O bridal chamber of God…"
trMA.Ode6C11.text = "O bridal chamber of God the Word, * we credit the theosis of all to you. O Virgin pure, rejoice! * The Prophets' echoing prophecies, * fulfilled in you, adorned what Apostles then proclaimed."

trMA.Ode6C12.ode = misc_en_US_dedes.Ode6
trMA.Ode6C12.mode = misc_en_US_dedes.Mode4
trMA.Ode6C12.melody = he.h.m4_en_US_dedes.TinTheianTaftin.name
trMA.Ode6C12.incipit = "The fleece upon which alone…"
trMA.Ode6C12.text = "The fleece upon which alone the dew * for Gideon dripped down once prefigured you, to whom we sing: Rejoice! * From you, O Virgin, did Christ come forth * as healing Dew, extinguishing pagan passions all."

trMA.Ode6C13.ode = misc_en_US_dedes.Ode6
trMA.Ode6C13.mode = misc_en_US_dedes.Mode4
trMA.Ode6C13.melody = he.h.m4_en_US_dedes.TinTheianTaftin.name
trMA.Ode6C13.incipit = "Behold, we greet you and…"
trMA.Ode6C13.text = "Behold, we greet you and sing: Rejoice! * As we are tossed about on the stormy sea of life's adversities * and all the snares of our enemy, * become for us the port and Fair Haven we desire."

trMA.Ode6C14.ode = misc_en_US_dedes.Ode6
trMA.Ode6C14.mode = misc_en_US_dedes.Mode4
trMA.Ode6C14.melody = he.h.m4_en_US_dedes.TinTheianTaftin.name
trMA.Ode6C14.incipit = "O cause of joy, you…"
trMA.Ode6C14.text = "O cause of joy, you inspire us * to call you unconsumed burning bush and shining pillar of a cloud, * because the Lord was in you as well. * You always guard the faithful, who sing to you: Rejoice!"

