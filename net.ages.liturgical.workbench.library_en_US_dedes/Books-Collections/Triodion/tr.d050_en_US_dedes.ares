trSY.commemoration.text = "On this day, the fourth Sunday of Lent, we commemorate our devout Father John, author of The Ladder."
trSY.commemoration.onotherday.text = "On this day, we commemorate our devout Father John, author of The Ladder."
trSY.commemoration.couplet1.title = titles_en_US_dedes.ti.Verses
trSY.commemoration.couplet1.verse1 = "John, who was dead in the flesh while living,"
trSY.commemoration.couplet1.verse2 = "Ever lives, while appearing to be dead and not breathing."
trSY.commemoration.couplet2.verse1 = "He left us his book, the Ladder for ascending,"
trSY.commemoration.couplet2.verse2 = "And shows us the way of his ascent to heaven."
trSY.commemoration.narration.p01 = "[paragraph 01]"
trSY.commemoration.narration.p02 = "[paragraph 02]"
trSY.commemoration.narration.p03 = "[paragraph 03]"
trSY.commemoration.narration.p04 = "[paragraph 04]"
trSY.commemoration.narration.p05 = "[paragraph 05]"
trSY.commemoration.narration.p06 = "[paragraph 06]"
trSY.commemoration.narration.p07 = "[paragraph 07]"
trSY.lastline = "By his intercessions, O God, have mercy on us and save us. Amen."

