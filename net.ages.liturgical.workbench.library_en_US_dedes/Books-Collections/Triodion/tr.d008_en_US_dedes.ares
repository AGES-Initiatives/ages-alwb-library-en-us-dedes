trMA.Exaposteilarion1.mode = miscellanea_en_US_dedes.misc.Mode2
trMA.Exaposteilarion1.greekmelody = he.a.m2_en_US_dedes.heAU.GynaikesAkoutisthite.greekname
trMA.Exaposteilarion1.melody = he.a.m2_en_US_dedes.heAU.GynaikesAkoutisthite.name
trMA.Exaposteilarion1.incipit = "The riches of Your gifts…"
trMA.Exaposteilarion1.text = "The riches of Your gifts of grace,* which You gave me, the wretched one,* I squandered badly, O Savior,* since without cause I departed,* and lived in great extravagance.* The demons tricked me to disperse.* And therefore as the Prodigal,* I am returning. Receive me,* O loving Father, and save me."

trMA.Exaposteilarion2.mode = miscellanea_en_US_dedes.misc.Mode2
trMA.Exaposteilarion2.greekmelody = he.a.m2_en_US_dedes.heAU.GynaikesAkoutisthite.greekname
trMA.Exaposteilarion2.melody = he.a.m2_en_US_dedes.heAU.GynaikesAkoutisthite.name
trMA.Exaposteilarion2.incipit = "The wealth You gave to…"
trMA.Exaposteilarion2.text = "The wealth You gave to me, O Lord,* I squandered and I spent it all.* And I the wretch have submitted * myself to the wicked demons.* O Savior all-compassionate,* take pity on this Prodigal. * And make me clean, for I am stained.* And give me again my former * robe of Your rule and kingdom."

trMA.ExapTheotokion.mode = miscellanea_en_US_dedes.misc.Mode2
trMA.ExapTheotokion.greekmelody = he.a.m2_en_US_dedes.heAU.GynaikesAkoutisthite.greekname
trMA.ExapTheotokion.melody = he.a.m2_en_US_dedes.heAU.GynaikesAkoutisthite.name
trMA.ExapTheotokion.incipit = "O Virgin Mother, holy Maid,*…"
trMA.ExapTheotokion.text = "O Virgin Mother, holy Maid,* you are the great resounding boast * of the Apostles and Martyrs * of the Devout and the Prophets.* And therefore make your Son and Lord * propitiously disposed toward us,* your servants, when He sits to judge * each according to his merits, * O all-holy Theotokos."

