meLI.MM.CommunionHymn.text = properties_en_US_dedes.media.key

meLI.Megalynarion.text = "As a trophy-bearer you proved to be * a great man in contest, and in miracles most renowned. * O thrice-blessed George, you were manifestly known as * the Church's great protector, the martyrs' pride and joy."
