meMA.Ode9C1M01.incipit = "…"
meMA.Ode9C1M01.text = "O my soul, magnify Saint George the holy Martyr, the foremost of victorious athletes and combatants."
meMA.Ode9C1M02.incipit = "…"
meMA.Ode9C1M02.text = "O my soul, magnify Saint George the Great Martyr, the pride and joy of Palestine and boast of Cappadocians."
meMA.Ode9C1MTr.incipit = pe.d071_en_US_dedes.peMA.Ode9C1MTr.incipit
meMA.Ode9C1MTr.text = pe.d071_en_US_dedes.peMA.Ode9C1MTr.text
