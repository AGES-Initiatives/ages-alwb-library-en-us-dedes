meMA.Ode4C11.greekmelody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.greekname
meMA.Ode4C11.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C11.incipit = "O Father Basil, you brought…"
meMA.Ode4C11.text = "O Father Basil, you brought down the haughty spirit of wicked heresy by your entreaties to the Lord, and thus brought joy and gladness to the Church."

meMA.Ode4C12.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C12.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C12.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C12.incipit = "O Father, the Judge of…"
meMA.Ode4C12.text = "O Father, the Judge of the contest accepted your relentless resistance to your opponents, and He settled you in the heavenly mansions."

meMA.Ode4C13.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C13.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C13.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C13.incipit = "O Saint, you endured persecution…"
meMA.Ode4C13.text = "O Saint, you endured persecution, and you passed your life amidst great perils, all for the sake of the traditions preserved in the Church of Christ."

meMA.Ode4C1T.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C1T.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C1T.incipit = "O immaculate Virgin, implore the…"
meMA.Ode4C1T.text = "O immaculate Virgin, implore the God whom you bore, to forgive the failings of us who supplicate you."

