meMA.Ode3C11.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode3C11.greekmelody = he.h.m8_en_US_dedes.heHE.SyEiToStereoma.greekname
meMA.Ode3C11.melody = he.h.m8_en_US_dedes.heHE.SyEiToStereoma.notmetered
meMA.Ode3C11.incipit = "O Saint, your admirable memory…"
meMA.Ode3C11.text = "O Saint, your admirable memory is celebrated by all, and it splendidly cheers Orthodox congregations."

meMA.Ode3C12.ode = miscellanea_en_US_dedes.misc.Ode3
meMA.Ode3C12.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode3C12.melody = he.h.m8_en_US_dedes.heHE.SyEiToStereoma.notmetered
meMA.Ode3C12.incipit = "O devout Basil, since you…"
meMA.Ode3C12.text = "O devout Basil, since you have stripped off the cloud of the body and are free from bonds, you have gone to be close to God."

meMA.Ode3C13.ode = miscellanea_en_US_dedes.misc.Ode3
meMA.Ode3C13.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode3C13.melody = he.h.m8_en_US_dedes.heHE.SyEiToStereoma.notmetered
meMA.Ode3C13.incipit = "O devout Father, you worthily…"
meMA.Ode3C13.text = "O devout Father, you worthily received a blessed end and the joy of the elect, for you were yourself elect and chosen."

meMA.Ode3C1T.ode = miscellanea_en_US_dedes.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode3C1T.melody = he.h.m8_en_US_dedes.heHE.SyEiToStereoma.notmetered
meMA.Ode3C1T.incipit = "O all-pure Mother of God…"
meMA.Ode3C1T.text = "O all-pure Mother of God, I shall not be put to shame, for I have you as my helper. I shall be delivered from my enemies, for I have you as my protectress."

meMA.Ode3Kathisma1.mode = miscellanea_en_US_dedes.misc.Mode1
meMA.Ode3Kathisma1.greekmelody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.name
meMA.Ode3Kathisma1.incipit = "Your holy memory, like the…"
meMA.Ode3Kathisma1.text = "Your holy memory, like the sun, has arisen * on us, shining its light on creation, wise Basil, * and making it luminous with your God-given miracles. * Celebrating it today, we pray you and cry out: * Holy Hierarch, on our behalf make entreaty * to Christ to save our souls."

meMA.Ode3KathismaT.mode = miscellanea_en_US_dedes.misc.Mode1
meMA.Ode3KathismaT.melody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.name
meMA.Ode3KathismaT.incipit = "…"
meMA.Ode3KathismaT.text = ""

meMA.Ode3KathismaSt.mode = miscellanea_en_US_dedes.misc.Mode1
meMA.Ode3KathismaSt.melody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.name
meMA.Ode3KathismaSt.incipit = "The bright and brilliant light…"
meMA.Ode3KathismaSt.text = "The bright and brilliant light of the sunshine was hidden * when You were lifted up on the Cross, Lord and Logos. * Amazed was creation all, and it shuddered and quaked in fear. * And your Mother stood nearby and wept as she uttered, “My beloved Child, alas, my light never-setting, * You die voluntarily!”"

