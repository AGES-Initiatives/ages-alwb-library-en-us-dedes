meMA.Ode5C11.greekmelody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.greekname
meMA.Ode5C11.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C11.incipit = "O God-bearer, the grace of…"
meMA.Ode5C11.text = "O God-bearer, the grace of God came and dwelt in you; it made you a most faithful pastor of the Church."

meMA.Ode5C12.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C12.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C12.incipit = "O glorious Basil, you followed…"
meMA.Ode5C12.text = "O glorious Basil, you followed after the spirit of life, and thus you deadened the passions and the mind set on the flesh."

meMA.Ode5C13.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C13.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C13.incipit = "O godly-minded Father, you led…"
meMA.Ode5C13.text = "O godly-minded Father, you led your whole life in chastity and righteousness; then you received the reward for virtue."

meMA.Ode5C1T.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C1T.incipit = "All-blameless Mother and Virgin, all-holy…"
meMA.Ode5C1T.text = "All-blameless Mother and Virgin, all-holy sanctuary, you gave birth to the Lord, the source of dispassion."

meMA.Ode6C1H.ode = miscellanea_en_US_dedes.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_en_US_dedes.misc.Mode8
