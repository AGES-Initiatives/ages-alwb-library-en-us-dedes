meVE.SticGlory.poet = poets_en_US_dedes.po.Vyzantos
meVE.SticGlory.mode = miscellanea_en_US_dedes.misc.Mode2
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "You bowed Your head before…"
meVE.SticGlory.text = "You bowed Your head before the Forerunner; You crushed the heads of the dragons; You came to the river; and you enlightened the universe, to glorify You, O Savior, the illumination of our souls."

meVE.ProphecyTroparion1.mode = miscellanea_en_US_dedes.misc.Mode5
meVE.ProphecyTroparion1.incipit = "You appeared in the world…"
meVE.ProphecyTroparion1.text = "You appeared in the world, even as the world’s Creator, to illumine those who sit in darkness. O benevolent Lord, glory to You!"
meVE.ProphecyTroparion1Refrain.incipit = "To illumine those who sit…"
meVE.ProphecyTroparion1Refrain.text = "To illumine those who sit in darkness. O benevolent Lord, glory to You!"

meVE.ProphecyTroparion2.mode = miscellanea_en_US_dedes.misc.Mode6
meVE.ProphecyTroparion2.incipit = "By the greatness of Your…"
meVE.ProphecyTroparion2.text = "By the greatness of Your mercy, You, O Savior, showed yourself to sinners and publicans. Where else was Your light to shine, if not among those who sit in darkness. Glory to You."
meVE.ProphecyTroparion2Refrain.incipit = "Where else was Your light…"
meVE.ProphecyTroparion2Refrain.text = "Where else was Your light to shine, if not among those who sit in darkness. Glory to You."

