meMA.Ode3KathismaT.melody = ""
meMA.Ode3KathismaT.incipit = me.m05.d19_en_US_dedes.meMA.Ode3KathismaT.incipit
meMA.Ode3KathismaT.text = me.m05.d19_en_US_dedes.meMA.Ode3KathismaT.text

meMA.Ode4C11.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C11.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C11.greekmelody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.greekname
meMA.Ode4C11.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C11.incipit = "O Constantine, Christ the Lord…"
meMA.Ode4C11.text = "O Constantine, Christ the Lord caught you in his net from heaven, like He did Paul of old, and He taught you to worship Him as the only King."

meMA.Ode4C12.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C12.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C12.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C12.incipit = "Christ the Sun, using stars…"
meMA.Ode4C12.text = "Christ the Sun, using stars, shined a very bright sign on you, O blessed Saint, and He made of you a luminary for those who were in darkness."

meMA.Ode4C13.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C13.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C13.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C13.incipit = "O blessed Helen, you were…"
meMA.Ode4C13.text = "O blessed Helen, you were God-loving in manner and admirable in your godly deeds. Therefore we faithfully glorify you."

meMA.Ode4C14.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C14.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C14.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C14.incipit = "O Helen, you uncovered the…"
meMA.Ode4C14.text = "O Helen, you uncovered the divine trophy of the Cross, which for many years was hidden, and by which we have been saved and have been delivered from demonic deception."

meMA.Ode4C1T.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C1T.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C1T.incipit = "O Ever-virgin, who gave birth…"
meMA.Ode4C1T.text = "O Ever-virgin, who gave birth to the Sun of Righteousness, shine light into my soul, for my transgressions have darkened it."

meMA.Ode5C11.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C11.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C11.greekmelody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.greekname
meMA.Ode5C11.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C11.incipit = "O divinely wise Emperor, you…"
meMA.Ode5C11.text = "O divinely wise Emperor, you woke up to the never-setting Sun and Master, and you were filled with light."

meMA.Ode5C12.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C12.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C12.incipit = "O Helen, with love and…"
meMA.Ode5C12.text = "O Helen, with love and perfect sympathy as as garment of royal purple, you are dwelling in the royal courts on high."

meMA.Ode5C13.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C13.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C13.incipit = "O Helen, you served God…"
meMA.Ode5C13.text = "O Helen, you served God by your virtuous deeds. Now you join the choirs of the bodiless hosts."

meMA.Ode5C1T.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C1T.incipit = "O Virgin, purify my soul…"
meMA.Ode5C1T.text = "O Virgin, purify my soul, for it is defiled by the pleasures of the body, through the spite of the serpent."

