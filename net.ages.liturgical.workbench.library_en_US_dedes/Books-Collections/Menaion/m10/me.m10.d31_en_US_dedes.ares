meMA.Ode1C11.ode = miscellanea_en_US_dedes.misc.Ode1
meMA.Ode1C11.mode = miscellanea_en_US_dedes.misc.Mode1
meMA.Ode1C11.melody = he.h.m1_en_US_dedes.heHE.SouITropaiouchos.notmetered
meMA.Ode1C11.incipit = "O heralds of God…"
meMA.Ode1C11.text = "O heralds of God, as we are honoring your sacred memory and joyfully celebrating it, free us from the mist of the passions, and make us partakers of the eternal joy."

meMA.Ode1C12.ode = miscellanea_en_US_dedes.misc.Ode1
meMA.Ode1C12.mode = miscellanea_en_US_dedes.misc.Mode1
meMA.Ode1C12.melody = he.h.m1_en_US_dedes.heHE.SouITropaiouchos.notmetered
meMA.Ode1C12.incipit = "O all-blessed Apostles…"
meMA.Ode1C12.text = "O all-blessed Apostles, you followed in the steps of Christ, who became incarnate for mercy's sake, and you obeyed His divine commands and became guides to the correct faith for all who had gone astray."

meMA.Ode1C13.ode = miscellanea_en_US_dedes.misc.Ode1
meMA.Ode1C13.mode = miscellanea_en_US_dedes.misc.Mode1
meMA.Ode1C13.melody = he.h.m1_en_US_dedes.heHE.SouITropaiouchos.notmetered
meMA.Ode1C13.incipit = "Let us faithfully assemble…"
meMA.Ode1C13.text = "Let us faithfully assemble and dutifully call them blessed who were disciples of Christ: Amplias, Apelles, Stachys, Urban, Aristobulus, and Narcissus."

meMA.Ode1C1T.ode = miscellanea_en_US_dedes.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_en_US_dedes.misc.Mode1
meMA.Ode1C1T.melody = he.h.m1_en_US_dedes.heHE.SouITropaiouchos.notmetered
meMA.Ode1C1T.incipit = "The Virgin Mary is blessed…"
meMA.Ode1C1T.text = "The Virgin Mary is blessed among women. She is called the famous mountain of God, the holy ark and table, the golden jar, the pure palace of the Lord. Let us call here blessed."

