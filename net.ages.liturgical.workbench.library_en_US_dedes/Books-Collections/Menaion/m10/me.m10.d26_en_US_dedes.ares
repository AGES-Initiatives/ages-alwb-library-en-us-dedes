meMA.Ode4C11.mode = miscellanea_en_US_dedes.misc.Mode6
meMA.Ode4C11.greekmelody = he.h.m6_en_US_dedes.heHE.ChristosMouDynamis.greekname
meMA.Ode4C11.melody = he.h.m6_en_US_dedes.heHE.ChristosMouDynamis.notmetered
meMA.Ode4C11.incipit = "O Lord and Master, You…"
meMA.Ode4C11.text = "O Lord and Master, You shake the earth, wishing to establish on the foundation of truth all of us who are shaken by the assaults of the wily enemy."

meMA.Ode4C12.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C12.mode = miscellanea_en_US_dedes.misc.Mode6
meMA.Ode4C12.melody = he.h.m6_en_US_dedes.heHE.ChristosMouDynamis.notmetered
meMA.Ode4C12.incipit = "O Lord and Master, at…"
meMA.Ode4C12.text = "O Lord and Master, at Your divine command, You quake the universe, and You make the hearts of them that dwell on the earth to tremble. We pray You, hold back the waves of Your righteous wrath."

meMA.Ode4C13.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C13.mode = miscellanea_en_US_dedes.misc.Mode6
meMA.Ode4C13.melody = he.h.m6_en_US_dedes.heHE.ChristosMouDynamis.notmetered
meMA.Ode4C13.incipit = "When we forget the fear…"
meMA.Ode4C13.text = "When we forget the fear of You, O Lord, You frighten us with the quaking of the earth. So we pray You, the only sympathetic God, as always show us how wonderful Your mercies are towards us."

meMA.Ode4C1T.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_en_US_dedes.misc.Mode6
meMA.Ode4C1T.melody = he.h.m6_en_US_dedes.heHE.ChristosMouDynamis.notmetered
meMA.Ode4C1T.incipit = "O Lady, knowing you to…"
meMA.Ode4C1T.text = "O Lady, knowing you to be the temple of God, in your holy temple now we lift up pitiful hands in earnest prayer. See our trouble and and lend your help to us."

