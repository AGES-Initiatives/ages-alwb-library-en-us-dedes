meVE.SticTheotokion.melody = ""
meVE.SticTheotokion.incipit = oc.m6.d1_en_US_dedes.ocVE.SticTheotokion.incipit
meVE.SticTheotokion.text = oc.m6.d1_en_US_dedes.ocVE.SticTheotokion.text

meVE.LityGlory.mode = miscellanea_en_US_dedes.misc.Mode2
meVE.LityGlory.melody = ""
meVE.LityGlory.incipit = "Come, all people…"
meVE.LityGlory.text = "Come, all people, let us celebrate today the feast in honor of the victorious Martyr Barbara. Nothing could defeat her: neither sword, nor fire, nor any other torture, not even the traps of the conniving devil. “O all-glorious and magnificent Martyr, entreat Christ to grant us great mercy.”"

