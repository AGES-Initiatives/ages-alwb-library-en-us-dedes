meVE.Aposticha1.mode = miscellanea_en_US_dedes.misc.Mode2
meVE.Aposticha1.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meVE.Aposticha1.incipit = "Acting as God, O Christ…"
meVE.Aposticha1.text = "Acting as God, O Christ, * You put on all of Adam * the first man, from the Virgin, * and being born You lay in a manger in a tiny cave."

meVE.Aposticha2.mode = miscellanea_en_US_dedes.misc.Mode2
meVE.Aposticha2.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meVE.Aposticha2.incipit = "David, recite your Psalms *…"
meVE.Aposticha2.text = "David, recite your Psalms * prophetically and strum them. * Today is Christ the Savior * begotten from your body, * from which the Theotokos came."

meVE.Aposticha3.mode = miscellanea_en_US_dedes.misc.Mode2
meVE.Aposticha3.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meVE.Aposticha3.incipit = "Let us the faithful praise…"
meVE.Aposticha3.text = "Let us the faithful praise * with Shepherds and the Magi, * and with the holy Angels * give glory to our God, whom the Virgin Mary bore today."

