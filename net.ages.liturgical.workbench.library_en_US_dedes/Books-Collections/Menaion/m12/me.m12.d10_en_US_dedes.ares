meVE.SticStavrotheotokion.incipit = me.m01.d21_en_US_dedes.meVE.SticStavrotheotokion.incipit
meVE.SticStavrotheotokion.text = me.m01.d21_en_US_dedes.meVE.SticStavrotheotokion.text

meVE.AposGlory.mode = miscellanea_en_US_dedes.misc.Mode6
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "Today the wise Martyrs, Menas…"
meVE.AposGlory.text = "Today the wise Martyrs, Menas, Evgraphos and Hermogenes, are honored as pious defenders of the faithful, for they brought down all the ungodliness of the tyrants. They are three in number, like the tri-luminous Holy Trinity, and they are illumined by the cloud of divine illumination. They rejoice now with the bodiless hosts, and they entreat our God and Savior, on behalf of our souls."

meVE.AposTheotokion.mode = miscellanea_en_US_dedes.misc.Mode6
meVE.AposTheotokion.greekmelody = he.a.m6_en_US_dedes.heAU.TriimerosAnestis.greekname
meVE.AposTheotokion.melody = he.a.m6_en_US_dedes.heAU.TriimerosAnestis.name
meVE.AposTheotokion.incipit = "All-holy Theotokos, you held *…"
meVE.AposTheotokion.text = "All-holy Theotokos, you held * our God in your immaculate arms. * We entreat you, the good hope of all the world, * extend them over us now, * and shelter us from every * violent tragedy, O Virgin pure."

meVE.AposStavrotheotokion.mode = miscellanea_en_US_dedes.misc.Mode6
meVE.AposStavrotheotokion.greekmelody = he.a.m6_en_US_dedes.heAU.TriimerosAnestis.greekname
meVE.AposStavrotheotokion.melody = he.a.m6_en_US_dedes.heAU.TriimerosAnestis.name
meVE.AposStavrotheotokion.incipit = "O Lord, when You were…"
meVE.AposStavrotheotokion.text = "O Lord, when You were crucified * You shook the earth, longsuffering Christ, * while confirming * the souls of those who believed. * Therefore with love and longing * we worship and extol You, whose power is incomprehensible."

