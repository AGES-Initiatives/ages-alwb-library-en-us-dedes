meMA.Aposticha1.text = me.m08.d05_en_US_dedes.meVE.Aposticha3.text

meMA.Aposticha2.mode = miscellanea_en_US_dedes.misc.Mode2
meMA.Aposticha2.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meMA.Aposticha2.incipit = "Christ, of old You appeared…"
meMA.Aposticha2.text = "Christ, of old You appeared * to Moses and Elias * in clouds and in thick darkness * and gentle breeze, and now in Your glory’s uncreated light."

meMA.Aposticha3.mode = miscellanea_en_US_dedes.misc.Mode2
meMA.Aposticha3.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meMA.Aposticha3.incipit = "Savior, the universe * was…"
meMA.Aposticha3.text = "Savior, the universe * was brightened when on Tabor * the triple-shining splendor * of Your divine glory like lightning flashed ineffably."

