meMA.Aposticha1.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meMA.Aposticha1.incipit = me.m08.d05_en_US_dedes.meVE.Aposticha3.incipit
meMA.Aposticha1.text = me.m08.d05_en_US_dedes.meVE.Aposticha3.text

meMA.Aposticha2.mode = misc_en_US_dedes.Mode2
meMA.Aposticha2.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meMA.Aposticha2.incipit = "Christ, of old You appeared…"
meMA.Aposticha2.text = "Christ, of old You appeared * to Moses and Elias * in clouds and in thick darkness * and gentle breeze, and now in Your glory's uncreated light."

meMA.Aposticha3.mode = misc_en_US_dedes.Mode2
