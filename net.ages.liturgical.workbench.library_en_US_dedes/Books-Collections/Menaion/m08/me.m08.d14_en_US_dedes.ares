meVE.Aposticha1.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meVE.Aposticha1.incipit = "Strange and novel indeed, *…"
meVE.Aposticha1.text = "Strange and novel indeed, * and wondrous is this marvel! * How the Virgin who bore Life * succumbs to death and now is * buried, enclosed within a tomb."

meVE.Aposticha2.mode = miscellanea_en_US_dedes.misc.Mode2
meVE.Aposticha2.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meVE.Aposticha2.incipit = "Let all humanity * now…"
meVE.Aposticha2.text = "Let all humanity * now dance in celebration. * Behold the blessed Virgin, * the daughter of Adam, * is now assumed into the heights."

meVE.Aposticha3.mode = miscellanea_en_US_dedes.misc.Mode2
meVE.Aposticha3.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meVE.Aposticha3.incipit = "Picture this in your mind:…"
meVE.Aposticha3.text = "Picture this in your mind: * The chorus of Apostles * is solemnly surrounding * the bier of the Virgin * and singing at her funeral."

