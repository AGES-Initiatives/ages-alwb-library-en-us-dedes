meMA.Ode9C11.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode9C11.greekmelody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.greekname
meMA.Ode9C11.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.notmetered
meMA.Ode9C11.incipit = "You are more radiant than…"
meMA.Ode9C11.text = "You are more radiant than all that dwell in Heaven. Do therefore illumine and make radiant with the splendor of your graces those who extol your radiant Dormition."

meMA.Ode9C12.ode = miscellanea_en_US_dedes.misc.Ode9
meMA.Ode9C12.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode9C12.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.notmetered
meMA.Ode9C12.incipit = "O Christians, let your hearts…"
meMA.Ode9C12.text = "O Christians, let your hearts skip for joy at the Dormition of the Theotokos, saved as you are by her unsleeping intercession."

meMA.Ode9C13.ode = miscellanea_en_US_dedes.misc.Ode9
meMA.Ode9C13.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode9C13.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.notmetered
meMA.Ode9C13.incipit = "O Maiden, earth keeps festival…"
meMA.Ode9C13.text = "O Maiden, earth keeps festival, and Angels leap for joy, and all the righteous are glad, on the occasion of your Dormition, as they sing hymns to you."

meMA.Ode9C14.ode = miscellanea_en_US_dedes.misc.Ode9
meMA.Ode9C14.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode9C14.melody = he.h.m8_en_US_dedes.heHE.KyriosTheotokon.notmetered
meMA.Ode9C14.incipit = "O good and loving Virgin…"
meMA.Ode9C14.text = "O good and loving Virgin, who gave birth to the All-good Logos, shower your benevolent mercies on us all, and save us."

