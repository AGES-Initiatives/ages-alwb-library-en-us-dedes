meVE.SticTheotokion.incipit = he.o_en_US_dedes.heOT.TheotokeSyEi.incipit
meVE.SticTheotokion.text = he.o_en_US_dedes.heOT.TheotokeSyEiApostolon.text

meVE.AposGlory.mode = miscellanea_en_US_dedes.misc.Mode6
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "O Apostle Titus, you were…"
meVE.AposGlory.text = "O Apostle Titus, you were a disciple of God’s preacher, Paul, that chosen vessel; and from him you learned the divine teachings. Then you were sent to the Gentiles, to convert them to the Faith, and to enlighten them by the radiance of your words. So you went to the ends of the earth, declaring to everyone the good news of God incarnate. We entreat you to pray to Him earnestly, to save us who faithfully observe your forever august memory."

meVE.AposTheotokion.mode = miscellanea_en_US_dedes.misc.Mode6
meVE.AposTheotokion.greekmelody = he.a.m6_en_US_dedes.heAU.TriimerosAnestis.greekname
meVE.AposTheotokion.melody = he.a.m6_en_US_dedes.heAU.TriimerosAnestis.name
meVE.AposTheotokion.incipit = "All-blameless Virgin, earnestly * entreat…"
meVE.AposTheotokion.text = "All-blameless Virgin, earnestly * entreat the Lord, to whom you gave birth, * to have mercy * on this wretched soul of mine, * and in the hour of judgment, * in His surpassing goodness, * to give a place to me among His elect."

