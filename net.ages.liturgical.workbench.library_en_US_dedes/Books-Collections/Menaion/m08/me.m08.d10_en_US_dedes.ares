meMA.Aposticha1.mode = miscellanea_en_US_dedes.misc.Mode2
meMA.Aposticha1.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meMA.Aposticha1.incipit = "Jesus, the Father's voice *…"
meMA.Aposticha1.text = "Jesus, the Father's voice * was clearly heard from heaven * at Your transfiguration. * It uttered, \"All you, hear Him, for this is my beloved Son.\""

meMA.Aposticha2.mode = miscellanea_en_US_dedes.misc.Mode2
meMA.Aposticha2.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meMA.Aposticha2.incipit = "Let us the faithful climb…"
meMA.Aposticha2.text = "Let us the faithful climb * noetically the mountain * of godly contemplation, * so that we too may gaze on the glory of our Savior God."

meMA.Aposticha3.mode = miscellanea_en_US_dedes.misc.Mode2
meMA.Aposticha3.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meMA.Aposticha3.incipit = "King David, who composed *…"
meMA.Aposticha3.text = "King David, who composed * the Psalms, O Christ my Savior, * cried out and said that clearly * Mount Tabor and Mount Hermon shall greatly in Your name rejoice."

meMA.AposGlory.mode = miscellanea_en_US_dedes.misc.Mode2
meMA.AposGlory.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
meMA.AposGlory.incipit = "Master and King of all…"
meMA.AposGlory.text = "Master and King of all, * grant peace unto our planet * hearing the intercessions * of the all-holy Virgin, the Queen of all, and all Your saints."


