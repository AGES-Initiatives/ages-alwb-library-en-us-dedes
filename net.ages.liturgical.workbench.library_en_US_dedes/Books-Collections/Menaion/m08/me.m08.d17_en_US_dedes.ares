meVE.Aposticha1.mode = misc_en_US_dedes.Mode2
meVE.Aposticha1.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meVE.Aposticha1.incipit = "From everywhere the host *…"
meVE.Aposticha1.text = "From everywhere the host * of Christ's Disciples gathered * and joined the holy Angels, * Mother of God, to bury * your body that had hosted God."

meVE.Aposticha2.mode = misc_en_US_dedes.Mode2
meVE.Aposticha2.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meVE.Aposticha2.incipit = "Come, all we born on…"
meVE.Aposticha2.text = "Come, all we born on earth, * today, and join the Angels * to form a merry choir, * and sing at the Dormition * of the only Mother of God."

meVE.Aposticha3.mode = misc_en_US_dedes.Mode2
meVE.Aposticha3.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meVE.Aposticha3.incipit = "Holy and undefiled, * O…"
meVE.Aposticha3.text = "Holy and undefiled, * O Lady, was your body. * When by the law's of nature * you died an it was buried, * it stays completely incorrupt."

