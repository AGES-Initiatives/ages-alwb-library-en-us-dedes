meVE.Aposticha2.text = "Prophet and priest, receive * into the holy Temple * the only blameless Maiden, * the spiritual mountain, * the holy table, bridge and jar."

meVE.Aposticha3.mode = miscellanea_en_US_dedes.misc.Mode2
