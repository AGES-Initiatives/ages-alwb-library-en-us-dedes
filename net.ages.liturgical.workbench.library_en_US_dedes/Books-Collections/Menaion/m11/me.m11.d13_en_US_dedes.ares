meVE.AposGlory.mode = misc_en_US_dedes.Mode6
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "You were like a trumpet"
meVE.AposGlory.text = "You were like a trumpet making golden sounds, O Chrysostom, the golden mouth producing golden words; and you made golden the hearts of the believers with your teachings wrought of gold. As the prophet said, the sound of your doctrines went out, O devout Father, and you illumined all the ends of the earth."

