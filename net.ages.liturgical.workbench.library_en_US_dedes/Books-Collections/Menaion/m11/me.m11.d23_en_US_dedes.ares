meMA.Exaposteilarion1.mode = misc_en_US_dedes.Mode3
meMA.Exaposteilarion1.melody = he.a.m3_en_US_dedes.EnPnevmatiToIero.notmetered
meMA.Exaposteilarion1.incipit = "O devout Amphilochios and blessed…"
meMA.Exaposteilarion1.text = "O devout Amphilochios and blessed Gregory, you were truly devout hierarchs and God-bearing shepherds of the Lamb and Shepherd. Intercede for us all, who observe your memory, that we may be delivered from dangers to our souls and bodies."

meMA.Exaposteilarion2.mode = misc_en_US_dedes.Mode3
meMA.Exaposteilarion2.melody = he.a.m3_en_US_dedes.EnPnevmatiToIero.name
meMA.Exaposteilarion2.incipit = "The Temple receives you today…"
meMA.Exaposteilarion2.text = "The Temple receives you today * into it inner sanctum, * O pure virgin Mother of God, * at the hands of the High Priest. * And therein from the age of three * until you were twelve years old * you remained and received your food * at the hands of an Angel, * since you were the holy ark * of God who created all things."

meMA.Aposticha1.mode = misc_en_US_dedes.Mode2
meMA.Aposticha1.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meMA.Aposticha1.incipit = "Now, by the Entry of…"
meMA.Aposticha1.text = "Now, by the Entry of * the very Theotokos, * the wall of separation * is broken down, and joined are the things on high with us below."

meMA.Aposticha2.mode = misc_en_US_dedes.Mode2
meMA.Aposticha2.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meMA.Aposticha2.incipit = "Choirs of virgins sang *…"
meMA.Aposticha2.text = "Choirs of virgins sang * a sacred and divine hymn * as they escorted Mary * the only blameless Maiden, * with lights into the house of God."

meMA.Aposticha3.mode = misc_en_US_dedes.Mode2
meMA.Aposticha3.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
