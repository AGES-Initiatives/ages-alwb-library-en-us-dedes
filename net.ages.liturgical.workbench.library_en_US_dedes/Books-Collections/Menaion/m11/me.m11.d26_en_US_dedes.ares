meDate.text = "November 26"
meCommemoration.text = "Memory of our Devout Fathers Alypios the Stylite and Nikon \"Repent\"" // full title of day
meShortCommemoration1.text = "Memory of St. Alypios the Stylite"
meCom1.title = "For St. Alypios."
meCom2.title = "For St. Nikon."
meInsert1 = "and our devout fathers Alypios the Stylite and Nikon \"Repent,\"" // for use in petitions and dismissal
meInsert2 = "whose memory we observe," // for use in petitions and dismissal
