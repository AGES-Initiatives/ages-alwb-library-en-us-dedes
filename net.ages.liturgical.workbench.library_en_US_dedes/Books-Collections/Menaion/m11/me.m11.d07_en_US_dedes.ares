meMA.Ode3C21.ode = misc_en_US_dedes.Ode3
meMA.Ode3C21.mode = misc_en_US_dedes.Mode4
meMA.Ode3C21.melody = he.h.m4_en_US_dedes.TousSousYmnologous.notmetered
meMA.Ode3C21.incipit = "Wise Lazarus, empowered…"
meMA.Ode3C21.text = "Wise Lazarus, empowered by the Master's might, you proved the evil one's devices against you to be children's darts. We pray you give us the strength to escape the devil's machinations."

meMA.Ode3C22.ode = misc_en_US_dedes.Ode3
meMA.Ode3C22.mode = misc_en_US_dedes.Mode4
meMA.Ode3C22.melody = he.h.m4_en_US_dedes.TousSousYmnologous.notmetered
meMA.Ode3C22.incipit = "O Lazarus, you were seen…"
meMA.Ode3C22.text = "O Lazarus, you were seen as an angel in body, joining the angels in their liturgy to God, transcending the heaviness of flesh and nature, shining with divine light, and famous for miracles."

meMA.Ode3C2T.ode = misc_en_US_dedes.Ode3
meMA.Ode3C2T.mode = misc_en_US_dedes.Mode4
meMA.Ode3C2T.melody = he.h.m4_en_US_dedes.TousSousYmnologous.notmetered
meMA.Ode3C2T.incipit = "Mother of God, be my intercessor…"
meMA.Ode3C2T.text = "Mother of God, be my intercessor, protection, and defender. Guide my life, shelter me from enemies, save me in your compassion, and reconcile me with your Son."

