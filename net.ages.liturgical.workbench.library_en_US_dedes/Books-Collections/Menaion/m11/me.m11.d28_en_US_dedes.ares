meMA.Ode4C11.ode = misc_en_US_dedes.Ode4
meMA.Ode4C11.mode = misc_en_US_dedes.Mode6
meMA.Ode4C11.melody = he.h.m6_en_US_dedes.ChristosMouDynamis.notmetered
meMA.Ode4C11.incipit = "God-inspired Father Stephen…"
meMA.Ode4C11.text = "God-inspired Father Stephen, you shone with the light of the Spirit. Imitating your Lord and God, you gave sight to the blind through your prayers to God."

meMA.Ode4C12.ode = misc_en_US_dedes.Ode4
meMA.Ode4C12.mode = misc_en_US_dedes.Mode6
meMA.Ode4C12.melody = he.h.m6_en_US_dedes.ChristosMouDynamis.notmetered
meMA.Ode4C12.incipit = "Blessed Father Stephen…"
meMA.Ode4C12.text = "Blessed Father Stephen, you appeared from a distance to people at sea, when they invoked your name with faith, and by divine grace you piloted them to a tranquil port."

meMA.Ode4C13.ode = misc_en_US_dedes.Ode4
meMA.Ode4C13.mode = misc_en_US_dedes.Mode6
meMA.Ode4C13.melody = he.h.m6_en_US_dedes.ChristosMouDynamis.notmetered
meMA.Ode4C13.incipit = "O Stephen, when you were sacrificed…"
meMA.Ode4C13.text = "O Stephen, when you were sacrificed, you were offered as a sacred victim to Him who had been sacrificed for you; and you joyfully retired to the dwellings of the first-born saints."

meMA.Ode4C14.ode = misc_en_US_dedes.Ode4
meMA.Ode4C14.mode = misc_en_US_dedes.Mode6
meMA.Ode4C14.melody = he.h.m6_en_US_dedes.ChristosMouDynamis.notmetered
meMA.Ode4C14.incipit = "O Martyr, a man who …"
meMA.Ode4C14.text = "O Martyr, a man who was half-withered was completely healed at your holy word, and he marveled at the grace that God had so abundantly given you from on high for the benefit of your fellow man."

meMA.Ode4C1T.ode = misc_en_US_dedes.Ode4
meMA.Ode4C1T.mode = misc_en_US_dedes.Mode6
meMA.Ode4C1T.melody = he.h.m6_en_US_dedes.ChristosMouDynamis.notmetered
meMA.Ode4C1T.incipit = "O all-blameless one…"
meMA.Ode4C1T.text = "O all-blameless one, I have put in you my hope of salvation. I have fled to your shelter. O Maiden, please be my helper, and rescue me from disasters."

