meMA.Exaposteilarion1.mode = misc_en_US_dedes.Mode3
meMA.Exaposteilarion1.melody = he.a.m3_en_US_dedes.EpeskepsatoImas.name
meMA.Exaposteilarion1.incipit = "O Stephen, you received *…"
meMA.Exaposteilarion1.text = "O Stephen, you received * a double crown from Christ the King, * for extreme asceticism * and martyrdom in the defense * of veneration of His image, * and that of all the Saints. * With them remember us to Him."

meMA.Exaposteilarion2.mode = misc_en_US_dedes.Mode3
meMA.Exaposteilarion2.melody = he.a.m3_en_US_dedes.EpeskepsatoImas.name
meMA.Exaposteilarion2.incipit = "O Stephen, you put to…"
meMA.Exaposteilarion2.text = "O Stephen, you put to shame * those who refused as godless men * to honor the icon of Christ. * And thus you were decorated * with handsome marks of martyrdom. * Therefore we will faithfully forever pronounce you blest."

meMA.ExapTheotokion.mode = misc_en_US_dedes.Mode3
meMA.ExapTheotokion.melody = he.a.m3_en_US_dedes.EpeskepsatoImas.name
meMA.ExapTheotokion.incipit = "Only mother who knew no…"
meMA.ExapTheotokion.text = "Only mother who knew no man, * Virgin pure and immaculate, * all-holy Queen and Lady, * we beseech you to entreat * our Redeemer born of you, * to save the world from every * evil and tribulation."

meMA.AposGlory.mode = misc_en_US_dedes.Mode3
meMA.AposGlory.melody = ""
meMA.AposGlory.incipit = "O devout Stephen, like another…"
meMA.AposGlory.text = "O devout Stephen, like another Samuel, you were dedicated to God from your mother's womb. As your name means crown, you proved to be a crown of the faithful, a staff and foundation of the true Faith, and a pure dwelling place for the Holy Spirit. Pray to God, that He grant great mercy to us who honor you."

meMA.AposTheotokion.mode = misc_en_US_dedes.Mode3
meMA.AposTheotokion.melody = ""
