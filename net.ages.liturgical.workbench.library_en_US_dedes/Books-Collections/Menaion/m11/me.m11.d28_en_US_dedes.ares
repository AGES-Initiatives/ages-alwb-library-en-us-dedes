meMA.Ode1C11.ode = misc_en_US_dedes.Ode1
meMA.Ode1C11.mode = misc_en_US_dedes.Mode6
meMA.Ode1C11.melody = he.h.m6_en_US_dedes.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C11.incipit = "O blessed Stephen…"
meMA.Ode1C11.text = "O blessed Stephen, you walked the narrow path of asceticism joyfully. You restricted your adversaries' attacks with the breadth of martyrdom; and you have inherited life in all its spaciousness."

meMA.Ode1C12.ode = misc_en_US_dedes.Ode1
meMA.Ode1C12.mode = misc_en_US_dedes.Mode6
meMA.Ode1C12.melody = he.h.m6_en_US_dedes.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C12.incipit = "O wise Father…"
meMA.Ode1C12.text = "O wise Father, a previously fruitless root, the namesake of Hannah, bore you like Samuel of old, and she gave you to God as a gift, signifying the grace of your life."

meMA.Ode1C13.ode = misc_en_US_dedes.Ode1
meMA.Ode1C13.mode = misc_en_US_dedes.Mode6
meMA.Ode1C13.melody = he.h.m6_en_US_dedes.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C13.incipit = "O Stephen, in a sacred manner…"
meMA.Ode1C13.text = "O Stephen, in a sacred manner you joined the divine order of monastics, and like a very splendid star you shone with virtue, and you enlightened the faithful mystically."

meMA.Ode1C14.ode = misc_en_US_dedes.Ode1
meMA.Ode1C14.mode = misc_en_US_dedes.Mode6
meMA.Ode1C14.melody = he.h.m6_en_US_dedes.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C14.incipit = "O wise Stephen…"
meMA.Ode1C14.text = "O wise Stephen, you restricted your body within the enclosure in a narrow dwelling, but your mind was enlarged with divine ascents and it soared to heaven."

meMA.Ode1C1T.ode = misc_en_US_dedes.Ode1
meMA.Ode1C1T.mode = misc_en_US_dedes.Mode6
meMA.Ode1C1T.melody = he.h.m6_en_US_dedes.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C1T.incipit = "Let us the sacred people…"
meMA.Ode1C1T.text = "Let us the sacred people glorify our holy Lady with sacred voices. She is blessed among women; she is the impassable gate and the undefiled temple of chastity."

