meMA.Ode8C11.ode = miscellanea_en_US_dedes.misc.Ode8
meMA.Ode8C11.mode = miscellanea_en_US_dedes.misc.Mode6
meMA.Ode8C11.greekmelody = he.h.m6_en_US_dedes.heHE.EkFlogosToisOsiois.greekname
meMA.Ode8C11.melody = he.h.m6_en_US_dedes.heHE.EkFlogosToisOsiois.notmetered
meMA.Ode8C11.incipit = "The lamb of Christ…"
meMA.Ode8C11.text = "The lamb of Christ was arrested, beaten, killed, and buried with outlaws by maniacs who acted like animals, living in iniquity unto the ages."

meMA.Ode8C12.ode = miscellanea_en_US_dedes.misc.Ode8
meMA.Ode8C12.mode = miscellanea_en_US_dedes.misc.Mode6
meMA.Ode8C12.melody = he.h.m6_en_US_dedes.heHE.EkFlogosToisOsiois.notmetered
meMA.Ode8C12.incipit = "The enemy brutally crushed…"
meMA.Ode8C12.text = "The enemy brutally crushed your skull, and the Most High God crowned it with the crown of victory, for your Herculean labors, O Stephen, the pride of the martyrs and all the devout saints."

meMA.Ode8C13.ode = miscellanea_en_US_dedes.misc.Ode8
meMA.Ode8C13.mode = miscellanea_en_US_dedes.misc.Mode6
meMA.Ode8C13.melody = he.h.m6_en_US_dedes.heHE.EkFlogosToisOsiois.notmetered
meMA.Ode8C13.incipit = "Godly-minded Andrew …"
meMA.Ode8C13.text = "Godly-minded Andrew staunchly opposed the persecutor, and he was wounded and mutilated. This righteous man was mercilessly put to death, and he sings to the Master Christ unto the ages."

meMA.Ode8C14.ode = miscellanea_en_US_dedes.misc.Ode8
meMA.Ode8C14.mode = miscellanea_en_US_dedes.misc.Mode6
meMA.Ode8C14.melody = he.h.m6_en_US_dedes.heHE.EkFlogosToisOsiois.notmetered
meMA.Ode8C14.incipit = "O Martyr St. Stephen…"
meMA.Ode8C14.text = "O Martyr St. Stephen, having become a very holy house, you had God, who rests in all the Saints, dwelling in your heart, unto all the ages."

meMA.Ode8C1T.ode = miscellanea_en_US_dedes.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_en_US_dedes.misc.Mode6
meMA.Ode8C1T.melody = he.h.m6_en_US_dedes.heHE.EkFlogosToisOsiois.notmetered
meMA.Ode8C1T.incipit = "O Virgin full of grace divine…"
meMA.Ode8C1T.text = "O Virgin full of grace divine, I who am stained with the filth of evil sin implore you to wash me, with a sprinkling of the divine blood that spilled out from the side of your Son and God."

