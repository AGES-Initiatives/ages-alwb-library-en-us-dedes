meMA.Ode9C15.incipit = "…"
meMA.Ode9C15.text = ""

meMA.Ode9C16.ode = misc_en_US_dedes.Ode9
meMA.Ode9C16.mode = misc_en_US_dedes.Mode4
meMA.Ode9C16.melody = he.h.m4_en_US_dedes.OsEmpsychoTheouKivoto.name
meMA.Ode9C16.incipit = "…"
