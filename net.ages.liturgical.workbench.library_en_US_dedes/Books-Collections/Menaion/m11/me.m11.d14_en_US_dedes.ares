alt1.meMA.Synaxarion.text = "On the 14th day of this month, the dormition of our Father among the Saints, Gregory Palamas, Archbishop of Thessaloniki."
alt1.meMA.couplet1.title = titles_en_US_dedes.ti.Verses
alt1.meMA.couplet1.verse1 = "The Light’s bright herald, the mind truly great,"
alt1.meMA.couplet1.verse2 = "is led to unsetting radiance by the Source of light."
alt1.meMA.couplet2.verse1 = "Synonymously watchful, you were joined to Christ,"
alt1.meMA.couplet2.verse2 = "In the grave you work wonders, and you live though deceased."
alt1.meMA.couplet3.verse1 = "How should anyone praise you, O Gregory,"
alt1.meMA.couplet3.verse2 = "Since you are allotted God’s honor for miracles."
alt1.meMA.single1.text = "Gregory the chief sacrificer died on the fourteenth."

