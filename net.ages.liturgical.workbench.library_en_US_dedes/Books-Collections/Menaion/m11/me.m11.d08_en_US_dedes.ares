meDA.date = "November 8"
meDA.commemoration.full = "The Synaxis in Honor of the Chief Commanders Michael and Gabriel, and All the Bodiless Powers" // full title of day
meDA.commemoration.first = "The Synaxis in Honor of the Chief Commanders Michael and Gabriel"
meDA.commemoration.first_short = "The Synaxis in Honor of the Chief Commanders Michael and Gabriel"
meDA.commemoration.first_title = "For Chief Commander Michael."
meDA.commemoration.first_name = "Archangels Michael and Gabriel"
meDA.commemoration.second_title = "For Chief Commander Gabriel."
meDA.commemoration.second_name = ""
meDA.insert1 = "the holy and glorious Supreme Commanders Michael and Gabriel, and the other bodiless hosts," // for use in petitions and dismissal
meDA.insert2 = "whose Synaxis we celebrate;" // for use in petitions and dismissal
meDA.note = ""
