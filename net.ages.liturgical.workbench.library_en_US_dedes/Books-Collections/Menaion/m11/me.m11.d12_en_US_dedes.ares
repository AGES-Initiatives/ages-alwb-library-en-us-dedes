meMA.Exaposteilarion1.melody = he.a.m3_en_US_dedes.heAU.OOuranonToisAstrois.name
meMA.Exaposteilarion1.incipit = "Let us all sing the…"
meMA.Exaposteilarion1.text = "Let us all sing the praises * of John who imitated God * the lover of tender mercies, * so that through him we may obtain * the cancellation of all debts * and remission of offences."

meMA.Exaposteilarion2.mode = miscellanea_en_US_dedes.misc.Mode3
meMA.Exaposteilarion2.melody = he.a.m3_en_US_dedes.heAU.OOuranonToisAstrois.name
meMA.Exaposteilarion2.incipit = "O godly-minded Nilus, * since…"
meMA.Exaposteilarion2.text = "O godly-minded Nilus, * since by the water of your tears * your soul is enriched and fruitful, * entreat the Lord to cleanse our souls * of all defilement, as we now * extol you with love and fervor."

