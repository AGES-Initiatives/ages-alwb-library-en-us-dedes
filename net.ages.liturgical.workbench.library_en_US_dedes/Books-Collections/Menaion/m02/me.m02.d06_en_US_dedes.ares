meVE.Stichera04.mode = misc_en_US_dedes.Mode6
meVE.Stichera04.melody = he.a.m6_en_US_dedes.TriimerosAnestis.name
meVE.Stichera04.incipit = "Your godly virtues' splendid light…"
meVE.Stichera04.text = "Your godly virtues' splendid light * shone forth from you, O Saint of God; * so He set you * on the lampstand mystically * of the holy Church of Smyrna. * O father, you illumined * with sacred teachings the believers there."

meVE.Stichera05.mode = misc_en_US_dedes.Mode6
meVE.Stichera05.melody = he.a.m6_en_US_dedes.TriimerosAnestis.name
meVE.Stichera05.incipit = "All holy father Bucolus, *…"
meVE.Stichera05.text = "All holy father Bucolus, * the Holy Spirit sanctified * you, and through you * grants to those who come in faith * divine sanctification, * also illumination * and redemption, O God-bearing Saint."

meVE.Stichera06.mode = misc_en_US_dedes.Mode6
meVE.Stichera06.melody = he.a.m6_en_US_dedes.TriimerosAnestis.name
meVE.Stichera06.incipit = "To people who were overcome…"
meVE.Stichera06.text = "To people who were overcome * by evils you proclaimed our God's * utter goodness, * and you thus did good to them, * O wise one, as a giver * of goodness; and you led them * to the Savior, sacred liturgist."

