meMA.Ode8C11.ode = miscellanea_en_US_dedes.misc.Ode8
meMA.Ode8C11.mode = miscellanea_en_US_dedes.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_en_US_dedes.heHE.LytrotaTouPantos.greekname
meMA.Ode8C11.melody = he.h.m4_en_US_dedes.heHE.LytrotaTouPantos.notmetered
meMA.Ode8C11.incipit = "O Saint, you devoted yourself…"
meMA.Ode8C11.text = "O Saint, you devoted yourself wholly to God, you shook off the comforts of the flesh, and thus you received the power to perform miracles, and you sang, “O all you works, bless and extol the Lord.”"

meMA.Ode8C12.ode = miscellanea_en_US_dedes.misc.Ode8
meMA.Ode8C12.mode = miscellanea_en_US_dedes.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_en_US_dedes.heHE.LytrotaTouPantos.notmetered
meMA.Ode8C12.incipit = "You took up your cross…"
meMA.Ode8C12.text = "You took up your cross and followed Christ. With Him as your ally, you terrified demons, and you sang, “O all you works, bless and extol the Lord.”"

meMA.Ode8C1T.ode = miscellanea_en_US_dedes.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_en_US_dedes.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_en_US_dedes.heHE.LytrotaTouPantos.notmetered
meMA.Ode8C1T.incipit = "All-pure Lady, when the Archangel…"
meMA.Ode8C1T.text = "All-pure Lady, when the Archangel appeared to you, he openly announced the ineffable conception of the Logos would save the world, and he cried out, “O all you works, bless and extol the Lord.”"

meMA.Ode9C1H.ode = miscellanea_en_US_dedes.misc.Ode9
