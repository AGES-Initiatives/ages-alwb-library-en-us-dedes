meMA.Ode7C11.ode = miscellanea_en_US_dedes.misc.Ode7
meMA.Ode7C11.mode = miscellanea_en_US_dedes.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_en_US_dedes.heHE.EnTiKaminoAvramiaioi.greekname
meMA.Ode7C11.melody = he.h.m4_en_US_dedes.heHE.EnTiKaminoAvramiaioi.notmetered
meMA.Ode7C11.incipit = "O Father, you were wounded…"
meMA.Ode7C11.text = "O Father, you were wounded with divine affection for the Master, and your heart unyieldingly longed for Him; and you cried out, “Blessed are You, my God and Lord.”"

meMA.Ode7C12.ode = miscellanea_en_US_dedes.misc.Ode7
meMA.Ode7C12.mode = miscellanea_en_US_dedes.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_en_US_dedes.heHE.EnTiKaminoAvramiaioi.notmetered
meMA.Ode7C12.incipit = "All-wise Auxentius, you mightily reproved…"
meMA.Ode7C12.text = "All-wise Auxentius, you mightily reproved the prince of darkness and cast down the rulers of this world, for you were luminous with the brightness of the Savior, and you cried out, “Blessed are You, my God and Lord.”"

meMA.Ode7C1T.ode = miscellanea_en_US_dedes.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_en_US_dedes.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_en_US_dedes.heHE.EnTiKaminoAvramiaioi.notmetered
meMA.Ode7C1T.incipit = "God the Fashioner willed to…"
meMA.Ode7C1T.text = "God the Fashioner willed to refashion human nature, which long ago had slipped and fallen from its ascent towards what is superior. So He dwelt in your womb, O all-pure Virgin who alone are extolled by all."

meMA.Ode8C1H.ode = miscellanea_en_US_dedes.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_en_US_dedes.misc.Mode4
