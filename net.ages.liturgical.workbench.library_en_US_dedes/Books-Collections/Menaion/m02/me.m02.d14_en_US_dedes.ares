meMA.Ode3C1H.incipit = he.h.m4_en_US_dedes.heHE.OukEnSofia.incipit
meMA.Ode3C1H.text = he.h.m4_en_US_dedes.heHE.OukEnSofia.text

meMA.Ode3C11.ode = miscellanea_en_US_dedes.misc.Ode3
meMA.Ode3C11.mode = miscellanea_en_US_dedes.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_en_US_dedes.heHE.OukEnSofia.greekname
meMA.Ode3C11.melody = he.h.m4_en_US_dedes.heHE.OukEnSofia.notmetered
meMA.Ode3C11.incipit = "O Saint, when you were…"
meMA.Ode3C11.text = "O Saint, when you were strengthened with the power of the Cross, by your asceticism you vanquished the violent assaults of demons; you destroyed their wiles, and you turned away their attacks."

meMA.Ode3C12.ode = miscellanea_en_US_dedes.misc.Ode3
meMA.Ode3C12.mode = miscellanea_en_US_dedes.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_en_US_dedes.heHE.OukEnSofia.notmetered
meMA.Ode3C12.incipit = "Father Auxentius, you earnestly strove…"
meMA.Ode3C12.text = "Father Auxentius, you earnestly strove for purity in prayer and perseverance in vigil, imitating on earth the life of Angels."

meMA.Ode3C13.ode = miscellanea_en_US_dedes.misc.Ode3
meMA.Ode3C13.mode = miscellanea_en_US_dedes.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_en_US_dedes.heHE.OukEnSofia.notmetered
meMA.Ode3C13.incipit = "Righteous and God-bearing Auxentius, by…"
meMA.Ode3C13.text = "Righteous and God-bearing Auxentius, by self-control you mortified the movements of sensual pleasure, because you knew that it produces the deadly poison for souls."

meMA.Ode3C1T.ode = miscellanea_en_US_dedes.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_en_US_dedes.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_en_US_dedes.heHE.OukEnSofia.notmetered
meMA.Ode3C1T.incipit = "All-blameless one, He who formed…"
meMA.Ode3C1T.text = "All-blameless one, He who formed me, a man, in the beginning from dust is formed in your womb for my sake, thus correcting the ancient fall."

meMA.Ode3Kathisma1.mode = miscellanea_en_US_dedes.misc.Mode1
meMA.Ode3Kathisma1.greekmelody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.notmetered
meMA.Ode3Kathisma1.incipit = "All-blessed Saint, you ascended the…"
meMA.Ode3Kathisma1.text = "All-blessed Saint, you ascended the mountain of contemplation and action, and you shined like the sun with the lightning of your miracles, illumining the ends of the earth. Therefore we celebrate your all-holy memory today, extolling you with faith, and we honor you with longing."

meMA.Ode3Kathisma2.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode3Kathisma2.greekmelody = he.a.m8_en_US_dedes.heAU.TinSofianKaiLogon.greekname
meMA.Ode3Kathisma2.melody = he.a.m8_en_US_dedes.heAU.TinSofianKaiLogon.notmetered
meMA.Ode3Kathisma2.incipit = "O blessed Saint, you ascended…"
meMA.Ode3Kathisma2.text = "O blessed Saint, you ascended the mountain of virtue, although you shut yourself in a tiny cell; you were narrowly confined on earth, and through ascetic practice you most wisely flew with wings of a perfect mind and ascended to the wide divine space. Therefore you were able to pull up those who had fallen into holes of temptations, and you performed terrific wonders, O devout Auxentius. Intercede with Christ our God, that He grant forgiveness of offenses, to us who with longing observe your holy memory."

meMA.Ode3KathismaT.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_en_US_dedes.heAU.TinSofianKaiLogon.notmetered
meMA.Ode3KathismaT.incipit = me.m02.d07_en_US_dedes.meMA.Ode3KathismaT.incipit
meMA.Ode3KathismaT.text = me.m02.d07_en_US_dedes.meMA.Ode3KathismaT.text

