meSV.Aposticha1.mode = misc_en_US_dedes.Mode2
meSV.Aposticha1.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meSV.Aposticha1.incipit = "In the Temple of God…"
meSV.Aposticha1.text = "In the Temple of God * today, the Savior Jesus * is offered as an infant, * and Symeon the Elder receives Him in his aged arms."

meSV.Aposticha2.mode = misc_en_US_dedes.Mode2
meSV.Aposticha2.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meSV.Aposticha2.incipit = "Christ is the burning coal…"
meSV.Aposticha2.text = "Christ is the burning coal * foreseen once by Isaiah. * And now the Theotokos, * with hands instead of tongs, gives Him to the Elder Symeon."

meSV.Aposticha3.mode = misc_en_US_dedes.Mode2
meSV.Aposticha3.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meSV.Aposticha3.incipit = "Holding with fear and joy…"
meSV.Aposticha3.text = "Holding with fear and joy * the Lord in his embraces, * Symeon blessed and lauded * the Mother of the Savior, and asked to be released from life."

meSV.AposGlory.mode = misc_en_US_dedes.Mode2
meSV.AposGlory.melody = he.a.m2_en_US_dedes.OikosTouEfratha.name
meSV.AposGlory.incipit = "\"Take now into your arms…"
meSV.AposGlory.text = "\"Take now into your arms * this babe, the Lord of glory, * who is the world's salvation.\" * So said the all-pure Mother of God to Elder Symeon."

meVE.Stichera01.poet = poets_en_US_dedes.GermanouPatriarchou
meVE.Stichera01.mode = misc_en_US_dedes.Mode1
meVE.Stichera01.melody = ""
meVE.Stichera01.incipit = "Tell us, Symeon. Whom are…"
meVE.Stichera01.text = "Tell us, Symeon. Whom are you holding in your arms, in the temple rejoicing? Whom are you speaking to and saying, \"Now I am set free, for I have seen my Savior. He is the One born of a Virgin; He is God, the Word of God, who became incarnate for us and saved humanity. Let us worship Him.\""

meVE.Stichera02.mode = misc_en_US_dedes.Mode1
meVE.Stichera02.melody = ""
meVE.Stichera02.incipit = "Symeon, receive the One whom…"
meVE.Stichera02.text = "Symeon, receive the One whom Moses saw in the dark cloud giving the Law on Sinai, and who now became an infant and is submitting to the Law. He is the One who spoke through the Law and who was spoken of by the Prophets; He became incarnate for us and saved humanity. Let us worship Him."

meVE.Stichera03.mode = misc_en_US_dedes.Mode1
meVE.Stichera03.melody = ""
meVE.Stichera03.incipit = "Let us also come; let…"
meVE.Stichera03.text = "Let us also come; let us meet Christ with inspired song, and let us receive Him whose salvation was seen by Symeon. He is the One whom David proclaimed. He is the One who spoke in the Prophets; He became incarnate for us and spoke through the Law. Let us worship Him."

