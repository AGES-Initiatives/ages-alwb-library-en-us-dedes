meMA.Ode3KathismaSt.melody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.notmetered
meMA.Ode3KathismaSt.incipit = me.m07.d08_en_US_dedes.meMA.Ode3KathismaSt.incipit
meMA.Ode3KathismaSt.text = me.m07.d08_en_US_dedes.meMA.Ode3KathismaSt.text

meMA.Ode4C11.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C11.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C11.greekmelody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.greekname
meMA.Ode4C11.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C11.incipit = "O Martyr, namesake of Christ…"
meMA.Ode4C11.text = "O Martyr, namesake of Christ, you did delight in the scourgings, because you were sweetened with divine visions of your Bridegroom and had your gaze fixed upon His beauty."

meMA.Ode4C12.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C12.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C12.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C12.incipit = "O Christina, your miserable and…"
meMA.Ode4C12.text = "O Christina, your miserable and utterly godless father threatened you with horrible torments, because he was unable to endure your longing for Christ."

meMA.Ode4C13.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C13.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C13.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C13.incipit = "O Martyr, in your sufferings…"
meMA.Ode4C13.text = "O Martyr, in your sufferings, you did cry: Wounded with longing for Your love, I have cleaved after You. Show me to be a victor. Your countenance shone with the beauty and gladness of divine glory, because it was made bright with the beautifying luminations of contests."

meMA.Ode4C1T.ode = miscellanea_en_US_dedes.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode4C1T.melody = he.h.m8_en_US_dedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C1T.incipit = "O Theotokos ever-virgin Mother, Esaias…"
meMA.Ode4C1T.text = "O Theotokos ever-virgin Mother, Esaias, glorious among the Prophets, in ancient times called you a rod that bare the Lord as a flower."

meMA.Ode5C11.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C11.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C11.greekmelody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.greekname
meMA.Ode5C11.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C11.incipit = "O Martyr, because you had…"
meMA.Ode5C11.text = "O Martyr, because you had become as an Angel in the flesh, the heavenly Angels fed you the food of the Angels."

meMA.Ode5C12.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C12.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C12.incipit = "O Christina, Because you did…"
meMA.Ode5C12.text = "O Christina, Because you did not deny the Rock of Life, your enemies bound you to a rock and cast you into the water."

meMA.Ode5C13.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C13.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C13.incipit = "You came to rest with…"
meMA.Ode5C13.text = "You came to rest with the Creator after you were caught up into the heights, flying like a dove on the pinions of the Divine Spirit."

meMA.Ode5C1T.ode = miscellanea_en_US_dedes.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_en_US_dedes.heHE.OrthrizontesVoomenSoi.notmetered
meMA.Ode5C1T.incipit = "O pure Virgin, quicken me…"
meMA.Ode5C1T.text = "O pure Virgin, quicken me who am dead in sin, for you gave birth to the sinless Life."

meMA.Ode6C11.ode = miscellanea_en_US_dedes.misc.Ode6
meMA.Ode6C11.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode6C11.greekmelody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.greekname
meMA.Ode6C11.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C11.incipit = "O prizewinner, your heart was…"
meMA.Ode6C11.text = "O prizewinner, your heart was charmed with the beauty of your most sweet Lover. While running after the His sufferings’ fragrance, you cried: I burn with longing for You, O King of all, and for Your love’s sake I am slaughtered."

meMA.Ode6C12.ode = miscellanea_en_US_dedes.misc.Ode6
meMA.Ode6C12.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode6C12.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C12.incipit = "O Martyr, when your begetter…"
meMA.Ode6C12.text = "O Martyr, when your begetter saw that you did love your Heavenly Father, he revealed his barbaric disposition and subjected you to manifold tortures; for the impious fellow did not know the limits of his own nature."

meMA.Ode6C13.ode = miscellanea_en_US_dedes.misc.Ode6
meMA.Ode6C13.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode6C13.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C13.incipit = "O modest Martyr, you put…"
meMA.Ode6C13.text = "O modest Martyr, you put forth flower as a lily in the valleys of the Martyrs, and as a sweet-smelling rose dropping the fragrant distillations of grace. You have made the hearts of the faithful to be fragrant, namesake of Christ and companion of the holy Angels."

meMA.Ode6C1T.ode = miscellanea_en_US_dedes.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode6C1T.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C1T.incipit = "O Virgin, the God-proclaiming Prophets…"
meMA.Ode6C1T.text = "O Virgin, the God-proclaiming Prophets, perceiving the depth of the mystery concerning you and illuminated from afar with the Divine Spirit, prophetically declared beforehand that you would become the very Mother of the Master of all."

meMA.Ode6H.ode = miscellanea_en_US_dedes.misc.Ode6
meMA.Ode6H.mode = miscellanea_en_US_dedes.misc.Mode8
meMA.Ode6H.greekmelody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.greekname
meMA.Ode6H.melody = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.melody
meMA.Ode6H.incipit = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.incipit
meMA.Ode6H.text = he.h.m8_en_US_dedes.heHE.TinDeisinEkcheo.text

