meMA.AposGlory.melody = ""
meMA.AposGlory.incipit = "O glorious Christina, extolled by…"
meMA.AposGlory.text = "O glorious Christina, extolled by all, when in your longing for Christ you departed the world, you were numbered among the ranks on high. And now, you entreat Him to grant us the great mercy."

