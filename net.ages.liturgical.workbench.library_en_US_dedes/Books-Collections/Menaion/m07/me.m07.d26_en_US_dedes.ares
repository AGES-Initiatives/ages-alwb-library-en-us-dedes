meLI.MM.CommunionHymn.text = properties_en_US_dedes.media.key

meMegalynarion.text = "Godly-minded martyr Paraskevi, * as the Church's bright star, and a fountain of many cures, * heaven-sent protection and shelter of the faithful, * who honor and salute you, crying aloud, \"Rejoice!\""
