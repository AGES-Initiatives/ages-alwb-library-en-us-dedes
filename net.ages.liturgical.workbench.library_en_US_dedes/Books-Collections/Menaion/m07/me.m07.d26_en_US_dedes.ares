meMA.Ode5C1T.ode = misc_en_US_dedes.Ode5
meMA.Ode5C1T.mode = misc_en_US_dedes.Mode8
meMA.Ode5C1T.melody = he.h.m8_en_US_dedes.OEkNyktosAgnoias.name
meMA.Ode5C1T.text = ""

meMA.Ode5C21.ode = misc_en_US_dedes.Ode5
meMA.Ode5C21.mode = misc_en_US_dedes.Mode4
meMA.Ode5C21.melody = he.h.m4_en_US_dedes.SyKyrieMouFos.notmetered
meMA.Ode5C21.text = "O august Paraskeve, you competed and you slew the tyrant with your all-wise sayings, intelligently proclaiming Christ before the tribunal."

meMA.Ode5C22.ode = misc_en_US_dedes.Ode5
meMA.Ode5C22.mode = misc_en_US_dedes.Mode4
meMA.Ode5C22.melody = he.h.m4_en_US_dedes.SyKyrieMouFos.notmetered
meMA.Ode5C22.text = "Your mind was set and your thoughts were steadfast, and you have put to shame all the religion of the idols using proofs from holy Scripture."

meMA.Ode5C23.ode = misc_en_US_dedes.Ode5
meMA.Ode5C23.mode = misc_en_US_dedes.Mode4
meMA.Ode5C23.melody = he.h.m4_en_US_dedes.SyKyrieMouFos.notmetered
meMA.Ode5C23.text = "You were counted worthy of the spiritual bridal chamber and wedding hall; and filled with God, you danced for joy with the wise virgins, O renowned Paraskeve."

meMA.Ode5C2T.ode = misc_en_US_dedes.Ode5
meMA.Ode5C2T.mode = misc_en_US_dedes.Mode4
meMA.Ode5C2T.melody = he.h.m4_en_US_dedes.SyKyrieMouFos.notmetered
meMA.Ode5C2T.text = "O Bride of God, we have you as an unbreakable shield against enemies; we possess you as an anchor and hope of our salvation."

meMA.Ode6C11.ode = misc_en_US_dedes.Ode6
meMA.Ode6C11.mode = misc_en_US_dedes.Mode8
