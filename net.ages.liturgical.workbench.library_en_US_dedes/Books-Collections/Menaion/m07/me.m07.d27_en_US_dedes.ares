meMA.Kathisma31.melody = he.a.m4_en_US_dedes.heAU.EpefanisSimeron.name
meMA.Kathisma31.incipit = "On this day, O martyr…"
meMA.Kathisma31.text = "On this day, O martyr saint Panteleimon, * your distinguished martyrdom is celebrated everywhere. * We glorify the Giver-of-Life, * for He has made you a powerful patron Saint."

