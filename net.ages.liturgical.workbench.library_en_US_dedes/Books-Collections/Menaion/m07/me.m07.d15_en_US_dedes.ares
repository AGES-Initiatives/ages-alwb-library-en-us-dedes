meVE.AposStavrotheotokion.mode = miscellanea_en_US_dedes.misc.Mode8
meVE.AposStavrotheotokion.greekmelody = he.a.m8_en_US_dedes.heAU.OiMartyresSouKyrie.greekname
meVE.AposStavrotheotokion.melody = he.a.m8_en_US_dedes.heAU.OiMartyresSouKyrie.name
meVE.AposStavrotheotokion.incipit = "The Virgin, who is likened…"
meVE.AposStavrotheotokion.text = "The Virgin, who is likened to * a spotless ewe who bore her Son, the young bull, * saw Him being crucified by lawless men, * and she lamented in her grief, * crying, “Woe is me, my most beloved Son. * How have these ungrateful people paid You back, * for all the good You did them! * And they wish to deprive me of You, my precious Child!”"

meVE.Apolytikion1.poet = poets_en_US_dedes.po.PatriarchouKyrillou
meVE.Apolytikion1.mode = miscellanea_en_US_dedes.misc.Mode4
meVE.Apolytikion1.greekmelody = he.a.m4_en_US_dedes.heAU.OYpsotheisEnToStavro.greekname
meVE.Apolytikion1.melody = he.a.m4_en_US_dedes.heAU.OYpsotheisEnToStavro.name
meVE.Apolytikion1.incipit = "The holy martyrs of the…"
meVE.Apolytikion1.text = "The holy martyrs of the Lord, Saint Julitta * and her all-blessed son, the three-year-old Cyricus, * appeared before the governor’s tribunal to be judged * and courageously proclaimed that they called themselves Christians. * They were not at all afraid of the threats of the tyrants. * And now in heaven they are wearing crowns * as they are standing near Christ with exultant joy."

meMA.Kathisma11.mode = miscellanea_en_US_dedes.misc.Mode1
meMA.Kathisma11.greekmelody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.greekname
meMA.Kathisma11.melody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.name
meMA.Kathisma11.incipit = "Amazed were all the hosts…"
meMA.Kathisma11.text = "Amazed were all the hosts of immaterial Angels, * beholding how, although you were a baby in body, * invincible Cyricus, you were stunning in victory, * when you wrestled with the incorporeal dragon, * and you threw him down, and you defeated him, crushing * his skull with the rock of faith."

