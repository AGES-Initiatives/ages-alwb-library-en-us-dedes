meVE.Apolytikion1.mode = miscellanea_en_US_dedes.misc.Mode8
meVE.Apolytikion1.melody = ""
meVE.Apolytikion1.incipit = "Supremely blessed are You…"
meVE.Apolytikion1.text = "Supremely blessed are You, O Christ our God. You established the holy Fathers upon the earth as beacons, and through them You have guided us all to the true Faith, O greatly merciful One, glory be to You."

