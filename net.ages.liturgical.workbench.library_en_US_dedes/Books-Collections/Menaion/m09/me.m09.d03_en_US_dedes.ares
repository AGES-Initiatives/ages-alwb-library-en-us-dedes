meVE.SticStavrotheotokion.text = oc.m6.d6_en_US_dedes.ocVE.AposTheotokion.text

meVE.AposGlory.mode = misc_en_US_dedes.Mode6
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = me.m01.d15_en_US_dedes.meVE.AposGlory.incipit
meVE.AposGlory.text = me.m01.d15_en_US_dedes.meVE.AposGlory.text

meVE.AposTheotokion.mode = misc_en_US_dedes.Mode6
meVE.AposTheotokion.melody = he.a.m6_en_US_dedes.TriimerosAnestis.name
meVE.AposTheotokion.incipit = "O Lady, all-immaculate, * the…"
meVE.AposTheotokion.text = "O Lady, all-immaculate, * the wicked devil, envious * of the people * who compose your holy flock, * is fighting to devour us. * We  pray you, Theotokos, * to keep us safe from being harmed by him."

meVE.AposStavrotheotokion.mode = misc_en_US_dedes.Mode6
meVE.AposStavrotheotokion.melody = he.a.m6_en_US_dedes.TriimerosAnestis.name
