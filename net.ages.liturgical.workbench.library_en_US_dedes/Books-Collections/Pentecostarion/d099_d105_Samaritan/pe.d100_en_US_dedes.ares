peMA.Aposticha1.mode = miscellanea_en_US_dedes.misc.Mode2
peMA.Aposticha1.greekmelody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.greekname
peMA.Aposticha1.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
peMA.Aposticha1.incipit = "Bending the heavens, Lord *…"
peMA.Aposticha1.text = "Bending the heavens, Lord * my Savior, You descended * and took flesh from a Virgin, * without mutation, gushing forgiveness of my sins to me."

peMA.Aposticha2.mode = miscellanea_en_US_dedes.misc.Mode2
peMA.Aposticha2.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
peMA.Aposticha2.incipit = "Logos, wholly in God, *…"
peMA.Aposticha2.text = "Logos, wholly in God, * You wished to lift me, fallen * in sins, and so united * yourself to me completely, O Lord who loves humanity."

peMA.Aposticha3.mode = miscellanea_en_US_dedes.misc.Mode2
peMA.Aposticha3.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
peMA.Aposticha3.incipit = "You are the King of…"
peMA.Aposticha3.text = "You are the King of all, * the great and holy Wisdom, * through whom the Father wisely * with the all-holy Spirit * created all the universe."

