peMA.Synaxarion.commemoration.text = "On this day, the Sunday after Pentecost, we celebrate the feast of All Saints, who are from every part of the world, those in Asia, Africa, Europe and America, from the North and from the South."
peMA.Synaxarion.couplet1.title = titles_en_US_dedes.Stichoi
peMA.Synaxarion.couplet1.verse1 = "I sing praises to all the Friends of my Lord."
peMA.Synaxarion.couplet1.verse2 = "If one is so destined, to their number be added."
peMA.Synaxarion.narration1.p01.text = "[paragraph 01]"
peMA.Synaxarion.narration1.p02.text = "[paragraph 02]"
