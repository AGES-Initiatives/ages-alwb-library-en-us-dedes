peSY.commemoration.text = "On this day, Thursday of the sixth week of Pascha, we celebrate the Ascension of our Lord, God and Savior Jesus Christ."
peSY.commemoration.couplet1.title = titles_en_US_dedes.Stichoi
peSY.commemoration.couplet1.verse1 = "O Word, You sat at the right of the Father,"
peSY.commemoration.couplet1.verse2 = "Having granted initiates faith that is surer."
peSY.commemoration.narration.p01 = "[paragraph 01]"
peSY.commemoration.narration.p02 = "[paragraph 02]"
peSY.commemoration.narration.p03 = "[paragraph 03]"
peSY.commemoration.narration.p04 = "[paragraph 04]"
peSY.commemoration.narration.p05 = "[paragraph 05]"
peSY.commemoration.narration.p06 = "[paragraph 06]"
peSY.commemoration.narration.p07 = "[paragraph 07]"
peSY.lastline = "Christ our God, taken up in glory, have mercy on us. Amen."

