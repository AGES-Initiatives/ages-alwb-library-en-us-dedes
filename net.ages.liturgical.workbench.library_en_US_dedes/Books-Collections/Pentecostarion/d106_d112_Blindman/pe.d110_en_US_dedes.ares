peMA.Ode9C11.ode = misc_en_US_dedes.Ode9
peMA.Ode9C11.mode = misc_en_US_dedes.Mode5
peMA.Ode9C11.melody = he.h.m5_en_US_dedes.SeTinYperNoun.name
peMA.Ode9C11.incipit = "Watching You the world's Redeemer…"
peMA.Ode9C11.text = "Watching You the world's Redeemer, O Christ our God, * be divinely exalted, the Apostles with reverent awe * did exultantly leap, and You they magnified."

peMA.Ode9C12.ode = misc_en_US_dedes.Ode9
peMA.Ode9C12.mode = misc_en_US_dedes.Mode5
peMA.Ode9C12.melody = he.h.m5_en_US_dedes.SeTinYperNoun.name
peMA.Ode9C12.incipit = "As the Angels saw Your…"
peMA.Ode9C12.text = "As the Angels saw Your deified flesh in the heights, * they signaled to one another: * This is truly our God."

peMA.Ode9C13.ode = misc_en_US_dedes.Ode9
peMA.Ode9C13.mode = misc_en_US_dedes.Mode5
peMA.Ode9C13.melody = he.h.m5_en_US_dedes.SeTinYperNoun.name
peMA.Ode9C13.incipit = "The ranks of incorporeals, O…"
peMA.Ode9C13.text = "The ranks of incorporeals, O Christ our God, * beholding You being lifted on the clouds, cried aloud: * Take away the gates for the King of glory."

peMA.Ode9C14.ode = misc_en_US_dedes.Ode9
peMA.Ode9C14.mode = misc_en_US_dedes.Mode5
peMA.Ode9C14.melody = he.h.m5_en_US_dedes.SeTinYperNoun.name
peMA.Ode9C14.incipit = "You, O Lord, descended to…"
peMA.Ode9C14.text = "You, O Lord, descended to the lowest part of the earth * and You saved humanity and raised it up on high * by Your ascension. You do we magnify."

peMA.Ode9C1T.ode = misc_en_US_dedes.Ode9
peMA.Ode9C1T.mode = misc_en_US_dedes.Mode5
peMA.Ode9C1T.melody = he.h.m5_en_US_dedes.SeTinYperNoun.name
peMA.Ode9C1T.incipit = "Rejoice, O Theotokos, Mother of…"
peMA.Ode9C1T.text = "Rejoice, O Theotokos, Mother of Christ our God. * As today you watched the One whom you had borne flying up * from the earth, with the Angels Him you magnified."

peMA.Ode9C2M01.incipit = "The Angels beholding the Master's ascension…"
peMA.Ode9C2M01.text = "The Angels beholding the Master's ascension * were filled with great astonishment * at how with glory He was lifted * from the earth into heaven."

peMA.Ode9C2H.ode = misc_en_US_dedes.Ode9
peMA.Ode9C2H.mode = misc_en_US_dedes.Mode4
peMA.Ode9C2H.melody = he.h.m4_en_US_dedes.ApasGigenis.melody
peMA.Ode9C2H.incipit = he.h.m4_en_US_dedes.ApasGigenis.incipit
peMA.Ode9C2H.text = he.h.m4_en_US_dedes.ApasGigenis.text

peMA.Ode9C21.ode = misc_en_US_dedes.Ode9
peMA.Ode9C21.mode = misc_en_US_dedes.Mode4
peMA.Ode9C21.melody = he.h.m4_en_US_dedes.ApasGigenis.name
peMA.Ode9C21.incipit = "Oh, the gifts of God…"
peMA.Ode9C21.text = "Oh, the gifts of God * beyond comprehension! Oh, the awesome mystery! * For the sovereign Lord of all * rising from earth to the heavenly abodes * sent down to His Disciples the all-holy Spirit of God, * who illumined their mind and their intellect, and effectively set them afire with grace."

peMA.Ode9C22.ode = misc_en_US_dedes.Ode9
peMA.Ode9C22.mode = misc_en_US_dedes.Mode4
peMA.Ode9C22.melody = he.h.m4_en_US_dedes.ApasGigenis.name
peMA.Ode9C22.incipit = "To the sacred band *…"
peMA.Ode9C22.text = "To the sacred band * of holy Disciples did the risen Lord command: * Tarry in Jerusalem * and I shall send you another Comforter, * who shares the Father's throne and is of equal honor with me * whom you see now into heaven taken up, * being carried aloft on a cloud of light."

peMA.Ode9C23.ode = misc_en_US_dedes.Ode9
peMA.Ode9C23.mode = misc_en_US_dedes.Mode4
peMA.Ode9C23.melody = he.h.m4_en_US_dedes.ApasGigenis.name
peMA.Ode9C23.incipit = "The magnificence * of the…"
peMA.Ode9C23.text = "The magnificence * of the One who in flesh became poor clearly was * higher than the heavens raised. * Our fallen nature has now been honored by * its session with the Father. Therefore let us celebrate * and with one voice jubilantly cry aloud, * and with gladness of heart let us clap our hands."

peMA.Ode9C2T.ode = misc_en_US_dedes.Ode9
peMA.Ode9C2T.mode = misc_en_US_dedes.Mode4
peMA.Ode9C2T.melody = he.h.m4_en_US_dedes.ApasGigenis.name
peMA.Ode9C2T.incipit = "O all-blameless one, * from…"
peMA.Ode9C2T.text = "O all-blameless one, * from you rose the Light which manifestly shone from Light * and which has entirely * dispelled the darkness and gloom of godlessness * and led unto the light those who had been asleep in the night. * We all, therefore, as we are obliged to do, * to eternity ever pronounce you blest."

