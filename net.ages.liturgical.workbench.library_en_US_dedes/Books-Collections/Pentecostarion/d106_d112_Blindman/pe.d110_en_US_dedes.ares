peMA.Ode9C1H.ode = misc_en_US_dedes.Ode9
peMA.Ode9C1H.mode = misc_en_US_dedes.Mode5
peMA.Ode9C1H.melody = he.h.m5_en_US_dedes.SeTinYperNoun.melody
peMA.Ode9C1H.incipit = he.h.m5_en_US_dedes.SeTinYperNoun.incipit
