peVE.SticGlory.text = pe.d120_en_US_dedes.peVE.Stichera01.text

peVE.Aposticha1.mode = miscellanea_en_US_dedes.misc.Mode2
peVE.Aposticha1.greekmelody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.greekname
peVE.Aposticha1.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
peVE.Aposticha1.incipit = "Logos and Son of God…"
peVE.Aposticha1.text = "Logos and Son of God, * indeed as You had promised, * You sent to Your Disciples * Your divine Holy Spirit, and thereby You illumined them."

peVE.Aposticha2.mode = miscellanea_en_US_dedes.misc.Mode2
peVE.Aposticha2.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
peVE.Aposticha2.incipit = "Suddenly from on high, *…"
peVE.Aposticha2.text = "Suddenly from on high, * the full and promised power * of the all-holy Spirit * made the Apostles all wise * and theologians God-inspired."

peVE.Aposticha3.mode = miscellanea_en_US_dedes.misc.Mode2
peVE.Aposticha3.melody = he.a.m2_en_US_dedes.heAU.OikosTouEfratha.name
peVE.Aposticha3.incipit = "When the foreigners heard *…"
peVE.Aposticha3.text = "When the foreigners heard * the Lord’s divine Apostles * speaking in their own language, * they were bewildered hearing them magnify the Trinity."

