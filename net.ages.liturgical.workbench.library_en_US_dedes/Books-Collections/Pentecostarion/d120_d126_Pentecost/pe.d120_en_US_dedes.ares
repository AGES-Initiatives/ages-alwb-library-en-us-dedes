peSY.commemoration.text = "On this day, the eighth Sunday from Pascha, we celebrate holy Pentecost."
peSY.commemoration.couplet1.title = titles_en_US_dedes.ti.Verses
peSY.commemoration.couplet1.verse1 = "In a violent wind Christ distributes"
peSY.commemoration.couplet1.verse2 = "to the Apostles the Spirit in tongues of fire."
peSY.commemoration.single1 = "On one great day, the Spirit is poured out on the fishermen."
peSY.commemoration.narration.p01 = "[paragraph 01]"
peSY.commemoration.narration.p02 = "[paragraph 02]"
peSY.commemoration.narration.p03 = "[paragraph 03]"
peSY.commemoration.narration.p04 = "[paragraph 04]"
peSY.commemoration.narration.p05 = "[paragraph 05]"
peSY.commemoration.narration.p06 = "[paragraph 06]"
peSY.commemoration.narration.p07 = "[paragraph 07]"
peSY.lastline = "By the intercessions of the holy Apostles, O Christ our God, have mercy on us. Amen."

