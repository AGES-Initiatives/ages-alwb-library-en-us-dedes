ocVE.Stichera6.melody = he.a.m7_en_US_dedes.OukEtiKolyometha.name
ocVE.Stichera6.incipit = "O Nicholas the wise Hierarch…"
ocVE.Stichera6.text = "O Nicholas the wise Hierarch, deliver * your supplicants from plagues and from tribulations, * earthquakes and necessity."

