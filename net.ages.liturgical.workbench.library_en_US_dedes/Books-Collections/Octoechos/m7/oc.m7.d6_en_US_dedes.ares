ocMA.OdeW6C11.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW6C11.ode = miscellanea_en_US_dedes.misc.Ode6
ocMA.OdeW6C11.melody = he.h.m7_en_US_dedes.heHE.OIonasEkKoilias.notmetered
ocMA.OdeW6C11.incipit = "Since human nature was sick…"
ocMA.OdeW6C11.text = "Since human nature was sick, You appeared on earth in Your tender-mercy, O Physician of the sick, and by Your Cross and Passion You cured it."

ocMA.OdeW6C12.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW6C12.ode = miscellanea_en_US_dedes.misc.Ode6
ocMA.OdeW6C12.melody = he.h.m7_en_US_dedes.heHE.OIonasEkKoilias.notmetered
ocMA.OdeW6C12.incipit = "Of old, Adam was condemned…"
ocMA.OdeW6C12.text = "Of old, Adam was condemned because of a tree. Now, by the tree of the Cross, he is justified, and he is given entry to Paradise and he enjoys its delight."

ocMA.OdeW6C13.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW6C13.ode = miscellanea_en_US_dedes.misc.Ode6
ocMA.OdeW6C13.melody = he.h.m7_en_US_dedes.heHE.OIonasEkKoilias.notmetered
ocMA.OdeW6C13.incipit = "O supremely good King, we…"
ocMA.OdeW6C13.text = "O supremely good King, we extol You who were lifted on the Cross; we glorify You who wore the crown of thorns and who crowned humanity with glory."

ocMA.OdeW6C14.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW6C14.ode = miscellanea_en_US_dedes.misc.Ode6
ocMA.OdeW6C14.melody = he.h.m7_en_US_dedes.heHE.OIonasEkKoilias.notmetered
ocMA.OdeW6C14.incipit = "The Athletes rejected all the…"
ocMA.OdeW6C14.text = "The Athletes rejected all the destructive harm of idolatry. They embraced the tortures and died, and now they reign with Christ."

ocMA.OdeW6C15.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW6C15.ode = miscellanea_en_US_dedes.misc.Ode6
