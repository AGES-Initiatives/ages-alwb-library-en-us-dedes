ocMA.OdeW3C11.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW3C11.ode = miscellanea_en_US_dedes.misc.Ode3
ocMA.OdeW3C11.melody = he.h.m7_en_US_dedes.heHE.EstereothiTiPistei.notmetered
ocMA.OdeW3C11.incipit = "How can You who in…"
ocMA.OdeW3C11.text = "How can You who in essence do no suffer undergo Passion? How can You who breath life into me now die on the Cross? Great are Your mercy and Your longsuffering!"

ocMA.OdeW3C12.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW3C12.ode = miscellanea_en_US_dedes.misc.Ode3
ocMA.OdeW3C12.melody = he.h.m7_en_US_dedes.heHE.EstereothiTiPistei.notmetered
ocMA.OdeW3C12.incipit = "O Logos, You were unjustly…"
ocMA.OdeW3C12.text = "O Logos, You were unjustly crucified between two robbers, and You justified the one who recognized You as the Creator of all creation, who was suffering willingly."

ocMA.OdeW3C13.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW3C13.ode = miscellanea_en_US_dedes.misc.Ode3
ocMA.OdeW3C13.melody = he.h.m7_en_US_dedes.heHE.EstereothiTiPistei.notmetered
ocMA.OdeW3C13.incipit = "O medaled Athletes, you endured…"
ocMA.OdeW3C13.text = "O medaled Athletes, you endured the flaying of your skin, amputation of your hands and feet and all your members; and for this you obtained glory, and you intercede for us."

ocMA.OdeW3C14.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW3C14.ode = miscellanea_en_US_dedes.misc.Ode3
ocMA.OdeW3C14.melody = he.h.m7_en_US_dedes.heHE.EstereothiTiPistei.notmetered
ocMA.OdeW3C14.incipit = "O wise Martyrs, you were…"
ocMA.OdeW3C14.text = "O wise Martyrs, you were divinely burnished by the extreme tortures, and now you have inherited the surpassing glory. You ever intercede for the salvation of our souls."

ocMA.OdeW3C1T.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW3C1T.ode = miscellanea_en_US_dedes.misc.Ode3
ocMA.OdeW3C1T.melody = he.h.m7_en_US_dedes.heHE.EstereothiTiPistei.notmetered
ocMA.OdeW3C1T.incipit = "O pure Virgin, you were…"
ocMA.OdeW3C1T.text = "O pure Virgin, you were pregnant with God the Logos. He became man and was nailed to the Cross. As His mother, intercede with Him to forgive us our debts."

