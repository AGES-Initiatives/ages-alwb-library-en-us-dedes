ocMA.OdeW1C11.ode = miscellanea_en_US_dedes.misc.Ode1
ocMA.OdeW1C11.melody = he.h.m7_en_US_dedes.heHE.ToEktinaxantiTheo.notmetered
ocMA.OdeW1C11.incipit = "When Christ was lifted on…"
ocMA.OdeW1C11.text = "When Christ was lifted on the Cross, He drew all mortals to himself, and cast down the enemy who had cursed everyone."

ocMA.OdeW1C12.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW1C12.ode = miscellanea_en_US_dedes.misc.Ode1
ocMA.OdeW1C12.melody = he.h.m7_en_US_dedes.heHE.ToEktinaxantiTheo.notmetered
ocMA.OdeW1C12.incipit = "O Master Jesus, from Your…"
ocMA.OdeW1C12.text = "O Master Jesus, from Your life-bearing side You poured out for me a stream of life, and becoming dead You put the enemy to death."

ocMA.OdeW1C13.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW1C13.ode = miscellanea_en_US_dedes.misc.Ode1
ocMA.OdeW1C13.melody = he.h.m7_en_US_dedes.heHE.ToEktinaxantiTheo.notmetered
ocMA.OdeW1C13.incipit = "O medaled Martyrs, offer fervent…"
ocMA.OdeW1C13.text = "O medaled Martyrs, offer fervent supplication to Christ on our behalf, that we all be delivered from the frightening judgment."

ocMA.OdeW1C14.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW1C14.ode = miscellanea_en_US_dedes.misc.Ode1
ocMA.OdeW1C14.melody = he.h.m7_en_US_dedes.heHE.ToEktinaxantiTheo.notmetered
ocMA.OdeW1C14.incipit = "O wise Martyrs, you were…"
ocMA.OdeW1C14.text = "O wise Martyrs, you were humiliated for the sake of Christ, and by divine grace you completely overthrew the pompous enemy."

ocMA.OdeW1C1T.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW1C1T.ode = miscellanea_en_US_dedes.misc.Ode1
ocMA.OdeW1C1T.melody = he.h.m7_en_US_dedes.heHE.ToEktinaxantiTheo.notmetered
ocMA.OdeW1C1T.incipit = "O Lady, when you saw…"
ocMA.OdeW1C1T.text = "O Lady, when you saw on the Cross your Son, who rose from you in His extreme compassion, you lamented and also glorified Him."

