ocMA.OdeW4C11.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW4C11.ode = miscellanea_en_US_dedes.misc.Ode4
ocMA.OdeW4C11.melody = he.h.m7_en_US_dedes.heHE.EisakikoaTinAkoin.notmetered
ocMA.OdeW4C11.incipit = "O Master who loves humanity…"
ocMA.OdeW4C11.text = "O benevolent Master, You were lifted on the Cross like a most ripe cluster of grapes, and You dripped with the sweet wine that gladdens hearts."

ocMA.OdeW4C12.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW4C12.ode = miscellanea_en_US_dedes.misc.Ode4
ocMA.OdeW4C12.melody = he.h.m7_en_US_dedes.heHE.EisakikoaTinAkoin.notmetered
ocMA.OdeW4C12.incipit = "O Master, when You accepted…"
ocMA.OdeW4C12.text = "O Master, when You accepted Your voluntary Passion in the flesh, You truly put an end to the very painful sufferings of humanity."

ocMA.OdeW4C13.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW4C13.ode = miscellanea_en_US_dedes.misc.Ode4
ocMA.OdeW4C13.melody = he.h.m7_en_US_dedes.heHE.EisakikoaTinAkoin.notmetered
ocMA.OdeW4C13.incipit = "The medaled Athletes competed with…"
ocMA.OdeW4C13.text = "The medaled Athletes competed with valor, and they wounded the waring opponent. And now they are physicians of our souls."

ocMA.OdeW4C14.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW4C14.ode = miscellanea_en_US_dedes.misc.Ode4
ocMA.OdeW4C14.melody = he.h.m7_en_US_dedes.heHE.EisakikoaTinAkoin.notmetered
ocMA.OdeW4C14.incipit = "The medaled Athletes lined up…"
ocMA.OdeW4C14.text = "The medaled Athletes lined up against the tyrants. They won, and they were awarded crowns of victory."

ocMA.OdeW4C1T.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.OdeW4C1T.ode = miscellanea_en_US_dedes.misc.Ode4
