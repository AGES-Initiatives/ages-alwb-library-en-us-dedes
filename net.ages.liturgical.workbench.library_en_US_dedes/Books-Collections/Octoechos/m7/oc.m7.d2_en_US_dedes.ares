ocMA.OdeW3C1H.mode = misc_en_US_dedes.Mode7
ocMA.OdeW3C1H.ode = misc_en_US_dedes.Ode3
ocMA.OdeW3C1H.melody = misc_en_US_dedes.HeirmosNotMetered
ocMA.OdeW3C1H.incipit = he.h.m7_en_US_dedes.OStereosasToLogo.incipit
ocMA.OdeW3C1H.text = he.h.m7_en_US_dedes.OStereosasToLogo.text

ocMA.OdeW3C11.mode = misc_en_US_dedes.Mode7
ocMA.OdeW3C11.ode = misc_en_US_dedes.Ode3
ocMA.OdeW3C11.melody = he.h.m7_en_US_dedes.OStereosasToLogo.notmetered
ocMA.OdeW3C11.incipit = "At night I am beset…"
ocMA.OdeW3C11.text = "At night I am beset by unseemly sins. O Giver of light, illumine me now with the light of repentance, that with faith I may glorify You who love humanity."

ocMA.OdeW3C12.mode = misc_en_US_dedes.Mode7
ocMA.OdeW3C12.ode = misc_en_US_dedes.Ode3
ocMA.OdeW3C12.melody = he.h.m7_en_US_dedes.OStereosasToLogo.notmetered
ocMA.OdeW3C12.incipit = "I have followed paths in…"
ocMA.OdeW3C12.text = "I have followed paths in this life, which have all brought me down to the pit of hedonism. O Jesus, show me again the divine paths of repentance."

ocMA.OdeW3C13.mode = misc_en_US_dedes.Mode7
ocMA.OdeW3C13.ode = misc_en_US_dedes.Ode3
ocMA.OdeW3C13.melody = he.h.m7_en_US_dedes.OStereosasToLogo.notmetered
ocMA.OdeW3C13.incipit = "O all-lauded Martyrs, you finished…"
ocMA.OdeW3C13.text = "O all-lauded Martyrs, you finished the fight with valor, and you were granted the heavenly awards. And now you intercede for us all."

ocMA.OdeW3C14.mode = misc_en_US_dedes.Mode7
ocMA.OdeW3C14.ode = misc_en_US_dedes.Ode3
ocMA.OdeW3C14.melody = he.h.m7_en_US_dedes.OStereosasToLogo.notmetered
ocMA.OdeW3C14.incipit = "O medaled Martyrs, you endured…"
ocMA.OdeW3C14.text = "O medaled Martyrs, you endured countless labors, and you received countless blessings. And now you are united with the countless bodiless hosts."

ocMA.OdeW3C1T.mode = misc_en_US_dedes.Mode7
ocMA.OdeW3C1T.ode = misc_en_US_dedes.Ode3
ocMA.OdeW3C1T.melody = he.h.m7_en_US_dedes.OStereosasToLogo.notmetered
ocMA.OdeW3C1T.incipit = "O Virgin, only you gave…"
ocMA.OdeW3C1T.text = "O Virgin, only you gave birth to the heavenly King. Take pity on me, and defeat the sin that reigns in me, and save me."

ocMA.OdeW3C21.mode = misc_en_US_dedes.Mode7
ocMA.OdeW3C21.ode = misc_en_US_dedes.Ode3
ocMA.OdeW3C21.melody = he.h.m7_en_US_dedes.EstereothiTiPistei.notmetered
ocMA.OdeW3C21.incipit = "O choirs of Angels, you…"
ocMA.OdeW3C21.text = "O choirs of Angels, you are the mediators of divine revelations; and being immaterial you all unceasingly cry out: Holy are You, O Lord, the only almighty One!"

ocMA.OdeW3C22.mode = misc_en_US_dedes.Mode7
ocMA.OdeW3C22.ode = misc_en_US_dedes.Ode3
ocMA.OdeW3C22.melody = he.h.m7_en_US_dedes.EstereothiTiPistei.notmetered
ocMA.OdeW3C22.incipit = "O Archangels, obeying the law…"
ocMA.OdeW3C22.text = "O Archangels, obeying the law of love, you pass down in order the divine manifestations, and you sing to Christ: Holy are You, O Lord, the only almighty One!"

ocMA.OdeW3C23.mode = misc_en_US_dedes.Mode7
ocMA.OdeW3C23.ode = misc_en_US_dedes.Ode3
ocMA.OdeW3C23.melody = he.h.m7_en_US_dedes.EstereothiTiPistei.notmetered
ocMA.OdeW3C23.incipit = "Those with correct belief aspire…"
ocMA.OdeW3C23.text = "Those with correct belief aspire to the secondary brightness of the primary radiance, and in unison they sing to Christ: Holy are You, O Lord the savior of our souls!"

ocMA.OdeW3C2T.mode = misc_en_US_dedes.Mode7
ocMA.OdeW3C2T.ode = misc_en_US_dedes.Ode3
ocMA.OdeW3C2T.melody = he.h.m7_en_US_dedes.EstereothiTiPistei.notmetered
ocMA.OdeW3C2T.incipit = "The Logos, who brought all…"
ocMA.OdeW3C2T.text = "The Logos, who brought all things from nonexistence into being by His will, is borrowing flesh from you, O Virgin, becoming man in His pity and love for humanity."

ocMA.OdeW4C1H.mode = misc_en_US_dedes.Mode7
ocMA.OdeW4C1H.ode = misc_en_US_dedes.Ode4
ocMA.OdeW4C1H.melody = misc_en_US_dedes.HeirmosNotMetered
ocMA.OdeW4C1H.incipit = he.h.m7_en_US_dedes.EkalypsenOuranous.incipit
ocMA.OdeW4C1H.text = he.h.m7_en_US_dedes.EkalypsenOuranous.text

ocMA.OdeW4C11.mode = misc_en_US_dedes.Mode7
ocMA.OdeW4C11.ode = misc_en_US_dedes.Ode4
ocMA.OdeW4C11.melody = he.h.m7_en_US_dedes.EkalypsenOuranous.notmetered
ocMA.OdeW4C11.incipit = "O compassionate Logos, You will…"
ocMA.OdeW4C11.text = "O compassionate Logos, You will that all should be saved. I have transgressed Your commandments. Please have no contempt for me, but rather save me."

ocMA.OdeW4C12.mode = misc_en_US_dedes.Mode7
ocMA.OdeW4C12.ode = misc_en_US_dedes.Ode4
ocMA.OdeW4C12.melody = he.h.m7_en_US_dedes.EkalypsenOuranous.notmetered
ocMA.OdeW4C12.incipit = "I have submitted to irrational…"
ocMA.OdeW4C12.text = "I have submitted to irrational passions, wretch that I am, and I have become like the animals. O compassionate Logos of God, take pity on me."

ocMA.OdeW4C13.mode = misc_en_US_dedes.Mode7
ocMA.OdeW4C13.ode = misc_en_US_dedes.Ode4
ocMA.OdeW4C13.melody = he.h.m7_en_US_dedes.EkalypsenOuranous.notmetered
ocMA.OdeW4C13.incipit = "O Martyrs, as you were…"
ocMA.OdeW4C13.text = "O Martyrs, as you were tortured and burnt in fire, you scorched deceptive error, for you were lit up with the zeal of the correct belief in God."

ocMA.OdeW4C14.mode = misc_en_US_dedes.Mode7
ocMA.OdeW4C14.ode = misc_en_US_dedes.Ode4
ocMA.OdeW4C14.melody = he.h.m7_en_US_dedes.EkalypsenOuranous.notmetered
ocMA.OdeW4C14.incipit = "O blessed Martyrs, you had…"
ocMA.OdeW4C14.text = "O blessed Martyrs, you had Christ the tree of life and Planter of all within you, and thus you were like Paradise."

ocMA.OdeW4C1T.mode = misc_en_US_dedes.Mode7
ocMA.OdeW4C1T.ode = misc_en_US_dedes.Ode4
ocMA.OdeW4C1T.melody = he.h.m7_en_US_dedes.EkalypsenOuranous.notmetered
ocMA.OdeW4C1T.incipit = "O pure Virgin full of…"
ocMA.OdeW4C1T.text = "O pure Virgin full of grace divine, give grace to my mind, and completely free it from the darkness of ignorance."

ocMA.OdeW4C21.mode = misc_en_US_dedes.Mode7
ocMA.OdeW4C21.ode = misc_en_US_dedes.Ode4
ocMA.OdeW4C21.melody = he.h.m7_en_US_dedes.AkikoaTinAkoinSou.notmetered
ocMA.OdeW4C21.incipit = "O choirs of Angels and…"
ocMA.OdeW4C21.text = "O choirs of Angels and select ministers, as you stand near Christ and pray to Him, heal the traumas of my soul."

ocMA.OdeW4C22.mode = misc_en_US_dedes.Mode7
ocMA.OdeW4C22.ode = misc_en_US_dedes.Ode4
ocMA.OdeW4C22.melody = he.h.m7_en_US_dedes.AkikoaTinAkoinSou.notmetered
ocMA.OdeW4C22.incipit = "O Lord, the armies of…"
ocMA.OdeW4C22.text = "O Lord, the armies of the bodiless reverently surround Your throne, and they ever cry aloud: \"Glory to Your power, O Lord!\""

ocMA.OdeW4C23.mode = misc_en_US_dedes.Mode7
ocMA.OdeW4C23.ode = misc_en_US_dedes.Ode4
ocMA.OdeW4C23.melody = he.h.m7_en_US_dedes.AkikoaTinAkoinSou.notmetered
ocMA.OdeW4C23.incipit = "The ranks of Angels were…"
ocMA.OdeW4C23.text = "The ranks of Angels were amazed, when they saw You, O Christ, on earth with a body, dwelling among mortal men."

ocMA.OdeW4C2T.mode = misc_en_US_dedes.Mode7
ocMA.OdeW4C2T.ode = misc_en_US_dedes.Ode4
ocMA.OdeW4C2T.melody = he.h.m7_en_US_dedes.AkikoaTinAkoinSou.notmetered
ocMA.OdeW4C2T.incipit = "O all-lauded and immaculate Mother…"
ocMA.OdeW4C2T.text = "O all-lauded and immaculate Mother of God, inexplicably you gave birth to God in the flesh. We implore you to intercede that He save us."

ocMA.OdeW5C1H.mode = misc_en_US_dedes.Mode7
ocMA.OdeW5C1H.ode = misc_en_US_dedes.Ode5
ocMA.OdeW5C1H.melody = misc_en_US_dedes.HeirmosNotMetered
ocMA.OdeW5C1H.incipit = he.h.m7_en_US_dedes.OiOrthrizontesLoge.incipit
ocMA.OdeW5C1H.text = he.h.m7_en_US_dedes.OiOrthrizontesLoge.text

ocMA.OdeW5C11.mode = misc_en_US_dedes.Mode7
ocMA.OdeW5C11.ode = misc_en_US_dedes.Ode5
ocMA.OdeW5C11.melody = he.h.m7_en_US_dedes.OiOrthrizontesLoge.notmetered
ocMA.OdeW5C11.incipit = "I, the prodigal, have wasted…"
ocMA.OdeW5C11.text = "I, the prodigal, have wasted my life in laziness, and I tremble thinking about the infallible tribunal, where I will be judged. Take pity on me, O Lord."

ocMA.OdeW5C12.mode = misc_en_US_dedes.Mode7
ocMA.OdeW5C12.ode = misc_en_US_dedes.Ode5
ocMA.OdeW5C12.melody = he.h.m7_en_US_dedes.OiOrthrizontesLoge.notmetered
ocMA.OdeW5C12.incipit = "O Logos, You gave sight…"
ocMA.OdeW5C12.text = "O Logos, You gave sight to the eyes of the blind. The eyes of my soul have become badly darkened. I pray You, open them to see the light of Your commandments."

ocMA.OdeW5C13.mode = misc_en_US_dedes.Mode7
ocMA.OdeW5C13.ode = misc_en_US_dedes.Ode5
ocMA.OdeW5C13.melody = he.h.m7_en_US_dedes.OiOrthrizontesLoge.notmetered
ocMA.OdeW5C13.incipit = "O brave Martyrs, we call…"
ocMA.OdeW5C13.text = "O brave Martyrs, we call you blessed, for you confessed Christ with a steady mind, and you endured all the pain of the tortures."

ocMA.OdeW5C14.mode = misc_en_US_dedes.Mode7
ocMA.OdeW5C14.ode = misc_en_US_dedes.Ode5
ocMA.OdeW5C14.melody = he.h.m7_en_US_dedes.OiOrthrizontesLoge.notmetered
ocMA.OdeW5C14.incipit = "O Martyrs, you sailed through…"
ocMA.OdeW5C14.text = "O Martyrs, you sailed through every storm of the tortures, and you reached the harbor of the Kingdom on high, where you are filled with true serenity."

ocMA.OdeW5C1T.mode = misc_en_US_dedes.Mode7
ocMA.OdeW5C1T.ode = misc_en_US_dedes.Ode5
ocMA.OdeW5C1T.melody = he.h.m7_en_US_dedes.OiOrthrizontesLoge.notmetered
ocMA.OdeW5C1T.incipit = "O all-holy Virgin, God did…"
ocMA.OdeW5C1T.text = "O all-holy Virgin, God did great things through you, beyond understanding, and He was incarnate of you. Therefore earnestly intercede with Him on behalf of all."

ocMA.OdeW5C21.mode = misc_en_US_dedes.Mode7
ocMA.OdeW5C21.ode = misc_en_US_dedes.Ode5
ocMA.OdeW5C21.melody = he.h.m7_en_US_dedes.ProsSeOrthrizo.notmetered
ocMA.OdeW5C21.incipit = "The Seraphim, aflame with the…"
ocMA.OdeW5C21.text = "The Seraphim, aflame with the emanations of immaterial light, open their most holy and eloquent mouths to sing in praise of the super-principal and super-divine First Principle."

ocMA.OdeW5C22.mode = misc_en_US_dedes.Mode7
ocMA.OdeW5C22.ode = misc_en_US_dedes.Ode5
ocMA.OdeW5C22.melody = he.h.m7_en_US_dedes.ProsSeOrthrizo.notmetered
ocMA.OdeW5C22.incipit = "The Cherubim, those hosts of…"
ocMA.OdeW5C22.text = "The Cherubim, those hosts of super-divine wisdom, dare not gaze at the divine Radiance, but they cover their faces and feet with their most sacred wings."

ocMA.OdeW5C23.mode = misc_en_US_dedes.Mode7
ocMA.OdeW5C23.ode = misc_en_US_dedes.Ode5
ocMA.OdeW5C23.melody = he.h.m7_en_US_dedes.ProsSeOrthrizo.notmetered
ocMA.OdeW5C23.incipit = "O glorious Thrones, by divine…"
ocMA.OdeW5C23.text = "O glorious Thrones, by divine and all-blessed intellection, you delight in the most beautiful effulgences, and in a manner completely supermundane, you behold things ineffable."

ocMA.OdeW5C2T.mode = misc_en_US_dedes.Mode7
ocMA.OdeW5C2T.ode = misc_en_US_dedes.Ode5
ocMA.OdeW5C2T.melody = he.h.m7_en_US_dedes.ProsSeOrthrizo.notmetered
ocMA.OdeW5C2T.incipit = "O all-pure Virgin, the divine…"
ocMA.OdeW5C2T.text = "O all-pure Virgin, the divine Mind is joined to the human, in your all-immaculate and all-pure womb, and He unites with it hypostatically, without confusion or alteration."

ocMA.OdeW6C1H.mode = misc_en_US_dedes.Mode7
ocMA.OdeW6C1H.ode = misc_en_US_dedes.Ode6
ocMA.OdeW6C1H.melody = misc_en_US_dedes.HeirmosNotMetered
ocMA.OdeW6C1H.incipit = he.h.m7_en_US_dedes.OIonasEkKoilias.incipit
ocMA.OdeW6C1H.text = he.h.m7_en_US_dedes.OIonasEkKoilias.text

ocMA.OdeW6C11.mode = misc_en_US_dedes.Mode7
ocMA.OdeW6C11.ode = misc_en_US_dedes.Ode6
ocMA.OdeW6C11.melody = he.h.m7_en_US_dedes.OIonasEkKoilias.notmetered
ocMA.OdeW6C11.incipit = "The deep ocean of transgressions…"
ocMA.OdeW6C11.text = "The deep ocean of transgressions has encompassed me, it has plunged me into the deep of sin. O Logos, raise me up again, as once You raised Jonah, from corruption to life."

ocMA.OdeW6C12.mode = misc_en_US_dedes.Mode7
ocMA.OdeW6C12.ode = misc_en_US_dedes.Ode6
ocMA.OdeW6C12.melody = he.h.m7_en_US_dedes.OIonasEkKoilias.notmetered
ocMA.OdeW6C12.incipit = "I am distressed by a…"
ocMA.OdeW6C12.text = "I am distressed by a storm of thoughts. O compassionate Lord, pilot me to the harbor of genuine repentance, and keep my heart serene."

ocMA.OdeW6C13.mode = misc_en_US_dedes.Mode7
ocMA.OdeW6C13.ode = misc_en_US_dedes.Ode6
ocMA.OdeW6C13.melody = he.h.m7_en_US_dedes.OIonasEkKoilias.notmetered
ocMA.OdeW6C13.incipit = "The Saints were protected by…"
ocMA.OdeW6C13.text = "The Saints were protected by the shield of the truth, and they ignored the false flattery of the tyrants. The died, and now they reign with Christ."

ocMA.OdeW6C14.mode = misc_en_US_dedes.Mode7
ocMA.OdeW6C14.ode = misc_en_US_dedes.Ode6
ocMA.OdeW6C14.melody = he.h.m7_en_US_dedes.OIonasEkKoilias.notmetered
ocMA.OdeW6C14.incipit = "The Martyrs were lifted up…"
ocMA.OdeW6C14.text = "The Martyrs were lifted up to God in love, and they despised the friendship with the world. And thus they became friends of the Maker of all."

ocMA.OdeW6C1T.mode = misc_en_US_dedes.Mode7
ocMA.OdeW6C1T.ode = misc_en_US_dedes.Ode6
ocMA.OdeW6C1T.melody = he.h.m7_en_US_dedes.OIonasEkKoilias.notmetered
ocMA.OdeW6C1T.incipit = "We extol you, O Maiden…"
ocMA.OdeW6C1T.text = "We extol you, O Maiden greatly extolled. You held in your womb the greatly extolled Logos, whom all the hosts of heaven extol with voices never silent."

ocMA.OdeW6C21.mode = misc_en_US_dedes.Mode7
ocMA.OdeW6C21.ode = misc_en_US_dedes.Ode6
ocMA.OdeW6C21.melody = he.h.m7_en_US_dedes.OIonasEkKoilias.notmetered
ocMA.OdeW6C21.incipit = "The Dominions, flashing vigorously with…"
ocMA.OdeW6C21.text = "The Dominions, flashing vigorously with the effulgences of the divine Dominion, ever extol Its ineffable glory."

ocMA.OdeW6C22.mode = misc_en_US_dedes.Mode7
ocMA.OdeW6C22.ode = misc_en_US_dedes.Ode6
ocMA.OdeW6C22.melody = he.h.m7_en_US_dedes.OIonasEkKoilias.notmetered
ocMA.OdeW6C22.incipit = "The divine Powers gaze with…"
ocMA.OdeW6C22.text = "The divine Powers gaze with intense love on the power of the omnipotent Lordship, and they are clearly empowered thereby and remain in force."

ocMA.OdeW6C2T.mode = misc_en_US_dedes.Mode7
ocMA.OdeW6C2T.ode = misc_en_US_dedes.Ode6
ocMA.OdeW6C2T.melody = he.h.m7_en_US_dedes.OIonasEkKoilias.notmetered
ocMA.OdeW6C2T.incipit = "O Virgin, you descended from…"
ocMA.OdeW6C2T.text = "O Virgin, you descended from the royal tribe, and you have given birth to the King of all, in a manner passing nature and reason, and truly you remained a virgin."

ocMA.OdeW7C1H.mode = misc_en_US_dedes.Mode7
ocMA.OdeW7C1H.ode = misc_en_US_dedes.Ode7
ocMA.OdeW7C1H.melody = misc_en_US_dedes.HeirmosNotMetered
ocMA.OdeW7C1H.incipit = he.h.m7_en_US_dedes.KaminonKaiomenin.incipit
ocMA.OdeW7C1H.text = he.h.m7_en_US_dedes.KaminonKaiomenin.text

ocMA.OdeW7C11.mode = misc_en_US_dedes.Mode7
ocMA.OdeW7C11.ode = misc_en_US_dedes.Ode7
ocMA.OdeW7C11.melody = he.h.m7_en_US_dedes.KaminonKaiomenin.notmetered
ocMA.OdeW7C11.incipit = "O my soul, sacrifice to…"
ocMA.OdeW7C11.text = "O my soul, sacrifice to God a sacrifice of praise. Hurry and repent, while the market of this life is still open. Now is the time to trade by giving away your goods."

ocMA.OdeW7C12.mode = misc_en_US_dedes.Mode7
ocMA.OdeW7C12.ode = misc_en_US_dedes.Ode7
ocMA.OdeW7C12.melody = he.h.m7_en_US_dedes.KaminonKaiomenin.notmetered
ocMA.OdeW7C12.incipit = "O my soul, the cut…"
ocMA.OdeW7C12.text = "O my soul, the cut off time of death is approaching. Produce worthy fruits, so that you are not rejected like a fruitless tree, and thrown into hell fire, where you will lament to no avail."

ocMA.OdeW7C13.mode = misc_en_US_dedes.Mode7
ocMA.OdeW7C13.ode = misc_en_US_dedes.Ode7
ocMA.OdeW7C13.melody = he.h.m7_en_US_dedes.KaminonKaiomenin.notmetered
ocMA.OdeW7C13.incipit = "The holy Martyrs extinguished the…"
ocMA.OdeW7C13.text = "The holy Martyrs extinguished the furnace of deceptive error with the shower of their blood. And they shouted like those Servants: \"Blessed are You unto the ages, O Lord, the God of our fathers.\""

ocMA.OdeW7C14.mode = misc_en_US_dedes.Mode7
ocMA.OdeW7C14.ode = misc_en_US_dedes.Ode7
ocMA.OdeW7C14.melody = he.h.m7_en_US_dedes.KaminonKaiomenin.notmetered
ocMA.OdeW7C14.incipit = "O Martyrs, you became sons…"
ocMA.OdeW7C14.text = "O Martyrs, you became sons of the light, being mingled with supreme light. And you lead to the light all who are in the darkness, and you dispel the mist of deception."

ocMA.OdeW7C1T.mode = misc_en_US_dedes.Mode7
ocMA.OdeW7C1T.ode = misc_en_US_dedes.Ode7
ocMA.OdeW7C1T.melody = he.h.m7_en_US_dedes.KaminonKaiomenin.notmetered
ocMA.OdeW7C1T.incipit = "O Pure Virgin, the bush…"
ocMA.OdeW7C1T.text = "O Pure Virgin, the bush that was not consumed prefigured you, for you gave birth to the unbearable fire. Therefore I cry out to you: Burn away my material passions."

ocMA.OdeW7C21.mode = misc_en_US_dedes.Mode7
ocMA.OdeW7C21.ode = misc_en_US_dedes.Ode7
ocMA.OdeW7C21.melody = he.h.m7_en_US_dedes.KaminonKaiomenin.notmetered
ocMA.OdeW7C21.incipit = "The glorious and most brilliant…"
ocMA.OdeW7C21.text = "The glorious and most brilliant Authorities, surrounding Your throne, unwaveringly sing Your glory, O almighty Lord; and they shout: \"Blessed are You, the God of the fathers!\""

ocMA.OdeW7C22.mode = misc_en_US_dedes.Mode7
ocMA.OdeW7C22.ode = misc_en_US_dedes.Ode7
ocMA.OdeW7C22.melody = he.h.m7_en_US_dedes.KaminonKaiomenin.notmetered
ocMA.OdeW7C22.incipit = "Principalities with superlative eagerness revolve…"
ocMA.OdeW7C22.text = "Principalities with superlative eagerness revolve around You, the one Principle; and they sing their unceasing doxologies, saying: \"Blessed are You, the God of the fathers!\""

ocMA.OdeW7C2T.mode = misc_en_US_dedes.Mode7
ocMA.OdeW7C2T.ode = misc_en_US_dedes.Ode7
ocMA.OdeW7C2T.melody = he.h.m7_en_US_dedes.KaminonKaiomenin.notmetered
ocMA.OdeW7C2T.incipit = "O Mother of God, without…"
ocMA.OdeW7C2T.text = "O Mother of God, without seed you have given birth to Christ, the only lover of humanity, who as one hypostasis in two essences, executed the plan for our salvation, as the God of our fathers."

