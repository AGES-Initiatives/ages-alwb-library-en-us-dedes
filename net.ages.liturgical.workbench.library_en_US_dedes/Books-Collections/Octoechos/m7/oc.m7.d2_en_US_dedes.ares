octrMA.Triadic1.mode = misc_en_US_dedes.Mode7
octrMA.Triadic1.incipit = "O Lord, in Your highest…"
octrMA.Triadic1.text = "O Lord, in Your highest power You are extolled by the cherubim, and in Your divine glory You are worshiped by the angels. Accept us sinners also, who dare unworthily to cry to You: Holy, holy, holy are You, our God!"

octrMA.Triadic2.mode = misc_en_US_dedes.Mode7
octrMA.Triadic2.incipit = "O my soul, cast off…"
octrMA.Triadic2.text = "O my soul, cast off laziness as if it were sleep, and show the Judge that you have woken and are mending your ways, and with fear cry aloud: Holy, holy, holy are You, our God!"

