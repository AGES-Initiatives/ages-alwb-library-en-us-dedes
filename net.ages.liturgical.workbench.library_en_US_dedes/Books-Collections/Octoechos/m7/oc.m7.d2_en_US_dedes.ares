ocVE.Stichera6.melody = he.a.m7_en_US_dedes.XylonZois.name
ocVE.Stichera6.incipit = "…"
ocVE.Stichera6.text = ""

ocVE.SticTheotokion.mode = misc_en_US_dedes.Mode7
ocVE.SticTheotokion.melody = he.a.m7_en_US_dedes.XylonZois.name
ocVE.SticTheotokion.incipit = "…"
ocVE.SticTheotokion.text = ""

ocVE.Aposticha1.mode = misc_en_US_dedes.Mode7
ocVE.Aposticha1.melody = ""
ocVE.Aposticha1.incipit = "Like the prodigal son, I…"
ocVE.Aposticha1.text = "Like the prodigal son, I have returned, loving Father. As one of Your hired hands accept me who fall down to You, and have mercy on me."

ocVE.Aposticha2.mode = misc_en_US_dedes.Mode7
ocVE.Aposticha2.melody = ""
ocVE.Aposticha2.incipit = "Like the man who fell…"
ocVE.Aposticha2.text = "Like the man who fell among the robbers and was wounded, because of my own sins, I have also fallen, and my soul has likewise been wounded. In whom shall I, who am at fault, take refuge, if not in You the compassionate Physician of our souls? O God, pour on me the oil of Your great mercy."

ocVE.Aposticha3.mode = misc_en_US_dedes.Mode7
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.incipit = "Glory be to You, Christ…"
ocVE.Aposticha3.text = "Glory be to You, Christ our God, the Apostles' pride and joy, exultation of the Martyr Saints, whose proclamation was the consubstantial Trinity."

ocVE.AposTheotokion.mode = misc_en_US_dedes.Mode7
ocVE.AposTheotokion.melody = he.a.m7_en_US_dedes.SimeronGrigoreiOIoudas.name
ocVE.AposTheotokion.incipit = "You have surpassed the orders…"
ocVE.AposTheotokion.text = "You have surpassed the orders of Angels, * in that you incarnated God. * But I have exceeded all others in sinning. * Woe to me, for I am fearful and tremble. * Therefore, I in no way even dare now to petition you. * Hence, O Sovereign Lady who gave birth to God, despise me not, * but hasten and save me, while * directing me on the straight and narrow way."

ocVE.ApolTheotokionVM.mode = misc_en_US_dedes.Mode7
ocVE.ApolTheotokionVM.melody = ""
ocVE.ApolTheotokionVM.incipit = oc.m7.d2_en_US_dedes.ocMA.Kathisma13.incipit
ocVE.ApolTheotokionVM.text = oc.m7.d2_en_US_dedes.ocMA.Kathisma13.text

