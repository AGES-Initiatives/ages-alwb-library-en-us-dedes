ocVE.Stichera5.mode = miscellanea_en_US_dedes.misc.Mode7
ocVE.Stichera5.melody = he.a.m7_en_US_dedes.heAU.KatafronisantesPanton.name
ocVE.Stichera5.incipit = "…"
