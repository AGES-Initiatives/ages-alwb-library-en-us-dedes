ocVE.Aposticha3.mode = miscellanea_en_US_dedes.misc.Mode7
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.incipit = "O holy Martyr Saints, who…"
ocVE.Aposticha3.text = "O holy Martyr Saints, who competed well and received the crowns of laurel, intercede with the Lord for us, that His mercy come upon our souls."

ocVE.AposTheotokion.mode = miscellanea_en_US_dedes.misc.Mode7
ocVE.AposTheotokion.greekmelody = he.a.m7_en_US_dedes.heAU.OukEtiKolyometha.greekname
ocVE.AposTheotokion.melody = he.a.m7_en_US_dedes.heAU.OukEtiKolyometha.name
ocVE.AposTheotokion.incipit = "Bring peace into our lives…"
ocVE.AposTheotokion.text = "Bring peace into our lives, O merciful Master, * through the intercessions of the Theotokos. * Glory to You, O Lord."

