ocMA.Kathisma13.mode = miscellanea_en_US_dedes.misc.Mode7
ocMA.Kathisma13.melody = ""
ocMA.Kathisma13.incipit = "O unwedded Theotokos, you are…"
ocMA.Kathisma13.text = "O unwedded Theotokos, you are the ardent protection and assistance of us Christians. As such, join the Forerunner and implore Your Son, that we might find mercy."

