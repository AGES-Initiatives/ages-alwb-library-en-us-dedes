octrMA.Ode7Martyricon.mode = misc_en_US_dedes.Mode7
octrMA.Ode7Martyricon.melody = ""
octrMA.Ode7Martyricon.text = "O holy Martyrs, you disdained the godlessness of the tyrants, you overlooked the pain of torture, and you did not renounce the faith in Christ. Therefore entreat our God who loves humanity to save our souls."

