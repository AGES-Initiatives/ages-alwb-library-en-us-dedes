ocVE.Aposticha2.incipit = oc.m8.d2_en_US_dedes.ocVE.Aposticha2.incipit
ocVE.Aposticha2.text = oc.m8.d2_en_US_dedes.ocVE.Aposticha2.text

ocVE.Aposticha3.mode = miscellanea_en_US_dedes.misc.Mode8
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.incipit = "Martyrs of the Lord, we…"
ocVE.Aposticha3.text = "Martyrs of the Lord, we pray you earnestly entreat our God, and ask for an abundance of tender mercy for our souls, and the forgiveness of our many sins."

ocVE.AposTheotokion.mode = miscellanea_en_US_dedes.misc.Mode8
ocVE.AposTheotokion.melody = ""
ocVE.AposTheotokion.incipit = "Your shelter, O Virgin Theotokos…"
ocVE.AposTheotokion.text = "Your shelter, O Virgin Theotokos, is for us a spiritual clinic; for therein taking refuge, we are freed from disorders that affect the soul."

