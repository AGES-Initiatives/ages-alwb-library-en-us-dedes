ocMA.Aposticha2.mode = miscellanea_en_US_dedes.misc.Mode8
ocMA.Aposticha2.melody = ""
ocMA.Aposticha2.incipit = oc.m8.d2_en_US_dedes.ocMA.Aposticha2.incipit
ocMA.Aposticha2.text = oc.m8.d2_en_US_dedes.ocMA.Aposticha2.text

ocMA.Aposticha3.mode = miscellanea_en_US_dedes.misc.Mode8
ocMA.Aposticha3.greekmelody = he.a.m8_en_US_dedes.heAU.TiYmasKalesomen.greekname
ocMA.Aposticha3.melody = he.a.m8_en_US_dedes.heAU.TiYmasKalesomen.notmetered
ocMA.Aposticha3.incipit = "Holy Martyrs, you contested greatly:…"
ocMA.Aposticha3.text = "Holy Martyrs, you contested greatly: you bravely endured the tortures of the lawless, and you confessed Christ before kings. And now, since you have passed from this life, you work miracles in the world, and you heal the infirm of their ailments. We entreat you to intercede for the salvation of our souls."

ocMA.AposTheotokion.mode = miscellanea_en_US_dedes.misc.Mode8
ocMA.AposTheotokion.melody = ""
ocMA.AposTheotokion.incipit = "O Virgin and holy Theotokos…"
ocMA.AposTheotokion.text = "O Virgin and holy Theotokos, I take refuge in your protection. I am confident of my salvation, for you, O Pure One, are able to help me."

ocMA.ApolTheotokion.mode = miscellanea_en_US_dedes.misc.Mode8
ocMA.ApolTheotokion.melody = ""
ocMA.ApolTheotokion.incipit = "By your prayers save us…"
ocMA.ApolTheotokion.text = "By your prayers save us, O Virgin immaculate, stirring your maternal affections for your Son and our God."

ocLI.Beatitudes1.mode = miscellanea_en_US_dedes.misc.Mode8
ocLI.Beatitudes1.melody = ""
ocLI.Beatitudes1.incipit = oc.m8.d1_en_US_dedes.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m8.d1_en_US_dedes.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = miscellanea_en_US_dedes.misc.Mode8
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.incipit = "Christ our God, You are…"
ocLI.Beatitudes2.text = "Christ our God, You are an ocean of compassion. * I pray You to dry up the seas of my offenses;* wash them away with the teardrops of my compunction, and wholly purify me, merciful Lord."

ocLI.Beatitudes3.mode = miscellanea_en_US_dedes.misc.Mode8
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.incipit = "Holy Baptist John, you baptized…"
ocLI.Beatitudes3.text = "Holy Baptist John, you baptized in the waters * Christ of old. Now I am drowning in the sea storm * of sinful passions. So guide me into the harbor * of true repentance, by your prayers for me."

ocLI.Beatitudes4.mode = miscellanea_en_US_dedes.misc.Mode8
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.incipit = "With your holy blood, O…"
ocLI.Beatitudes4.text = "With your holy blood, O Martyrs, you extinguished * the unholy fire of the worshiping of idols; * and now each day you pour out streams of wondrous healings, * curing all kinds of ills and suffering."

ocLI.BeatitudesTr.mode = miscellanea_en_US_dedes.misc.Mode8
ocLI.BeatitudesTr.melody = ""
ocLI.BeatitudesTr.incipit = "Unoriginate Father and Son and…"
ocLI.BeatitudesTr.text = "Unoriginate Father and Son and Spirit, * I entreat you now to end the chronic passions * of my soul, and to save me Your humble servant, * by the entreaties of the Baptist John."

ocLI.BeatitudesT.mode = miscellanea_en_US_dedes.misc.Mode8
ocLI.BeatitudesT.melody = ""
ocLI.BeatitudesT.incipit = "Deliver us from all the…"
ocLI.BeatitudesT.text = "Deliver us from all the shameful sinful passions, * also from the torments of punishment in Hades, * by your entreaties, immaculate Theotokos, * for us who honor you and call you blest."

