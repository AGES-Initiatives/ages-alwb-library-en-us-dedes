ocMA.Kathisma32.mode = misc_en_US_dedes.Mode8
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.incipit = "This tabernacle is splendidly illumined…"
ocMA.Kathisma32.text = "This tabernacle is splendidly illumined today with heavenly light. For herein do the armies of Angels greatly rejoice, and with them the hosts of the Righteous are cheerful, as the victorious Martyrs are commemorated. O Christ, by their supplications, send down peace and great mercy to Your world."

