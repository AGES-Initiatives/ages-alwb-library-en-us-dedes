ocLI.Beatitudes1.incipit = oc.m2.d1_en_US_dedes.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m2.d1_en_US_dedes.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = miscellanea_en_US_dedes.misc.Mode2
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.incipit = "Holy Martyrs, since you have…"
ocLI.Beatitudes2.text = "Holy Martyrs, since you have imitated Christ’s sufferings, * you continually cure all kinds of human suffering."

ocLI.Beatitudes3.mode = miscellanea_en_US_dedes.misc.Mode2
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.incipit = "The Apostles, monastic Saints, holy…"
ocLI.Beatitudes3.text = "The Apostles, monastic Saints, holy Teachers and Prophets all * were well pleasing to the Lord and Creator of the universe."

ocLI.Beatitudes4.mode = miscellanea_en_US_dedes.misc.Mode2
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.incipit = "We entreat You, O God…"
ocLI.Beatitudes4.text = "We entreat You, O God on high, grant repose there with all Your saints * to Your faithful servants whom You took to yourself, O Lord."

ocLI.BeatitudesTr.mode = miscellanea_en_US_dedes.misc.Mode2
ocLI.BeatitudesTr.melody = ""
ocLI.BeatitudesTr.incipit = "Consubstantial Trinity, have compassion on…"
ocLI.BeatitudesTr.text = "Consubstantial Trinity, have compassion on those who sing * glorifying You, and ever save them from the snares of the foe."

ocLI.BeatitudesT.mode = miscellanea_en_US_dedes.misc.Mode2
ocLI.BeatitudesT.melody = ""
ocLI.BeatitudesT.incipit = "Overlook not the prayers of…"
ocLI.BeatitudesT.text = "Overlook not the prayers of us your servants, all-blameless Maid; * save us from afflictions that result from our many sins."

