ocMA.OdeS9C1H.mode = misc_en_US_dedes.Mode2
ocMA.OdeS9C1H.ode = misc_en_US_dedes.Ode9
ocMA.OdeS9C1H.melody = misc_en_US_dedes.Heirmos
ocMA.OdeS9C1H.incipit = he.h.m2_en_US_dedes.AnarchouGennitoros.incipit
ocMA.OdeS9C1H.text = he.h.m2_en_US_dedes.AnarchouGennitoros.text

ocMA.OdeS9C11.mode = misc_en_US_dedes.Mode2
ocMA.OdeS9C11.ode = misc_en_US_dedes.Ode9
ocMA.OdeS9C11.melody = he.h.m2_en_US_dedes.AnarchouGennitoros.name
ocMA.OdeS9C11.incipit = "The thrice-blessed tree…"
ocMA.OdeS9C11.text = "The thrice-blessed tree of Your immaculate Cross, O Lord, * was planted on Calvary as if in Paradise. * O Christ, it was watered by water and blood that poured * from Your side, O Savior, * as if from a fountain, and it blossomed forth with life for us."

ocMA.OdeS9C12.mode = misc_en_US_dedes.Mode2
ocMA.OdeS9C12.ode = misc_en_US_dedes.Ode9
ocMA.OdeS9C12.melody = he.h.m2_en_US_dedes.AnarchouGennitoros.name
ocMA.OdeS9C12.incipit = "You put down the mighty…"
ocMA.OdeS9C12.text = "You put down the mighty, being crucified, Almighty Lord, * and exalted humanity's nature that was held * below in the prison of Hades, and set it on the throne of Your Father. * With it You will come again; and we magnify and worship You."

ocMA.OdeS9C1Tr.mode = misc_en_US_dedes.Mode2
ocMA.OdeS9C1Tr.ode = misc_en_US_dedes.Ode9
ocMA.OdeS9C1Tr.melody = he.h.m2_en_US_dedes.AnarchouGennitoros.name
ocMA.OdeS9C1Tr.incipit = "As Orthodox faithful…"
ocMA.OdeS9C1Tr.text = "As Orthodox faithful, let us sing and glorify * the ternary Unity and consubstantial Trinity, * and praise the inseparate nature super-divine, the radiance unsetting, * triple-shining, and unique, uncompounded, shining light on us."

ocMA.OdeS9C21.mode = misc_en_US_dedes.Mode2
ocMA.OdeS9C21.ode = misc_en_US_dedes.Ode9
ocMA.OdeS9C21.melody = he.h.m2_en_US_dedes.AnarchouGennitoros.name
ocMA.OdeS9C21.incipit = "O Christ, amid the two…"
ocMA.OdeS9C21.text = "O Christ, amid the two convicts * as a lamb You were fastened * upon the Cross on Calvary, O Lord, * and You were pierced in the side with a lance; * and to us who were fashioned * of dust, You granted life, as we in faith * now extol Your divine Resurrection, O Benevolent."

ocMA.OdeS9C22.mode = misc_en_US_dedes.Mode2
ocMA.OdeS9C22.ode = misc_en_US_dedes.Ode9
ocMA.OdeS9C22.melody = he.h.m2_en_US_dedes.AnarchouGennitoros.name
ocMA.OdeS9C22.incipit = "Let us believers all worship…"
ocMA.OdeS9C22.text = "Let us believers all worship * God who virtually rendered * impotent the dominion of death * by means of His own voluntary death. * For the ones who for ages * were dead He resurrected with himself, * and He freely gives life and the Resurrection unto all."

ocMA.OdeS9C31.mode = misc_en_US_dedes.Mode2
ocMA.OdeS9C31.ode = misc_en_US_dedes.Ode9
ocMA.OdeS9C31.melody = he.h.m2_en_US_dedes.AnarchouGennitoros.name
ocMA.OdeS9C31.incipit = "Unto our rotten nature, O…"
ocMA.OdeS9C31.text = "Unto our rotten nature, O pure one, there has been given a rod of strength in your womb, * the very Logos of God, * who raised it when it fell to hell. * Therefore, all-pure one, you as Theotokos do we magnify."

ocMA.OdeS9C32.mode = misc_en_US_dedes.Mode2
ocMA.OdeS9C32.ode = misc_en_US_dedes.Ode9
ocMA.OdeS9C32.melody = he.h.m2_en_US_dedes.AnarchouGennitoros.name
ocMA.OdeS9C32.incipit = "With sympathy receive as ambassador…"
ocMA.OdeS9C32.text = "With sympathy receive as ambassador who acts on our behalf, Your Mother whom You chose, * O Master; and everything * will then be with Your goodness filled; * that You as Benefactor we might all together magnify."

