ocMA.ApolTheotokion.melody = ""
ocMA.ApolTheotokion.incipit = ho.s03_en_US_dedes.hoMA.SunTroparion1.incipit
ocMA.ApolTheotokion.text = ho.s03_en_US_dedes.hoMA.SunTroparion1.text

ocLI.Beatitudes1.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes1.melody = he.h.m2_en_US_dedes.SyToLogoSouKyrie.name
ocLI.Beatitudes1.incipit = "We present You the robber's words…"
ocLI.Beatitudes1.text = "We present You the robber's words and we prayerfully echo them; and like him we cry out, O Savior: In Your kingdom remember us."

ocLI.Beatitudes2.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes2.melody = he.h.m2_en_US_dedes.SyToLogoSouKyrie.name
ocLI.Beatitudes2.incipit = "We present You the Cross…"
ocLI.Beatitudes2.text = "We present You the Cross, O Lord, that You suffered on our behalf, to forgive our sins and offences, in Your love for humanity."

ocLI.Beatitudes3.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes3.melody = he.h.m2_en_US_dedes.SyToLogoSouKyrie.name
ocLI.Beatitudes3.incipit = "Lord, we honor Your burial…"
ocLI.Beatitudes3.text = "Lord, we honor Your burial and Your resurrection from the dead, by which You have rescued the world from corruption, O Friend of man."

ocLI.Beatitudes4.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes4.melody = he.h.m2_en_US_dedes.SyToLogoSouKyrie.name
ocLI.Beatitudes4.incipit = "By Your own death was…"
ocLI.Beatitudes4.text = "By Your own death was death itself swallowed up, O our Savior Christ; by Your resurrection You rescued the world and You saved mankind."

ocLI.Beatitudes5.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes5.melody = he.h.m2_en_US_dedes.SyToLogoSouKyrie.name
ocLI.Beatitudes5.incipit = "When the souls who were…"
ocLI.Beatitudes5.text = "When the souls who were lying in the darkness of the netherworld saw You, Christ, the true light, in Hades, they rose and were granted life."

ocLI.Beatitudes6.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes6.melody = he.h.m2_en_US_dedes.SyToLogoSouKyrie.name
ocLI.Beatitudes6.incipit = "When You rose from the…"
ocLI.Beatitudes6.text = "When You rose from the sepulcher, You commanded the Myrrh-bearing women to inform Your Disciples that You had risen from the dead."

ocLI.BeatitudesTr.mode = misc_en_US_dedes.Mode2
ocLI.BeatitudesTr.melody = he.h.m2_en_US_dedes.SyToLogoSouKyrie.name
ocLI.BeatitudesTr.incipit = "Come, believers, and glorify and…"
ocLI.BeatitudesTr.text = "Come, believers, and glorify and adore and with faith extol the Father and the Son and the Spirit, our one God in Trinity."

ocLI.BeatitudesT.mode = misc_en_US_dedes.Mode2
ocLI.BeatitudesT.melody = he.h.m2_en_US_dedes.SyToLogoSouKyrie.name
ocLI.BeatitudesT.incipit = "We salute you and say…"
ocLI.BeatitudesT.text = "We salute you and say, Rejoice, fiery throne and unwedded Bride. Rejoice, holy Virgin, for you gave birth to God for humanity."

