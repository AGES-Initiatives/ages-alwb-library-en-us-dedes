ocMA.OdeS1C1H.mode = misc_en_US_dedes.Mode2
ocMA.OdeS1C1H.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C1H.melody = misc_en_US_dedes.Heirmos
ocMA.OdeS1C1H.incipit = he.h.m2_en_US_dedes.EnVythoKatestrose.incipit
ocMA.OdeS1C1H.text = he.h.m2_en_US_dedes.EnVythoKatestrose.text

ocMA.OdeS1C11.mode = misc_en_US_dedes.Mode2
ocMA.OdeS1C11.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C11.melody = he.h.m2_en_US_dedes.EnVythoKatestrose.name
ocMA.OdeS1C11.incipit = "Good One, disobeying Your command…"
ocMA.OdeS1C11.text = "Good One, disobeying Your command, * we were sadly registered to the ruler of this world as his property. * By Your Cross is he condemned; * for assaulting You as if merely a mortal man, * he fell into Your authority, and he was rendered powerless."

ocMA.OdeS1C12.mode = misc_en_US_dedes.Mode2
ocMA.OdeS1C12.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C12.melody = he.h.m2_en_US_dedes.EnVythoKatestrose.name
ocMA.OdeS1C12.incipit = "Master, You have come…"
ocMA.OdeS1C12.text = "Master, You have come into the world * as Redeemer of the human race and as the Author of immortal life. * For You shredded shrouds of death * by Your resurrection, which we all now glorify. * For You are forever glorious, and You are greatly glorified."

ocMA.OdeS1C1T.mode = misc_en_US_dedes.Mode2
ocMA.OdeS1C1T.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C1T.melody = he.h.m2_en_US_dedes.EnVythoKatestrose.name
ocMA.OdeS1C1T.incipit = "Unto the Creator…"
ocMA.OdeS1C1T.text = "Unto the Creator you gave birth, * when in His good pleasure He became incarnate, O Maid, in your virgin womb. * Hence you are superior * to creation all, invisible and visible. * Intercede with Him with confidence, entreating Him to save our souls."

ocMA.OdeS1C21.mode = misc_en_US_dedes.Mode2
ocMA.OdeS1C21.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C21.melody = he.h.m2_en_US_dedes.AtriptonAsynithi.name
ocMA.OdeS1C21.incipit = "Power of the infirm ones…"
ocMA.OdeS1C21.text = "Power of the infirm ones, * the raising of the fallen, and incorruption * of those who died have You become, O Christ, * by Your flesh’s suffering; * for it is glorified."

ocMA.OdeS1C22.mode = misc_en_US_dedes.Mode2
ocMA.OdeS1C22.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C22.melody = he.h.m2_en_US_dedes.AtriptonAsynithi.name
ocMA.OdeS1C22.incipit = "God the Creator pitied *…"
ocMA.OdeS1C22.text = "God the Creator pitied * His fallen, shattered image; and as Renewer * He resurrected it through His own death. * This is what has come to pass. * For He is glorified."

ocMA.OdeS1C31.mode = misc_en_US_dedes.Mode2
ocMA.OdeS1C31.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C31.melody = he.h.m2_en_US_dedes.AtriptonAsynithi.name
ocMA.OdeS1C31.incipit = "Pure one, that dreamt-of ladder…"
ocMA.OdeS1C31.text = "Pure one, that dreamt-of ladder * and the seaway that strangely turned into dry land * were the depictions of your childbirth; * which we all extol with hymns, * for d01it is glorified."

ocMA.OdeS1C32.mode = misc_en_US_dedes.Mode2
ocMA.OdeS1C32.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C32.melody = he.h.m2_en_US_dedes.AtriptonAsynithi.name
ocMA.OdeS1C32.incipit = "Wisdom of God and perfect…"
ocMA.OdeS1C32.text = "Wisdom of God and perfect * hypostasis and power of the Most High took * from you a body, Lady undefiled, * and conversed with mortal men; * for He is glorified."

ocMA.OdeS1C33.mode = misc_en_US_dedes.Mode2
ocMA.OdeS1C33.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C33.melody = he.h.m2_en_US_dedes.AtriptonAsynithi.name
ocMA.OdeS1C33.incipit = "Through the untrodden gateway, *…"
ocMA.OdeS1C33.text = "Through the untrodden gateway, * O Pure one, of your womb that has been again shut, * the Sun of Righteousness did make His way, * and He shone upon the world; * for He is glorified."

ocMA.OdeS3C1H.mode = misc_en_US_dedes.Mode2
ocMA.OdeS3C1H.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C1H.melody = misc_en_US_dedes.Heirmos
ocMA.OdeS3C1H.incipit = he.h.m2_en_US_dedes.ExinthisenIErimos.incipit
ocMA.OdeS3C1H.text = he.h.m2_en_US_dedes.ExinthisenIErimos.text

ocMA.OdeS3C11.mode = misc_en_US_dedes.Mode2
ocMA.OdeS3C11.ode = misc_en_US_dedes.Ode4
ocMA.OdeS3C11.melody = he.h.m2_en_US_dedes.ExinthisenIErimos.name
ocMA.OdeS3C11.incipit = "Creation, at Your Passion…"
ocMA.OdeS3C11.text = "Creation, at Your Passion, Lord, was confounded when it saw * that You, who had established all * by divine bidding, were in outward form * humiliated and sneered at by lawless men."

ocMA.OdeS3C12.mode = misc_en_US_dedes.Mode2
ocMA.OdeS3C12.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C12.melody = he.h.m2_en_US_dedes.ExinthisenIErimos.name
ocMA.OdeS3C12.incipit = "You fashioned me from dust…"
ocMA.OdeS3C12.text = "You fashioned me from dust, O Christ, * in Your image, with Your hand. * When I through sin was crushed again * to the dust of death, You went down with me * into Hades and raised me up again with You."

ocMA.OdeS3C1T.mode = misc_en_US_dedes.Mode2
ocMA.OdeS3C1T.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C1T.melody = he.h.m2_en_US_dedes.ExinthisenIErimos.name
ocMA.OdeS3C1T.incipit = "O Virgin, the angelic hosts…"
ocMA.OdeS3C1T.text = "O Virgin, the angelic hosts * were amazed that you gave birth * to God, O all-immaculate; * and the hearts of the people shook in awe. * With faith we honor you as the Mother of God."

ocMA.OdeS3C21.mode = misc_en_US_dedes.Mode2
ocMA.OdeS3C21.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C21.melody = he.h.m2_en_US_dedes.ToxonSynetriviDynaston.name
ocMA.OdeS3C21.incipit = "Christ who is transcendent over…"
ocMA.OdeS3C21.text = "Christ who is transcendent over all * was made a little lower than the angelic rank, * when He suffered in the flesh."

ocMA.OdeS3C22.mode = misc_en_US_dedes.Mode2
ocMA.OdeS3C22.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C22.melody = he.h.m2_en_US_dedes.ToxonSynetriviDynaston.name
ocMA.OdeS3C22.incipit = "Numbered with the lawless as…"
ocMA.OdeS3C22.text = "Numbered with the lawless as one dead, * O Christ, then You shone forth * unto the women with Your Resurrection’s glowing light."

ocMA.OdeS3C31.mode = misc_en_US_dedes.Mode2
ocMA.OdeS3C31.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C31.melody = he.h.m2_en_US_dedes.ToxonSynetriviDynaston.name
ocMA.OdeS3C31.incipit = "He who is transcendent of…"
ocMA.OdeS3C31.text = "He who is transcendent of all time, * as the Maker of time, * was as a babe formed from you, O Virgin, voluntarily."

ocMA.OdeS3C32.mode = misc_en_US_dedes.Mode2
ocMA.OdeS3C32.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C32.melody = he.h.m2_en_US_dedes.ToxonSynetriviDynaston.name
ocMA.OdeS3C32.incipit = "The womb that is wider…"
ocMA.OdeS3C32.text = "The womb that is wider than the heavens, and by means of which * in heaven Adam lives with joy, * let us faithful now extol."

ocMA.OdeS4C1H.mode = misc_en_US_dedes.Mode2
ocMA.OdeS4C1H.ode = misc_en_US_dedes.Ode4
ocMA.OdeS4C1H.melody = misc_en_US_dedes.Heirmos
ocMA.OdeS4C1H.incipit = he.h.m2_en_US_dedes.ElilythasEkParthenou.incipit
ocMA.OdeS4C1H.text = he.h.m2_en_US_dedes.ElilythasEkParthenou.text

ocMA.OdeS4C11.mode = misc_en_US_dedes.Mode2
ocMA.OdeS4C11.ode = misc_en_US_dedes.Ode4
ocMA.OdeS4C11.melody = he.h.m2_en_US_dedes.ElilythasEkParthenou.name
ocMA.OdeS4C11.incipit = "You stood trial…"
ocMA.OdeS4C11.text = "You stood trial at Caiaphas's court, Lord, not crying out, * as You brought forth justice to the Gentiles, as Isaiah said. * This is the salvation that You worked for the whole world, * through Your Passion, O Christ."

ocMA.OdeS4C12.mode = misc_en_US_dedes.Mode2
ocMA.OdeS4C12.ode = misc_en_US_dedes.Ode4
ocMA.OdeS4C12.melody = he.h.m2_en_US_dedes.ElilythasEkParthenou.name
ocMA.OdeS4C12.incipit = "By Your Passion…"
ocMA.OdeS4C12.text = "By Your Passion, forsaken are the swords of the enemy, * as the Prophet David said. * By Your descent into Hades, Lord, * the cities of the foe were pulled down, * and the tyrant's insolence was overthrown."

ocMA.OdeS4C1T.mode = misc_en_US_dedes.Mode2
ocMA.OdeS4C1T.ode = misc_en_US_dedes.Ode4
ocMA.OdeS4C1T.melody = he.h.m2_en_US_dedes.ElilythasEkParthenou.name
ocMA.OdeS4C1T.incipit = "You, O Lady Theotokos…"
ocMA.OdeS4C1T.text = "You, O Lady Theotokos, are known by the faithful all, * our fortress unshakable, * the harbor where we go to be saved. * By your intercessions you rescue our souls from all dangers and calamities."

