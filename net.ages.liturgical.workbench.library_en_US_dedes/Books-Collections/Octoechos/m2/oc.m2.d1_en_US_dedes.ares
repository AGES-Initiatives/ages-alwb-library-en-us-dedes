ocMA.OdeS6C1H.incipit = he.h.m2_en_US_dedes.EnAvyssoPtaismaton.incipit
ocMA.OdeS6C1H.text = he.h.m2_en_US_dedes.EnAvyssoPtaismaton.text

ocMA.OdeS6C11.mode = misc_en_US_dedes.Mode2
ocMA.OdeS6C11.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C11.melody = he.h.m2_en_US_dedes.EnAvyssoPtaismaton.name
ocMA.OdeS6C11.incipit = "The Righteous One was judged…"
ocMA.OdeS6C11.text = "The Righteous One was judged as a criminal, * and He has been crucified with those who broke the law; * and by His precious blood He gives * guilty sinners forgiveness of all their sins."

ocMA.OdeS6C12.mode = misc_en_US_dedes.Mode2
ocMA.OdeS6C12.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C12.melody = he.h.m2_en_US_dedes.EnAvyssoPtaismaton.name
ocMA.OdeS6C12.incipit = "As through one man…"
ocMA.OdeS6C12.text = "As through one man, the first Adam, sin of old * came into the world and through it death spread to all, * much more through one, the Son of God, * resurrection has been manifested to all."

ocMA.OdeS6C1T.mode = misc_en_US_dedes.Mode2
ocMA.OdeS6C1T.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C1T.melody = he.h.m2_en_US_dedes.EnAvyssoPtaismaton.name
ocMA.OdeS6C1T.incipit = "You, O Virgin, conceived…"
ocMA.OdeS6C1T.text = "You, O Virgin, conceived without knowing man * and remained a virgin, even after giving birth, * exhibiting the tokens that prove * the true divinity of your Son and God."

