ocLI.Beatitudes1.incipit = oc.m2.d1_en_US_dedes.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m2.d1_en_US_dedes.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.incipit = "You were lifted upon the…"
ocLI.Beatitudes2.text = "You were lifted upon the Cross, and You lifted up with yourself * all of human nature, O Savior, which now extols You ceaselessly."

ocLI.Beatitudes3.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.incipit = "By the spear that was…"
ocLI.Beatitudes3.text = "By the spear that was thrust in You, You tore up Adam's bond of sin, * and You have enrolled Him in the book of the living, O Friend of man."

ocLI.Beatitudes4.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.incipit = "Imitating the One who was…"
ocLI.Beatitudes4.text = "Imitating the One who was voluntarily crucified, * now you are participants with Him in glory, O martyr saints."

ocLI.BeatitudesTr.mode = misc_en_US_dedes.Mode2
ocLI.BeatitudesTr.melody = ""
ocLI.BeatitudesTr.incipit = "In one honor and glory…"
ocLI.BeatitudesTr.text = "In one honor and glory is the unoriginate Father praised * with the Holy Spirit and the Son, also truly unoriginate."

ocLI.BeatitudesT.mode = misc_en_US_dedes.Mode2
ocLI.BeatitudesT.melody = ""
ocLI.BeatitudesT.incipit = "As you witnessed the One…"
ocLI.BeatitudesT.text = "As you witnessed the One you bore without seed being crucified, * you, His virgin Mother, extolled His mercy ineffable."

