//The order of the Triadics is according to the Triodion
octrMA.Triadic1.mode = misc_en_US_dedes.Mode2
octrMA.Triadic1.incipit = "We on earth…"
octrMA.Triadic1.text = "We on earth, imitating the powers on high, send up a triumphal hymn to You, O good One: Holy, holy, holy are You, our God."

octrMA.Triadic2.mode = misc_en_US_dedes.Mode2
octrMA.Triadic2.incipit = "O uncreated nature…"
octrMA.Triadic2.text = "O uncreated nature and Creator of the universe, open our lips, so that we may proclaim Your praise and cry out: Holy, holy, holy are You, our God."

octrMA.Triadic3.mode = misc_en_US_dedes.Mode2
octrMA.Triadic3.incipit = "From my bed and sleep…"
octrMA.Triadic3.text = "From my bed and sleep You have roused me, O Lord. Illumine my mind and heart, and open my lips to extol You, O Holy Trinity: Holy, holy, holy are You, our God."

