ocLI.Beatitudes2.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.incipit = "Lord, I pray You to…"
ocLI.Beatitudes2.text = "Lord, I pray You to overlook my offences and many sins; * make me a partaker of Your light, in Your love for man."

ocLI.Beatitudes3.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.incipit = "Hosts of Angels implore You…"
ocLI.Beatitudes3.text = "Hosts of Angels implore You, Lord, on behalf of believers all, * who with love and longing sing sacred hymns extolling You."

ocLI.Beatitudes4.mode = misc_en_US_dedes.Mode2
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.incipit = "Most benevolent Jesus Christ, by…"
ocLI.Beatitudes4.text = "Most benevolent Jesus Christ, by the prayers of Your martyr saints, * be forgiving to us in that hour on Judgment Day."

