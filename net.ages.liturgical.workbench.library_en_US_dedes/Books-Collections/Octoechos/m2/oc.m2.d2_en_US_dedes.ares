ocLI.BeatitudesT.mode = misc_en_US_dedes.Mode2
ocLI.BeatitudesT.melody = ""
ocLI.BeatitudesT.incipit = "O pure Virgin the holy…"
ocLI.BeatitudesT.text = "O pure Virgin the holy Fruit of your womb made Adam and Eve, * who had slipped because of fruit, citizens of Paradise."

