ocVE.AposTheotokion.mode = misc_en_US_dedes.Mode3
ocVE.AposTheotokion.melody = he.a.m3_en_US_dedes.MegaliTouStavrouSou.name
ocVE.AposTheotokion.incipit = "The purest Virgin cried aloud…"
ocVE.AposTheotokion.text = "The purest Virgin cried aloud to You, O Word of God: * The world is granted mercy, * creation is enlightened, * and the Gentiles find salvation through Your Cross, O Master. * But now I suffer, my Son, as I behold * Your voluntary suffering."

