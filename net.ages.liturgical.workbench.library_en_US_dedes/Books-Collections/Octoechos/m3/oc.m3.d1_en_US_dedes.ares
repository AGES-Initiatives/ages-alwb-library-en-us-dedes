ocMA.OdeS6C1H.text = he.h.m3_en_US_dedes.AvyssosEschati.text

ocMA.OdeS6C11.mode = misc_en_US_dedes.Mode3
ocMA.OdeS6C11.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C11.melody = he.h.m3_en_US_dedes.AvyssosEschati.name
ocMA.OdeS6C11.incipit = "Mercy and compassion…"
ocMA.OdeS6C11.text = "Mercy and compassion encircle me now like a sea, * for in Your love You came down to me. * You became incarnate, Master, and took on the form of a slave, * and You deified and, with yourself, You glorified me."

ocMA.OdeS6C12.mode = misc_en_US_dedes.Mode3
ocMA.OdeS6C12.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C12.melody = he.h.m3_en_US_dedes.AvyssosEschati.name
ocMA.OdeS6C12.incipit = "When he saw the One…"
ocMA.OdeS6C12.text = "When he saw the One who was put to death come back to life, * he who wielded death was put to death. * These are the tokens of Your resurrection, O Christ our God, * and the prizes of Your all-immaculate Passion."

ocMA.OdeS6C1T.mode = misc_en_US_dedes.Mode3
ocMA.OdeS6C1T.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C1T.melody = he.h.m3_en_US_dedes.AvyssosEschati.name
ocMA.OdeS6C1T.incipit = "You incomprehensibly intercede…"
ocMA.OdeS6C1T.text = "You incomprehensibly intercede with the Creator * on behalf of humanity. * Only all-pure Virgin, pray your Son, that He be merciful * and the champion of your servants who are sinners."

