ocMA.OdeS1C1H.mode = misc_en_US_dedes.Mode3
ocMA.OdeS1C1H.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C1H.melody = misc_en_US_dedes.Heirmos
ocMA.OdeS1C1H.incipit = he.h.m3_en_US_dedes.OTaYdata.incipit
ocMA.OdeS1C1H.text = he.h.m3_en_US_dedes.OTaYdata.text

ocMA.OdeS1C11.mode = misc_en_US_dedes.Mode3
ocMA.OdeS1C11.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C11.melody = he.h.m3_en_US_dedes.OTaYdata.name
ocMA.OdeS1C11.incipit = "He who once laid a curse…"
ocMA.OdeS1C11.text = "He who once laid a curse on the ground, to bring forth * for him who disobeyed thorns for his labors, * let a crown of thorns be laid on His head in the flesh, * by the hands of lawless men. He is our God, and He lifted * the ancestral curse, for He is greatly glorified."

ocMA.OdeS1C12.mode = misc_en_US_dedes.Mode3
ocMA.OdeS1C12.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C12.melody = he.h.m3_en_US_dedes.OTaYdata.name
ocMA.OdeS1C12.incipit = "By submitting to death…"
ocMA.OdeS1C12.text = "By submitting to death, He became the victor * triumphantly over death, taking trophies. * He assumed a passible human body and soul. * As our God and Lord, He wrestled with the tyrant and beat him. * With himself He raised all men, for He is glorified."

ocMA.OdeS1C1T.mode = misc_en_US_dedes.Mode3
ocMA.OdeS1C1T.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C1T.melody = he.h.m3_en_US_dedes.OTaYdata.name
ocMA.OdeS1C1T.incipit = "Without seed you gave birth…"
ocMA.OdeS1C1T.text = "Without seed you gave birth to God. Now all nations * extol you, the only true Theotokos. * For the One who entered into your sanctified womb * is our God and Lord, and He assumed our whole human nature; * and when He was born from you, He was both God and man."

ocMA.OdeS1C21.mode = misc_en_US_dedes.Mode3
ocMA.OdeS1C21.ode = misc_en_US_dedes.Ode1
ocMA.OdeS1C21.melody = he.h.m3_en_US_dedes.AsmaKainon.name
ocMA.OdeS1C21.incipit = "…"
