ocMA.OdeS3C11.mode = misc_en_US_dedes.Mode3
ocMA.OdeS3C11.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C11.melody = he.h.m3_en_US_dedes.OEkMiOnton.name
ocMA.OdeS3C11.incipit = "The ungodly one…"
ocMA.OdeS3C11.text = "The ungodly one was put to shame by Your Cross. * He opened up a pit, and he fell into the hole he made. * And exalted was the horn * of the humble, O Christ, by Your resurrection."

ocMA.OdeS3C12.mode = misc_en_US_dedes.Mode3
ocMA.OdeS3C12.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C12.melody = he.h.m3_en_US_dedes.OEkMiOnton.name
ocMA.OdeS3C12.incipit = "The proclamation of knowledge…"
ocMA.OdeS3C12.text = "The proclamation of knowledge and faith in God * has covered the whole wide world, * as much water covers the seas. * Rising from the tomb, O Lord * who love mankind, You revealed the Trinity's light."

ocMA.OdeS3C1T.mode = misc_en_US_dedes.Mode3
ocMA.OdeS3C1T.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C1T.melody = he.h.m3_en_US_dedes.OEkMiOnton.name
ocMA.OdeS3C1T.incipit = "Glorious things have been spoken…"
ocMA.OdeS3C1T.text = "Glorious things have been spoken concerning you, * O Lady, the animate city of Him who forever reigns; * for it was through you that God * was seen upon the earth and lived among men."

