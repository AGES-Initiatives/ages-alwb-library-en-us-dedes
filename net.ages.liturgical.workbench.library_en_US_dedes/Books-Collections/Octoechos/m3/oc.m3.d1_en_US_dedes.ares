ocMA.OdeS5C11.mode = misc_en_US_dedes.Mode3
ocMA.OdeS5C11.ode = misc_en_US_dedes.Ode5
ocMA.OdeS5C11.melody = he.h.m3_en_US_dedes.ProsSeOrthrizo.name
ocMA.OdeS5C11.incipit = "To an unjust judge…"
ocMA.OdeS5C11.text = "To an unjust judge, envious leaders of the Jews * delivered You, the all-seeing God who judges * all earth righteously. And You delivered * Adam who was condemned of old."

ocMA.OdeS5C12.mode = misc_en_US_dedes.Mode3
ocMA.OdeS5C12.ode = misc_en_US_dedes.Ode5
ocMA.OdeS5C12.melody = he.h.m3_en_US_dedes.ProsSeOrthrizo.name
ocMA.OdeS5C12.incipit = "By the invincible power…"
ocMA.OdeS5C12.text = "By the invincible power of Your Cross, O Christ, * who on the third day rose from the dead, O Savior, * grant Your peace to Your Church, we entreat You, * O Lord our God, and save our souls."

ocMA.OdeS5C1T.mode = misc_en_US_dedes.Mode3
ocMA.OdeS5C1T.ode = misc_en_US_dedes.Ode5
ocMA.OdeS5C1T.melody = he.h.m3_en_US_dedes.ProsSeOrthrizo.name
ocMA.OdeS5C1T.incipit = "O Ever-Virgin, you became…"
ocMA.OdeS5C1T.text = "O Ever-Virgin, you became like the holy Tent * and wider than the heavens, for you accepted * within your womb the Logos of God, * whom all creation cannot contain."

