ocMA.ApolTheotokion.text = oc.m1.d3_en_US_dedes.ocMA.ApolTheotokion.text

ocLI.Beatitudes1.mode = misc_en_US_dedes.Mode1
ocLI.Beatitudes1.melody = ""
ocLI.Beatitudes1.incipit = oc.m1.d1_en_US_dedes.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m1.d1_en_US_dedes.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_en_US_dedes.Mode1
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.incipit = "O Apostles, you enlightened all…"
ocLI.Beatitudes2.text = "O Apostles, you enlightened all * the ends of earth with the godly rays * of your true teachings, and dispelled * the darkness of ungodliness. * Having gone now to unsetting light, * you are blessed forevermore."

ocLI.Beatitudes3.mode = misc_en_US_dedes.Mode1
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.incipit = "O Christ's disciples, you had…"
ocLI.Beatitudes3.text = "O Christ's disciples, you had the Father's own * enhypostatic Wisdom dwelling in you, * which made you wise in things divine. * By the folly of your preaching, * you enlightened the whole wide world, * and you led it knowledge of God."

ocLI.Beatitudes4.mode = misc_en_US_dedes.Mode1
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.incipit = "You endured the tortures manfully…"
ocLI.Beatitudes4.text = "You endured the tortures manfully, * Martyrs of Christ, as though bodiless. * And you defeated all the foes who are bodiless mightily. * Therefore by all people now * you are blessed and extolled each day."

ocLI.BeatitudesTr.mode = misc_en_US_dedes.Mode1
ocLI.BeatitudesTr.melody = ""
ocLI.BeatitudesTr.incipit = "Let us believers worship and…"
ocLI.BeatitudesTr.text = "Let us believers worship and adore * the Holy Trinity, Father, Son, * and living Spirit, one in throne, * undivided, inseparable, * single God, to whom we cry aloud: * You are holy, O thrice-holy God."

ocLI.BeatitudesT.mode = misc_en_US_dedes.Mode1
ocLI.BeatitudesT.melody = ""
ocLI.BeatitudesT.incipit = "All-pure Virgin, we now call…"
ocLI.BeatitudesT.text = "All-pure Virgin, we now call you blest, * as you yourself once had prophesied. * For you gave birth to God, whom hosts * of Apostles proclaimed to all. * Pray with them for forgiveness * of our sins, O all-blameless one."

