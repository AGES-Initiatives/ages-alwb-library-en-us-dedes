ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.incipit = he.ga_en_US_dedes.heGA.TasAlgidonas.incipit
ocMA.Kathisma32.text = he.ga_en_US_dedes.heGA.TasAlgidonas.text

ocMA.Kathisma33.mode = miscellanea_en_US_dedes.misc.Mode1
ocMA.Kathisma33.greekmelody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.greekname
ocMA.Kathisma33.melody = he.a.m1_en_US_dedes.heAU.TonTafonSouSotir.name
ocMA.Kathisma33.incipit = "The Maiden free of stain…"
ocMA.Kathisma33.text = "The Maiden free of stain, the rational Ewe-lamb, * when she beheld the Lamb and Shepherd crucified, cried out, * lamenting and shedding tears as she saw her Son hanging dead, * \"How shall I extol Your condescension which no one * comprehends, my Son, and Your voluntary Passion, * O God most benevolent?\""

