ocMA.Kathisma32.mode = misc_en_US_dedes.Mode4
ocMA.Kathisma32.melody = 
ocMA.Kathisma32.text = "Your Church is arrayed in the holy blood of Your Martyrs who witnessed throughout the world, as though in purple and fine linen. Through them she cries to You, Christ our God, \"Send down to Your people Your tender love, grant peace from above to Your commonwealth, and to our souls Your great mercy.\""

