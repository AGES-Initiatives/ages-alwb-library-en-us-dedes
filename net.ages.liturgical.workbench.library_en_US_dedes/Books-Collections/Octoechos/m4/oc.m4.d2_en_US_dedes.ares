//The order of the Triadics is according to the Triodion
octrMA.Triadic1.mode = misc_en_US_dedes.Mode4
octrMA.Triadic1.incipit = "We mortals dare to offer…"
octrMA.Triadic1.text = "We mortals dare to offer You the hymn of Your noetic ministers, and we say: Holy, holy, holy are You our God."

octrMA.Triadic2.mode = misc_en_US_dedes.Mode4
octrMA.Triadic2.incipit = "Now like the order of…"
octrMA.Triadic2.text = "Now like the orders of Angels in heaven, and the ranks of people on earth, with fear we offer You a hymn of victory, O good One. Holy, holy, holy are You our God."

octrMA.Triadic3.mode = misc_en_US_dedes.Mode4
octrMA.Triadic3.incipit = "Like the Cherubim we dare…"
octrMA.Triadic3.text = "Like the Cherubim we dare to glorify the unoriginate Father, and You, O Christ God, and Your all-holy Spirit, and we say: Holy, holy, holy are You our God."

