ocMA.OdeS3C11.mode = misc_en_US_dedes.Mode4
ocMA.OdeS3C11.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C11.melody = he.h.m4_en_US_dedes.EffrainetaiEpiSoi.name
ocMA.OdeS3C11.incipit = "The Savior, whom we perceive…"
ocMA.OdeS3C11.text = "The Savior, whom we perceive * to be the tree of life, the true Vine himself, * was on the Cross crucified, * to all pouring out immortality."

ocMA.OdeS3C12.mode = misc_en_US_dedes.Mode4
ocMA.OdeS3C12.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C12.melody = he.h.m4_en_US_dedes.EffrainetaiEpiSoi.name
ocMA.OdeS3C12.incipit = "As God who is incorrupt…"
ocMA.OdeS3C12.text = "As God who is incorrupt, * the Lord has risen from the tomb bodily. * He humbled hell's haughtiness, * for He is our great and terrific God."

ocMA.OdeS3C1T.mode = misc_en_US_dedes.Mode4
ocMA.OdeS3C1T.ode = misc_en_US_dedes.Ode3
ocMA.OdeS3C1T.melody = he.h.m4_en_US_dedes.EffrainetaiEpiSoi.name
ocMA.OdeS3C1T.incipit = "From you only we on earth…"
ocMA.OdeS3C1T.text = "Through you only we on earth * procure divine and supernatural gifts, * all-holy Mother of God. * Therefore we salute you and sing, \"Rejoice!\""

