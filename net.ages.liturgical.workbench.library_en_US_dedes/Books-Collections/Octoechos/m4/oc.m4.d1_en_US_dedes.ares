ocMA.OdeS4C11.mode = misc_en_US_dedes.Mode4
ocMA.OdeS4C11.ode = misc_en_US_dedes.Ode4
ocMA.OdeS4C11.melody = he.h.m4_en_US_dedes.EparthentaSeIdousa.name
ocMA.OdeS4C11.incipit = "You were crucified…"
ocMA.OdeS4C11.text = "You were crucified, O Savior, to cure my passions * by what You suffered in Your all-immaculate body, * which You voluntarily put on. So we sing to You: * Glory to Your strength, O Lord!"

ocMA.OdeS4C12.mode = misc_en_US_dedes.Mode4
ocMA.OdeS4C12.ode = misc_en_US_dedes.Ode4
ocMA.OdeS4C12.melody = he.h.m4_en_US_dedes.EparthentaSeIdousa.name
ocMA.OdeS4C12.incipit = "Almighty Master…"
ocMA.OdeS4C12.text = "Almighty Master, when death had tasted Your sinless * and life-giving body, it was fittingly embittered * and utterly put to death. And we now sing out to You: * Glory to Your strength, O Lord!"

ocMA.OdeS4C1T.mode = misc_en_US_dedes.Mode4
ocMA.OdeS4C1T.ode = misc_en_US_dedes.Ode4
ocMA.OdeS4C1T.melody = he.h.m4_en_US_dedes.EparthentaSeIdousa.name
ocMA.OdeS4C1T.incipit = "Remaining virgin, you conceived…"
ocMA.OdeS4C1T.text = "Remaining virgin, you conceived in your womb, O Virgin, * and even after childbirth, you remained a virgin. * Therefore with unwavering faith, O Lady, unceasingly * singing, we salute you: Rejoice!"

