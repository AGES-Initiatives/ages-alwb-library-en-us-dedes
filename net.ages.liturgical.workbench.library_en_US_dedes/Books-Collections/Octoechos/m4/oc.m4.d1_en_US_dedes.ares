ocMA.OdeS5C1H.text = he.h.m4_en_US_dedes.SyKyrieMouFos.text

ocMA.OdeS5C11.mode = misc_en_US_dedes.Mode4
ocMA.OdeS5C11.ode = misc_en_US_dedes.Ode5
ocMA.OdeS5C11.melody = he.h.m4_en_US_dedes.SyKyrieMouFos.name
ocMA.OdeS5C11.incipit = "With sympathy for us…"
ocMA.OdeS5C11.text = "With sympathy for us * You descended to earth, O Lord. * You lifted humanity's fallen nature * when You were suspended on the Cross, O Christ."

ocMA.OdeS5C12.mode = misc_en_US_dedes.Mode4
ocMA.OdeS5C12.ode = misc_en_US_dedes.Ode5
ocMA.OdeS5C12.melody = he.h.m4_en_US_dedes.SyKyrieMouFos.name
ocMA.OdeS5C12.incipit = "O Christ, You took away…"
ocMA.OdeS5C12.text = "O Christ, You took away * every charge against me for sins. * By Your divine resurrection, * O compassionate Savior, You ended the distress of death."

ocMA.OdeS5C1T.mode = misc_en_US_dedes.Mode4
ocMA.OdeS5C1T.ode = misc_en_US_dedes.Ode5
ocMA.OdeS5C1T.melody = he.h.m4_en_US_dedes.SyKyrieMouFos.name
ocMA.OdeS5C1T.incipit = "Against our every foe…"
ocMA.OdeS5C1T.text = "Against our every foe, * we have you, our unbreakable shield. * We hope in you for salvation. * We have you as our anchor, all-holy Virgin, Bride of God."

