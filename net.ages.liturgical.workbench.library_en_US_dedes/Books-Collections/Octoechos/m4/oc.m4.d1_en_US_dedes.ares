ocMA.OdeS6C1H.text = he.h.m4_en_US_dedes.ThysoSoiMetaFonis.text

ocMA.OdeS6C11.mode = misc_en_US_dedes.Mode4
ocMA.OdeS6C11.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C11.melody = he.h.m4_en_US_dedes.ThysoSoiMetaFonis.name
ocMA.OdeS6C11.incipit = "O Savior, You went up…"
ocMA.OdeS6C11.text = "O Savior, You went up on the Cross, having girt yourself * with might, and wrestled the tyrant; * and as God from heaven, You took him down. * As for Adam, Your invincible hand resurrected him."

ocMA.OdeS6C12.mode = misc_en_US_dedes.Mode4
ocMA.OdeS6C12.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C12.melody = he.h.m4_en_US_dedes.ThysoSoiMetaFonis.name
ocMA.OdeS6C12.incipit = "O Christ, dazzling…"
ocMA.OdeS6C12.text = "O Christ, dazzling and handsome, You rose from the sepulcher; * and by divine might and power, * You utterly scattered all Your opponents; * and as God You filled all things with divine joy and happiness."

ocMA.OdeS6C1T.mode = misc_en_US_dedes.Mode4
ocMA.OdeS6C1T.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C1T.melody = he.h.m4_en_US_dedes.ThysoSoiMetaFonis.name
ocMA.OdeS6C1T.incipit = "O miracle, more novel…"
ocMA.OdeS6C1T.text = "O miracle, more novel than all other miracles! * Behold, without man a Virgin * in her body conceived the One who holds * in His hands the universe, and He was not confined by her."

