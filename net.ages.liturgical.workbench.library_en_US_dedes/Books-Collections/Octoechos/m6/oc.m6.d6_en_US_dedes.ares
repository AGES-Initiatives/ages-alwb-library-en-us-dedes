ocLI.Beatitudes1.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.Beatitudes1.incipit = oc.m6.d1_en_US_dedes.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m6.d1_en_US_dedes.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_en_US_dedes.Mode6
ocLI.Beatitudes2.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.Beatitudes2.incipit = "Being crucified, O Lord, You…"
ocLI.Beatitudes2.text = "Being crucified, O Lord, You spread Your arms out * and embraced all the nations, that now extol * Your tender mercy, O Christ our Savior and God."

ocLI.Beatitudes3.mode = misc_en_US_dedes.Mode6
ocLI.Beatitudes3.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.Beatitudes3.incipit = "You were nailed upon the…"
ocLI.Beatitudes3.text = "You were nailed upon the Cross of Your own will, Lord. * Being crucified, You, by Your might, O Christ, * obliterated the stingers of the evil one."

ocLI.Beatitudes4.mode = misc_en_US_dedes.Mode6
ocLI.Beatitudes4.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.Beatitudes4.incipit = "Holy Martyrs, having reveled in…"
ocLI.Beatitudes4.text = "Holy Martyrs, having reveled in the tortures, * you were granted the pleasures of Paradise; * and you unceasingly pray on behalf of the world."

ocLI.BeatitudesTr.mode = misc_en_US_dedes.Mode6
ocLI.BeatitudesTr.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.BeatitudesTr.incipit = "Father, Son, and Spirit, reigning…"
ocLI.BeatitudesTr.text = "Father, Son, and Spirit, reigning as the one God, * we Your servants entreat You, our single Lord, * to extricate us from torments and punishment."

ocLI.BeatitudesT.mode = misc_en_US_dedes.Mode6
ocLI.BeatitudesT.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.BeatitudesT.incipit = "Then the Virgin saw You…"
ocLI.BeatitudesT.text = "Then the Virgin saw You crucified, O Savior, * and she wept, heaving sighs of maternal grief. * She intercedes now for us, that You save us, O Lord."

