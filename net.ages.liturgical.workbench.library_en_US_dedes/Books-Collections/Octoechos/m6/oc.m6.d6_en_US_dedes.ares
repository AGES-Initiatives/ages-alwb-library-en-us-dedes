ocMA.AposTheotokion.mode = misc_en_US_dedes.Mode6
ocMA.AposTheotokion.melody = he.a.m6_en_US_dedes.TriimerosAnestis.name
ocMA.AposTheotokion.incipit = "As man, I have been…"
ocMA.AposTheotokion.text = "As man, I have been nailed to a cross, * and truly I am put to death. * As a mortal, I am laid within a tomb, * but as God, I will in glory * rise again on the third day, * O holy Mother, ever-virgin and pure."

