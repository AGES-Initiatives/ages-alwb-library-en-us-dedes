ocMA.OdeS5C11.mode = misc_en_US_dedes.Mode6
ocMA.OdeS5C11.ode = misc_en_US_dedes.Ode5
ocMA.OdeS5C11.melody = he.h.m6_en_US_dedes.ToTheioFengeiSou.name
ocMA.OdeS5C11.incipit = "The cherubim now stand down…"
ocMA.OdeS5C11.text = "The cherubim now stand down for me, * and the fiery sword that guards the tree of life turns away from me, O Lord; * for they saw that You, the Word of God and truly God, * had led the way for the Robber into Paradise."

ocMA.OdeS5C12.mode = misc_en_US_dedes.Mode6
ocMA.OdeS5C12.ode = misc_en_US_dedes.Ode5
ocMA.OdeS5C12.melody = he.h.m6_en_US_dedes.ToTheioFengeiSou.name
ocMA.OdeS5C12.incipit = "I am no longer afraid…"
ocMA.OdeS5C12.text = "I am no longer afraid of death * nor of my return to earth, O Master Christ, for You raised me up from earth, * where I was forgotten, in Your great mercy, to heights * of incorruption, by Your resurrection, O Lord."

ocMA.OdeS5C1T.mode = misc_en_US_dedes.Mode6
ocMA.OdeS5C1T.ode = misc_en_US_dedes.Ode5
ocMA.OdeS5C1T.melody = he.h.m6_en_US_dedes.ToTheioFengeiSou.name
ocMA.OdeS5C1T.incipit = "O gracious Lady …"
ocMA.OdeS5C1T.text = "O gracious Lady of all the world, * save us who wholeheartedly confess you Theotokos, Mother of God. * We are blest to have you as unassailable * protection, for you truly have given birth to God."

