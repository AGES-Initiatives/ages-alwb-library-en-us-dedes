ocMA.OdeS4C11.mode = misc_en_US_dedes.Mode6
ocMA.OdeS4C11.ode = misc_en_US_dedes.Ode4
ocMA.OdeS4C11.melody = he.h.m6_en_US_dedes.ChristosMouDynamis.name
ocMA.OdeS4C11.incipit = "The tree of true eternal life…"
ocMA.OdeS4C11.text = "The tree of true eternal life has blossomed, O Christ. * For on Calvary planted, Your precious Cross * was watered by blood and water spilling from Your holy side; * it produced the fruit of life for us."

ocMA.OdeS4C12.mode = misc_en_US_dedes.Mode6
ocMA.OdeS4C12.ode = misc_en_US_dedes.Ode4
ocMA.OdeS4C12.melody = he.h.m6_en_US_dedes.ChristosMouDynamis.name
ocMA.OdeS4C12.incipit = "Since Christ, who fashioned…"
ocMA.OdeS4C12.text = "Since Christ, who fashioned human nature, being God, * has removed all obstructions and opened wide * the path to life for me, the serpent whispers no more to me * false suggestions of becoming a god."

ocMA.OdeS4C1T.mode = misc_en_US_dedes.Mode6
ocMA.OdeS4C1T.ode = misc_en_US_dedes.Ode4
ocMA.OdeS4C1T.melody = he.h.m6_en_US_dedes.ChristosMouDynamis.name
ocMA.OdeS4C1T.incipit = "Incomprehensible, indeed ineffable…"
ocMA.OdeS4C1T.text = "Incomprehensible, indeed ineffable, * for all people on earth and the hosts on high, * are the divine mysteries, O Virgin Mother of God, * of your marvelous conception of God."

