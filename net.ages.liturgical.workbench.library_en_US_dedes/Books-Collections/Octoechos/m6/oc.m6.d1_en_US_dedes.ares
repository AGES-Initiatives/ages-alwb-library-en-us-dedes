ocMA.OdeS6C1H.melody = he.h.m6_en_US_dedes.TouViouTinThalassan.melody
ocMA.OdeS6C1H.incipit = he.h.m6_en_US_dedes.TouViouTinThalassan.incipit
ocMA.OdeS6C1H.text = he.h.m6_en_US_dedes.TouViouTinThalassan.text

ocMA.OdeS6C11.mode = misc_en_US_dedes.Mode6
ocMA.OdeS6C11.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C11.melody = he.h.m6_en_US_dedes.TouViouTinThalassan.name
ocMA.OdeS6C11.incipit = "O Master, You freed the world…"
ocMA.OdeS6C11.text = "O Master, You freed the world, being crucified for us; * for by the nails, the curse on us * has been canceled, and also the bond that stood * against Adam and us all * was shredded by the spear that pierced Your side, O Lord."

ocMA.OdeS6C12.mode = misc_en_US_dedes.Mode6
ocMA.OdeS6C12.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C12.melody = he.h.m6_en_US_dedes.TouViouTinThalassan.name
ocMA.OdeS6C12.incipit = "Enticed by deceptive means…"
ocMA.OdeS6C12.text = "Enticed by deceptive means, Adam slipped and tumbled down * to Hades' pit of punishment. * O compassionate Christ, and by nature God, * You went down to look for him. * On Your shoulders You put him, and You raised him with You."

ocMA.OdeS6C1T.mode = misc_en_US_dedes.Mode6
ocMA.OdeS6C1T.ode = misc_en_US_dedes.Ode6
ocMA.OdeS6C1T.melody = he.h.m6_en_US_dedes.TouViouTinThalassan.name
ocMA.OdeS6C1T.incipit = "All-pure and immaculate Lady…"
ocMA.OdeS6C1T.text = "All-pure and immaculate Lady, you for us gave birth * to Him who governs all as Lord. * As my passions relentlessly and with force * disturb me, I pray to you, * that you grant to my heart a sense of peace and calm."

