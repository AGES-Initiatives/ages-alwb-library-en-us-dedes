ocLI.Beatitudes1.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.Beatitudes1.incipit = oc.m6.d1_en_US_dedes.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m6.d1_en_US_dedes.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_en_US_dedes.Mode6
ocLI.Beatitudes2.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.Beatitudes2.incipit = "O Apostles of the Lord…"
ocLI.Beatitudes2.text = "O Apostles of the Lord, you were illumined, * and like luminous clouds you showered the world * with the water of inspired knowledge of God."

ocLI.Beatitudes3.mode = misc_en_US_dedes.Mode6
ocLI.Beatitudes3.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.Beatitudes3.incipit = "You, O glorious Apostles, have…"
ocLI.Beatitudes3.text = "You, O glorious Apostles, have illumined * with the true faith in God all the human race, * for you traversed the whole world like mystical rays."

ocLI.Beatitudes4.mode = misc_en_US_dedes.Mode6
ocLI.Beatitudes4.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.Beatitudes4.incipit = "You experienced the fire of…"
ocLI.Beatitudes4.text = "You experienced the fire of the tortures * for your faith, holy Martyrs, and you received * divine refreshment like dew from the Lord of all."

ocLI.BeatitudesTr.mode = misc_en_US_dedes.Mode6
ocLI.BeatitudesTr.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.BeatitudesTr.incipit = "O believers, as in unison…"
ocLI.BeatitudesTr.text = "O believers, as in unison we worship * the divine Holy Trinity, let us say: * \"At the entreaties of the Apostles, save us all.\""

ocLI.BeatitudesT.mode = misc_en_US_dedes.Mode6
ocLI.BeatitudesT.melody = he.h.m6_en_US_dedes.IToChaireDiAngelou.name
ocLI.BeatitudesT.incipit = "You gave birth the One…"
ocLI.BeatitudesT.text = "You gave birth the One who with the Father * is beginningless, ineffably through a word. * O Theotokos, entreat Him to save our souls."

