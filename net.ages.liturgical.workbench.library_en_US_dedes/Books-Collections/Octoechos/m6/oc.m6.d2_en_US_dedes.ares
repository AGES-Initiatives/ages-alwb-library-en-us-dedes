ocVE.Stichera5.mode = miscellanea_en_US_dedes.misc.Mode6
ocVE.Stichera5.melody = he.a.m6_en_US_dedes.heAU.OlinApothemenoi.name
ocVE.Stichera5.incipit = "…"
