//The order of the Triadics is according to the Triodion
octrMA.Triadic1.mode = miscellanea_en_US_dedes.misc.Mode6
octrMA.Triadic1.incipit = "With fear the Cherubim…"
octrMA.Triadic1.text = "With fear the Cherubim standing near You, and trembling the Seraphim astounded, sing to You the thrice-holy hymn, with never-silent voices. And we, sinners though we be, join them and cry out: Holy, holy, holy are You, our God!"

octrMA.Triadic2.mode = miscellanea_en_US_dedes.misc.Mode6
octrMA.Triadic2.incipit = "The six-winged Seraphim…"
octrMA.Triadic2.text = "The six-winged Seraphim, with their bodiless mouths, never cease to sing the thrice-holy hymn to You, our God. And we on earth, albeit with unworthy lips, send up praises to You: Holy, holy, holy are You, our God!"

octrMA.Triadic3.mode = miscellanea_en_US_dedes.misc.Mode6
octrMA.Triadic3.incipit = "Let us glorify the Godhead…"
octrMA.Triadic3.text = "Let us glorify the Godhead, which is unconfused Tri-unity. And let us sing the hymn of the Angels: Holy, holy, holy are You, our God!"

