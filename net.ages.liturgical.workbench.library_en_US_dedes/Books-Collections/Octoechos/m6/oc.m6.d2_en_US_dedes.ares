ocLI.Beatitudes1.melody = he.h.m6_en_US_dedes.heHE.IToChaireDiAngelou.name
ocLI.Beatitudes1.incipit = oc.m6.d1_en_US_dedes.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m6.d1_en_US_dedes.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = miscellanea_en_US_dedes.misc.Mode6
ocLI.Beatitudes2.melody = he.h.m6_en_US_dedes.heHE.IToChaireDiAngelou.name
ocLI.Beatitudes2.incipit = "Overlooking * all the sins…"
ocLI.Beatitudes2.text = "Overlooking * all the sins I did in knowledge * and in ignorance, Jesus my loving Lord, * I pray You, grant me a place with the saved on high."

ocLI.Beatitudes3.mode = miscellanea_en_US_dedes.misc.Mode6
ocLI.Beatitudes3.melody = he.h.m6_en_US_dedes.heHE.IToChaireDiAngelou.name
ocLI.Beatitudes3.incipit = "You make brilliant * all…"
ocLI.Beatitudes3.text = "You make brilliant * all the orders of the Angels. * At their earnest entreaties on my behalf, * also enlighten the eyes of my heart, O Christ."

ocLI.Beatitudes4.mode = miscellanea_en_US_dedes.misc.Mode6
ocLI.Beatitudes4.melody = he.h.m6_en_US_dedes.heHE.IToChaireDiAngelou.name
ocLI.Beatitudes4.incipit = "For enduring * the excruciating…"
ocLI.Beatitudes4.text = "For enduring * the excruciating torments, * you, O Martyrs, were given the grace from God * to heal our every trauma and tormenting pain."

ocLI.BeatitudesTr.mode = miscellanea_en_US_dedes.misc.Mode6
ocLI.BeatitudesTr.melody = he.h.m6_en_US_dedes.heHE.IToChaireDiAngelou.name
ocLI.BeatitudesTr.incipit = "The angelic * hosts on…"
ocLI.BeatitudesTr.text = "The angelic * hosts on high ever extol You. * We on earth sing Your glory and worship You, * and pray: O Holy Trinity, save our souls."

ocLI.BeatitudesT.mode = miscellanea_en_US_dedes.misc.Mode6
ocLI.BeatitudesT.melody = he.h.m6_en_US_dedes.heHE.IToChaireDiAngelou.name
ocLI.BeatitudesT.incipit = "I implore you, * rescue…"
ocLI.BeatitudesT.text = "I implore you, * rescue me from the eternal * fire and well-deserved torments in store for me. * And I will call you blessed, O Mother of God."

ocLI.MM.Beatitudes1234TrTas78910GB.text = properties_en_US_dedes.media.key

