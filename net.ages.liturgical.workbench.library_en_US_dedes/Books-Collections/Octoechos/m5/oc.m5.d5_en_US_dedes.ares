ocMA.Kathisma32.mode = misc_en_US_dedes.Mode5
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.incipit = "Lord, Your medal-winning martyrs longed…"
ocMA.Kathisma32.text = "Lord, Your medal-winning martyrs longed to drink the cup of Your Passion. So they forsook the delights of this life, and they became partners with the Angels. At their supplications, grant to our souls forgiveness of sins and the great mercy."

