ocMA.OdeS6C11.mode = miscellanea_en_US_dedes.misc.Mode5
ocMA.OdeS6C11.ode = miscellanea_en_US_dedes.misc.Ode6
ocMA.OdeS6C11.melody = he.h.m5_en_US_dedes.MainomeninKlydoni.name
ocMA.OdeS6C11.incipit = "Master Christ, our ancestor…"
ocMA.OdeS6C11.text = "Master Christ, our ancestor in the garden ate forbidden fruit, * and he slipped and fell into decay and death. * Now he is raised to life again, by Your Passion, O Lord."

ocMA.OdeS6C12.mode = miscellanea_en_US_dedes.misc.Mode5
ocMA.OdeS6C12.ode = miscellanea_en_US_dedes.misc.Ode6
ocMA.OdeS6C12.melody = he.h.m5_en_US_dedes.MainomeninKlydoni.name
ocMA.OdeS6C12.incipit = "Master Christ and Life itself…"
ocMA.OdeS6C12.text = "Master Christ and Life itself, You went down to Hades and became * the destruction of the one destroying us. * You have gushed forth resurrection by dying for us."

ocMA.OdeS6C1T.mode = miscellanea_en_US_dedes.misc.Mode5
ocMA.OdeS6C1T.ode = miscellanea_en_US_dedes.misc.Ode6
ocMA.OdeS6C1T.melody = he.h.m5_en_US_dedes.MainomeninKlydoni.name
ocMA.OdeS6C1T.incipit = "Virgin Mary bore a child…"
ocMA.OdeS6C1T.text = "Virgin Mary bore a child; after childbirth she continued chaste. * As a truly virgin Mother, in her arms * she held the One who holds the whole wide world in His hands."

