ocMA.OdeS5C11.mode = miscellanea_en_US_dedes.misc.Mode5
ocMA.OdeS5C11.ode = miscellanea_en_US_dedes.misc.Ode5
ocMA.OdeS5C11.melody = he.h.m5_en_US_dedes.OAnavallomenos.name
ocMA.OdeS5C11.incipit = "Christ, the Lord of glory…"
ocMA.OdeS5C11.text = "Christ, the Lord of glory, who assumed inglorious form, * is voluntarily stripped of honor * and unjustly crucified, thus for me procuring * glory divine ineffably."

ocMA.OdeS5C12.mode = miscellanea_en_US_dedes.misc.Mode5
ocMA.OdeS5C12.ode = miscellanea_en_US_dedes.misc.Ode5
ocMA.OdeS5C12.melody = he.h.m5_en_US_dedes.OAnavallomenos.name
ocMA.OdeS5C12.incipit = "Tasting death's destruction…"
ocMA.OdeS5C12.text = "Tasting death's destruction in the flesh without decay, * O Christ my Savior, and within three days * rising from the sepulcher, * You put the imperishable on my nature, O Lord."

ocMA.OdeS5C1T.mode = miscellanea_en_US_dedes.misc.Mode5
ocMA.OdeS5C1T.ode = miscellanea_en_US_dedes.misc.Ode5
ocMA.OdeS5C1T.melody = he.h.m5_en_US_dedes.OAnavallomenos.name
ocMA.OdeS5C1T.incipit = "You, O Theotokos…"
ocMA.OdeS5C1T.text = "You, O Theotokos, without seed gave birth to Christ, * who is our justice and our redemption; * in so doing you have freed * the nature of Adam, our forefather, from the curse."

