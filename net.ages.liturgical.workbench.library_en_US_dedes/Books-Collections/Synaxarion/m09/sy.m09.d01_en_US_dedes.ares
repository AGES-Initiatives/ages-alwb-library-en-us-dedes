S00.days = "The Month of September, which has 30 days."
S00.hours = "The day has 12 hours, and the night has 12 hours."

S01.commemoration.text = "On September 1 is the beginning of the Indiction, i.e. the new [Ecclesiastical] Year."
S01.narration1.p01.text = "[paragraph 01]"
S01.narration1.p02.text = "[paragraph 02]"
S01.narration1.p03.text = "[paragraph 03]"
S01.narration1.p04.text = "[paragraph 04]"
S01.narration1.p05.text = "[paragraph 05]"
S01.narration1.p06.text = "[paragraph 06]"
S01.narration1.p07.text = "[paragraph 07]"

S02.commemoration.text = "On this day we commemorate the miracle done by the holy Theotokos in the Monastery of Miasinae; and we also commemorate the fire that took place."
S02.narration1.p01.text = "[paragraph 01]"
S02.narration1.p02.text = "[paragraph 02]"
S02.narration1.p03.text = "[paragraph 03]"
S02.narration1.p04.text = "[paragraph 04]"
S02.narration1.p05.text = "[paragraph 05]"
S02.narration1.p06.text = "[paragraph 06]"
S02.narration1.p07.text = "[paragraph 07]"

S03.commemoration.text = "On this day we also commemorate our devout father Symeon the Stylite."

S04.commemoration.text = "On this day we also commemorate devout St. Martha, mother of St. Symeon; and devout St. Evanthia; and also the repose of Joshua, son of Nun."

S05.commemoration.text = "On this day we also commemorate the forty holy women Martyrs, Virgins, and Ascetics, and also their teacher, the Deacon Ammon."

S06.commemoration.text = "On this day we also commemorate the holy martyrs and siblings, Evodus, Callista, and Hermogenes."

S07.commemoration.text = "Devout St. Meletius the New, who lived as an ascetic on the Mountain of Myoupolis, died in peace."

S08.commemoration.text = "The holy Neomartyr Angelis, who witnessed in Constantinople in the year 1680, died by the sword."

S09.commemoration.text = ""

S10.commemoration.text = ""

S99.lastline.text= "By their holy intercessions, O God, have mercy on us. Amen."

