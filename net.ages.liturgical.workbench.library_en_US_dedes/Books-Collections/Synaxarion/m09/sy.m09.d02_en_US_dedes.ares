syMA.commemoration01.text = "On September 2, we commemorate the holy martyr Mamas."

syMA.commemoration02.text = "On this day we commemorate our holy father John the Faster, Patriarch of Constantinople."

syMA.commemoration03.text = "St. Diomides died when he was run through by a sword."

syMA.commemoration04.text = "St. Julian died when he was beaten on the head by a plank."

syMA.commemoration05.text = "St. Philip died by the sword."

syMA.commemoration06.text = "St. Eutychian died on a fire grill."

syMA.commemoration07.text = "St. Hesychios died by hanging."

syMA.commemoration08.text = "St. Leonides died by fire."

syMA.commemoration09.text = "St. Eutychios died by crucifixion."

syMA.commemoration10.text = "St. Philadelphus died with the stone weight around his neck."

syMA.commemoration11.text = "St. Melanippus died by fire."

syMA.commemoration12.text = "St. Parthagape died in the sea."

syMA.commemoration13.text = "On this day we commemorate the just priests Eleazar and Phineas and also the holy martyrs Aeithala and Ammoun."

syMA.lastline = "By the intercessions of Your saints, O God, have mercy on us. Amen."

