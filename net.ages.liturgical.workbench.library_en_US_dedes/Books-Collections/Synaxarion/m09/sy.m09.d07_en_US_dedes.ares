syMA.commemoration01.text = "On September 7, we commemorate the holy Martyr Sozon."
syMA.commemoration01as02.text = "On this day we commemorate the holy Martyr Sozon."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.Stichoi
