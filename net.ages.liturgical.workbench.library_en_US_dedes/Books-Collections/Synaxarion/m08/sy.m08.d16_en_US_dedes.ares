syMA.commemoration01.text = "On August 16 we commemorate the holy Martyr Diomedes."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration01.couplet1.verse1 = ""
syMA.commemoration01.couplet1.verse2 = ""
syMA.commemoration01.single1 = ""
syMA.commemoration01.narration.p01 = ""

syMA.commemoration02.text = "On this day devout Saint Chaeremon died in peace."
syMA.commemoration02.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration02.couplet1.verse1 = ""
syMA.commemoration02.couplet1.verse2 = ""

syMA.commemoration03.text = "On this day we also commemorate the holy 33 Martyrs from Palestine who died by the sword."
syMA.commemoration03.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration03.couplet1.verse1 = ""
syMA.commemoration03.couplet1.verse2 = ""

syMA.commemoration04.text = "On this day we commemorate the entry of the Icon of the Face of our Lord God and Savior Jesus Christ, which was not made by hands, from the city of Edessa into that God-protected Queen of Cities, Constantinople."
syMA.commemoration04.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration04.couplet1.verse1 = ""
syMA.commemoration04.couplet1.verse2 = ""
syMA.commemoration04.couplet2.title = "For the Tile"
syMA.commemoration04.couplet2.verse1 = ""
syMA.commemoration04.couplet2.verse2 = ""
syMA.commemoration04.narration.p01 = ""
syMA.commemoration04.narration.p02 = ""
syMA.commemoration04.narration.p03 = ""
syMA.commemoration04.narration.p04 = ""
syMA.note1 = ""
syMA.commemoration04.narration.p05 = ""
syMA.commemoration04.narration.p06 = ""
syMA.commemoration04.narration.p07 = ""
syMA.commemoration04.narration.p08 = ""
syMA.commemoration04.narration.p09 = ""
syMA.commemoration04.narration.p10 = ""
syMA.commemoration04.narration.p11 = ""
