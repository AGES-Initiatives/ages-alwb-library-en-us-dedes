syMA.commemoration01.text = "On August 27, we commemorate our devout father Pimen."
syMA.commemoration01as02.text = "Today, we also commemorate our devout father Pimen."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration01.couplet1.verse1 = ""
syMA.commemoration01.couplet1.verse2 = ""
