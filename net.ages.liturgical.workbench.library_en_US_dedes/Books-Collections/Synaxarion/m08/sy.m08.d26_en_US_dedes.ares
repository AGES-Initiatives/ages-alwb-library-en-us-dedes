A_Resource_Whose_Name = sy.m08.d26_en_US_dedes

syMA.commemoration01.text = "On August 26 we commemorate the holy Martyrs Adrian and Natalie and their company."

syMA.commemoration02.text = "On this day those who suffered with St. Adrian died by the sword."

syMA.commemoration03.text = "On this day we also commemorate the Holy Martyrs Atticus and Sisinos, and our devout Father St. Hivistian."

syMA.commemoration04.text = "On this day we also commemorate another Adrian."

syMA.commemoration05.text = "On this day we also commemorate the devout monk St. Ioasaf, son of Anevir the king."

syMA.commemoration06.text = "On this day the devout monk St. Tithoes reposed in peace."

syMA.lastline = "By the intercessions of Your Saints, O God, have mercy on us. Amen."

