A_Resource_Whose_Name = sy.m08.d05_en_US_dedes 

syMA.commemoration01.text = "On August 5 is the Forefeast of the holy Transfiguration of our Lord, God and Savior Jesus Christ. We also commemorate the holy Martyr Eusignius."

syMA.commemoration02.text = "On this day, the two holy Martyrs and brothers Cattidius and Cattidianus were stoned to death."

syMA.commemoration03.text = "On this day, Saint Nonna, the mother of Saint Gregory the Theologian, died in peace."

syMA.commemoration04.text = "On this day, the holy Martyr Soleb the Egyptian died when he was shot with arrows."

syMA.commemoration05.text = "On this day the holy Hieromartyr Fabian, Bishop of Rome, died by the sword."

syMA.lastline = "By the intercessions of Your Saints, O God, have mercy on us. Amen."

