A_Resource_Whose_Name = sy.m12.d28_en_US_dedes

syMA.commemoration01.text = "On December 28 we commemorate the Holy 20,000 who burned in Nicomedia."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration01.couplet1.verse1 = ""
syMA.commemoration01.couplet1.verse2 = ""
syMA.commemoration01.single1 = ""
syMA.commemoration01.narration.p01 = ""

syMA.commemoration02.text = "On this day we also commemorate the holy Martyrs, who were in the Senate, who fled from the fire, died."

syMA.commemoration03.text = "St. Indos, died by drowning in the sea."
syMA.commemoration03.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration03.couplet1.verse1 = ""
syMA.commemoration03.couplet1.verse2 = ""

syMA.commemoration04.text = "Sts. Gorgon and Peter, died in the sea."
syMA.commemoration04.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration04.couplet1.verse1 = ""
syMA.commemoration04.couplet1.verse2 = ""

syMA.commemoration05.text = "The holy Martyr Zeno, died by the sword."
syMA.commemoration05.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration05.couplet1.verse1 = ""
syMA.commemoration05.couplet1.verse2 = ""

syMA.commemoration06.text = "The holy Martyr Mardonios, died by fire."
syMA.commemoration06.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration06.couplet1.verse1 = ""
syMA.commemoration06.couplet1.verse2 = ""

syMA.commemoration07.text = "St. Dorotheos, the courtier, died by the sword."
syMA.commemoration07.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration07.couplet1.verse1 = ""
syMA.commemoration07.couplet1.verse2 = ""

syMA.commemoration08.text = "St. Theophilos the Deacon, died by stoning."
syMA.commemoration08.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration08.couplet1.verse1 = ""
syMA.commemoration08.couplet1.verse2 = ""

syMA.commemoration09.text = "St. Mygdonios died by being buried in a pit."
syMA.commemoration09.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration09.couplet1.verse1 = ""
syMA.commemoration09.couplet1.verse2 = ""

syMA.commemoration10.text = "St. Glycerius the Priest, died by fire."
syMA.commemoration10.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration10.couplet1.verse1 = ""
syMA.commemoration10.couplet1.verse2 = ""
syMA.commemoration10.narration.p01 = ""

syMA.commemoration11.text = "On this day St. Domna, was cut by the sword and died by fire."
syMA.commemoration11.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration11.couplet1.verse1 = ""
syMA.commemoration11.couplet1.verse2 = ""
syMA.commemoration11.narration.p01 = ""

syMA.commemoration12.text = "The devout St. Simon the Myrrh-Streamer, the founder of the holy Monastery of Simonopetra on Athos reposed in peace."
syMA.commemoration12.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration12.couplet1.verse1 = ""
syMA.commemoration12.couplet1.verse2 = ""

syMA.lastline = "By the intercessions of Your Saints, O God, have mercy and save us. Amen."
