syMA.commemoration01.couplet1.verse2 = ""
syMA.commemoration01.single1 = ""
syMA.commemoration01.couplet2.title = "Περὶ τῆς εἰς Αἴγυπτον φυγῆς τῆς Ὑπεραγίας Θεοτόκου"
