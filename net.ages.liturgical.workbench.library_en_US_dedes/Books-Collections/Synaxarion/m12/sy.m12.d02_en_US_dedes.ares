A_Resource_Whose_Name = sy.m12.d02_en_US_dedes

S01.commemoration.text = "On December 2, we commemorate the holy Prophet Habakkuk."

S02.commemoration.text = "On this day we also commemorate our devout fathers the hermits John, Heraklaemon, Andrew, and Theophilos."

S03.commemoration.text = "On this day we also commemorate the holy martyr Myropi."

S04.commemoration.text = "We also commemorate our devout father Cyril Phileotis, who was an ascetic in the year 1060."

S05.commemoration.text = "Saint Habib the New, died by fire."

S06.commemoration.text = "Our father among the saints Solomon, Archbishop of Ephesus, died in peace."

S07.commemoration.text = "On this day we commemorate our devout and God-bearing father Porphyrios of Kafsokalyvia, the clairvoyant and miracle-worker."
S07.couplet1.title = titles_en_US_dedes.Stichoi
S07.couplet1.verse1 = "Porphyrios, you wished  to emulate the Hut Dweller,"
S07.couplet1.verse2 = "so you ran to Athos and lived as an ascetic."
S07.single1.text = "On the second, Porphyrios exchanged earth for Paradise."
S07.narration1.text = ""

S08.commemoration.text = ""

S09.commemoration.text = ""

S10.commemoration.text = ""

S99.lastline.text= "By their holy intercessions, O God, have mercy on us. Amen."
