A_Resource_Whose_Name = sy.m12.d02_en_US_dedes

syMA.commemoration01.text = "On December 2, we commemorate the holy Prophet Habakkuk."

syMA.commemoration02.text = "On this day we also commemorate our devout fathers the hermits John, Heraklaemon, Andrew, and Theophilos."

syMA.commemoration03.text = "On this day we also commemorate the holy martyr Myropi."

syMA.commemoration04.text = "We also commemorate our devout father Cyril Phileotis, who was an ascetic in the year 1060."

syMA.commemoration05.text = "Saint Habib the New, died by fire."

syMA.commemoration06.text = "Our father among the saints Solomon, Archbishop of Ephesus, died in peace."

syMA.commemoration07.text = "On this day we commemorate our devout and God-bearing father Porphyrios of Kafsokalyvia, the clairvoyant and miracle-worker."
syMA.commemoration07.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration07.couplet1.verse1 = "Porphyrios, you wished  to emulate the Hut Dweller,"
syMA.commemoration07.couplet1.verse2 = "so you ran to Athos and lived as an ascetic."
syMA.commemoration07.single1 = "On the second, Porphyrios exchanged earth for Paradise."

syMA.commemoration08.text = ""

syMA.commemoration09.text = ""

syMA.commemoration10.text = ""

syMA.lastline = "By their holy intercessions, O God, have mercy on us. Amen."

