syMA.commemoration01.text = "On December 2, we commemorate the holy Prophet Habakkuk."
syMA.commemoration01as02.text = "On this day we also commemorate the holy Prophet Habakkuk."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration01.couplet1.verse1 = ""
syMA.commemoration01.couplet1.verse2 = ""
syMA.commemoration01.single1 = ""
syMA.commemoration01.narration.p01 = ""
syMA.commemoration01.narration.p02 = ""

syMA.commemoration02.text = "On this day we also commemorate our devout fathers the hermits John, Heraklaemon, Andrew, and Theophilos."
syMA.commemoration02.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration02.couplet1.verse1 = ""
syMA.commemoration02.couplet1.verse2 = ""
syMA.commemoration02.narration.p01 = ""

syMA.commemoration03.text = "On this day we also commemorate the holy martyr Myropi."
syMA.commemoration03.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration03.couplet1.verse1 = ""
syMA.commemoration03.couplet1.verse2 = ""
syMA.commemoration03.narration.p01 = ""
syMA.commemoration03.narration.p02 = ""
syMA.commemoration03.narration.p03 = ""
syMA.commemoration03.narration.p04 = ""
syMA.commemoration03.narration.p05 = ""

syMA.commemoration04.text = "We also commemorate our devout father Cyril Phileotis, who was an ascetic in the year 1060."
syMA.commemoration04.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration04.couplet1.verse1 = ""
syMA.commemoration04.couplet1.verse2 = ""

syMA.commemoration05.text = "Saint Habib the New, died by fire."
syMA.commemoration05.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration05.couplet1.verse1 = ""
syMA.commemoration05.couplet1.verse2 = ""

syMA.commemoration06.text = "Our father among the saints Solomon, Archbishop of Ephesus, died in peace."
syMA.commemoration06.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration06.couplet1.verse1 = ""
syMA.commemoration06.couplet1.verse2 = ""

syMA.commemoration07.text = "On this day we also commemorate our devout and God-bearing father Porphyrios of Kafsokalyvia, the clairvoyant and miracle-worker."
syMA.commemoration07.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration07.couplet1.verse1 = "Porphyrios, you wished to emulate the Hut Dweller,"
syMA.commemoration07.couplet1.verse2 = "so you ran to Athos and lived as an ascetic."
syMA.commemoration07.single1 = "On the second, Porphyrios exchanged earth for Paradise."
syMA.commemoration07.narration.p01 = ""

alt1.syMA.commemoration01.text = "On December 2, we commemorate our devout and God-bearing father Porphyrios of Kafsokalyvia, the clairvoyant and miracle-worker."
alt1.syMA.commemoration01.couplet1.title = titles_en_US_dedes.ti.Verses
