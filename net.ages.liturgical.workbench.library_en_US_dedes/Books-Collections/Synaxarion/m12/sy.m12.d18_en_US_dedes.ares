syMA.commemoration02.text = "On this day we also commemorate the holy martyr Eubiotus."
syMA.commemoration02.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration02.couplet1.verse1 = ""
syMA.commemoration02.couplet1.verse2 = ""
syMA.commemoration02.narration.p01 = ""
syMA.commemoration02.narration.p02 = ""

syMA.commemoration03.text = "On this day we also commemorate our devout father Florus, Bishop of Amisus."
syMA.commemoration03.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration03.couplet1.verse1 = ""
syMA.commemoration03.couplet1.verse2 = ""
syMA.commemoration03.narration.p01 = ""

syMA.commemoration04.text = "On this day we also commemorate the holy martyrs Phokas and Hermylus, who died by the sword."
syMA.commemoration04.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration04.couplet1.verse1 = ""
syMA.commemoration04.couplet1.verse2 = ""

syMA.commemoration05.text = "On this day we also commemorate the holy martyrs Zacchaeus the Deacon and Alfaeus the Reader, who contested in Caesarea."
syMA.commemoration05.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration05.couplet1.verse1 = ""
syMA.commemoration05.couplet1.verse2 = ""

syMA.commemoration06.text = "On this day we commemorate the holy miracle-worker Sophia. Also, the Consecration of the eminent Church of the Most Holy Theotokos in Chalkoprateia."

syMA.commemoration07.text = "We also commemorate our devout father Michael the Chancellor, the Confessor."
syMA.commemoration07.couplet1.title = titles_en_US_dedes.Stichoi
