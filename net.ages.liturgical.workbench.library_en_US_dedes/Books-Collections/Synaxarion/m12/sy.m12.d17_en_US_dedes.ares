alt1.syMA.commemoration01.text = "On December 17, we commemorate our father among the saints Dionysius the New, Archbishop of Aegina, the Wonder-worker, from the Island of Zakynthos."
alt1.syMA.commemoration01.couplet1.title = titles_en_US_dedes.ti.Verses
alt1.syMA.commemoration01.couplet1.verse1 = "Earth may have the body of Dionysius"
alt1.syMA.commemoration01.couplet1.verse2 = "the New, but heaven holds his soul."
alt1.syMA.commemoration01.single1 = "Famous Dionysius died on the seventeenth."

syMA.lastline = "By their holy intercessions, O God, have mercy on us and save us. Amen."
