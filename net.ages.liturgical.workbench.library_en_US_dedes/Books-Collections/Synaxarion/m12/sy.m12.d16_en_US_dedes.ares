A_Resource_Whose_Name = sy.m12.d16_en_US_dedes

syMA.commemoration01.text = "On December 16, we commemorate the holy and glorious Prophet Haggai."

syMA.commemoration02.text = "On this day we also commemorate the holy Martyr Marinos."

syMA.commemoration03.text = "On this day, Saints Probus (or Promus) and Hilarion died by fire."

syMA.commemoration04.text = "On this day we also commemorate our father among the saints Modestos, Archbishop of Jerusalem."

syMA.commemoration05.text = "We remember the consecration of the church of the holy Martyr Christopher, next to St. Polyeuctos."

syMA.commemoration06.text = "On this day we also commemorate the wonder-working Saint Theofano, the Empress, wife of Emperor Leo the Wise."

syMA.commemoration07.text = "We also commemorate our father among the saints Nicholas [Chrysovergis], Patriarch of Constantinople; and Memnon, Archbishop of Ephesus."

syMA.commemoration08.text = ""

syMA.commemoration09.text = ""

syMA.commemoration10.text = ""

syMA.lastline = "By their holy intercessions, O God, have mercy on us and save us. Amen."

