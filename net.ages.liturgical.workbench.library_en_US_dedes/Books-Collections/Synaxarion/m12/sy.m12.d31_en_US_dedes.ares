A_Resource_Whose_Name = sy.m12.d31_en_US_dedes

syMA.commemoration01.text = "On December 31, we commemorate our devout mother Melania the Roman."

syMA.commemoration02.text = "On this day we also commemorate the Martyr and Priest Zotikos who cared for orphans."

syMA.commemoration03.text = "On this day St. Gelasios reposed in peace."

syMA.commemoration04.text = "On this day the holy 10 Virgins in Nikomedia died after their eyes were pierced and the sides were scraped."

syMA.commemoration05.text = "On this day the holy Martyr Olympiodora died by fire."

syMA.commemoration06.text = "On this day St. Gaios reposed in peace."

syMA.commemoration07.text = "The holy Martyrs Bousiris, Gaudentios and Neme died by the sword."

syMA.commemoration08.text = ""

syMA.commemoration09.text = ""

syMA.commemoration10.text = ""

syMA.lastline = "By the intercessions of Your Saints, O God, have mercy on us and save us. Amen."

