syMA.commemoration02.text = "On this day we also commemorate the holy Martyrs in Martyropolis; and also devout Saint Marouthas, who rebuilt the city and named it after the Martyrs."
syMA.commemoration02.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration02.couplet1.verse1 = ""
syMA.commemoration02.couplet1.verse2 = ""
