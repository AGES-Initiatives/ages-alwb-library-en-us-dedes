syMA.commemoration01.text = "On April 13, we commemorate our father among the saints Martin the Confessor, Pope of Rome."

syMA.commemoration02.text = "On this day we also commemorate the holy martyrs Maximus, Quintilian, and Dadas."

syMA.commemoration03.text = "On this day we also commemorate the holy martyr Eleftherios the Persian."

syMA.commemoration04.text = "On this day we also commemorate the holy martyr Theodosios."

syMA.commemoration05.text = "On this day we also commemorate the holy martyr Zoilos."

