syMA.commemoration03.couplet2.verse2 = ""

syMA.commemoration04.text = "On this day we also commemorate our father among the saints Maximian, Patriarch of Constantinople."
syMA.commemoration04.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration04.couplet1.verse1 = ""
syMA.commemoration04.couplet1.verse2 = ""
syMA.commemoration04.narration.p01 = ""

syMA.commemoration05.text = "On this day we also commemorate our devout father Anastasios of Sinai."
