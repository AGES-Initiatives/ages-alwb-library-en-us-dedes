S01.commemoration.text = "On April 20, we commemorate our devout father Theodore, called \"Trichinas,\" (which means he wore a hair-shirt)."
S01.narration1.p01.text = "[paragraph 01]"
S01.narration1.p02.text = "[paragraph 02]"
S01.narration1.p03.text = "[paragraph 03]"
S01.narration1.p04.text = "[paragraph 04]"
S01.narration1.p05.text = "[paragraph 05]"
S01.narration1.p06.text = "[paragraph 06]"
S01.narration1.p07.text = "[paragraph 07]"

S02.commemoration.text = "On this day we also commemorate the holy Martyrs Victor, Zotikos, Zeno, Acindynos, Caesarius, Severian, Christopher, Theonas, and Antoninus."
S02.narration1.p01.text = "[paragraph 01]"
S02.narration1.p02.text = "[paragraph 02]"
S02.narration1.p03.text = "[paragraph 03]"
S02.narration1.p04.text = "[paragraph 04]"
S02.narration1.p05.text = "[paragraph 05]"
S02.narration1.p06.text = "[paragraph 06]"
S02.narration1.p07.text = "[paragraph 07]"

S03.commemoration.text = "On this day we also commemorate our devout father John Paleolavriotes."

S04.commemoration.text = "On this day we also commemorate the holy Hieromartyr Anastasios of Antioch."

S05.commemoration.text = "On this day we also commemorate the holy Apostle Zacchaeus."

S06.commemoration.text = "On this day, devout Saint Athanasios, who built the Monastery of Meteora in Thessaly around the year 1310, and who was originally from Patras, died in peace."

S07.commemoration.text = "Devout Saint Ioasaf, the fellow ascetic of Saint Athanasios of Meteora, died in peace."

S08.commemoration.text = ""

S09.commemoration.text = ""

S10.commemoration.text = ""

S99.lastline.text= "By the intercessions of Your saints, O Christ God, have mercy on us. Amen."

