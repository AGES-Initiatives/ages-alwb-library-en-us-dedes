syMA.commemoration01.text = "On April 10, we commemorate the holy martyrs Terence, Africanos, Maximos, Pompey, and another thirty-six; and also those with the blessed Zeno and Alexander and Theodore."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration01.couplet1.verse1 = ""
syMA.commemoration01.couplet1.verse2 = ""
