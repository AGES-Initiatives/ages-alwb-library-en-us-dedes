syMA.commemoration01.text = "On April 1 we commemorate our devout Mother Mary of Egypt."

syMA.commemoration02.text = "On this day we also commemorate our devout Father Makarios the Confessor, Abbot of the Hewn Monastery."

syMA.commemoration03.text = "On this day we also commemorate the holy martyrs Gerontios and Vasilides."

syMA.commemoration04.text = "On this day we also commemorate the holy and righteous Ahaz."

syMA.commemoration05.text = "We also commemorate our devout Russian father Efthymios of Suzdal, the wonder-worker."

syMA.commemoration06.text = ""

syMA.commemoration07.text = ""

syMA.commemoration08.text = ""

syMA.commemoration09.text = ""

syMA.commemoration10.text = ""

syMA.lastline = "By their holy intercessions, O God, have mercy on us. Amen."

