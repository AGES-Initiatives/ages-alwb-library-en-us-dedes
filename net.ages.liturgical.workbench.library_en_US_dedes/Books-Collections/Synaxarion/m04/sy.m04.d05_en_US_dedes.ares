syMA.commemoration01.text = "On April 5 we commemorate the holy martyrs Claudius, Diodoros, Victor, Victorinus, Pappias, Serapion, and Nikephoros."

syMA.commemoration02.text = "On this day we also commemorate our devout mother Theodora of Thessaloniki."

syMA.commemoration03.text = "On this day we also commemorate the holy martyrs Theodora and Didymos."

syMA.commemoration04.text = "On this day we also commemorate the holy martyr Thermos."

syMA.commemoration05.text = "On this day we also commemorate the holy women, a Lady and her Servant."

syMA.commemoration06.text = "On this day we also commemorate the holy martyr Pompey."

syMA.commemoration07.text = "On this day, Saint Zeno died after they smeared him with pitch, threw him into fire, and ran him through with a spear while he was in the fire."

syMA.commemoration08.text = "On this day we also commemorate the holy martyrs Maximus and Terence."

syMA.commemoration09.text = "On this day we also commemorate the holy five young women martyrs of Lesbos."

syMA.commemoration10.text = "On this day St. George the Neomartyr, who contested in New Ephesus in the year 1801, died by the sword."

syMA.lastline = "By the intercessions of Your saints, Christ our God, have mercy on us. Amen."

