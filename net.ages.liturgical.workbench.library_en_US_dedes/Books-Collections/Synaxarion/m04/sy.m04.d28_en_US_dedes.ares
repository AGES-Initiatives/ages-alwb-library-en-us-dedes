syMA.commemoration02.text = "On this day we also commemorate our devout father Memnon the wonder-worker."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration01.couplet1.verse1 = ""
syMA.commemoration01.couplet1.verse2 = ""
syMA.commemoration01.narration.p01 = ""

syMA.commemoration03.text = "Our devout father Afxivios died in peace."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration01.couplet1.verse1 = ""
syMA.commemoration01.couplet1.verse2 = ""

syMA.commemoration04.text = "Today a story is told about a miracle that took place in Africa, in the city of Carthage."
