syMA.commemoration01.text = "On May 26 we commemorate the holy and glorious Apostle Carpus, one of the Seventy Apostles."

syMA.commemoration02.text = "On this day the holy Apostle Alphaeus died in peace"

syMA.commemoration03.text = "Saint Abercius, son of the Apostle Alphaeus, died after he was laid naked on beehives and was stung by bees."

syMA.commemoration04.text = "Saint Helen, Abercius' sister, died by stoning."

syMA.commemoration05.text = "Saint Alexander the neo-martyr of Thessaloniki witnessed in Smyrna and died by the sword."

