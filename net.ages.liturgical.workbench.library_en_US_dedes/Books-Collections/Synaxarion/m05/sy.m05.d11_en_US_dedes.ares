A_Resource_Whose_Name = sy.m05.d11_en_US_dedes

syMA.commemoration01.text = "On May 11 we spiritually commemorate the Birthday, i.e. the Dedication, of the City of Constantinople, which was dedicated especially to our holy Lady the Theotokos."

syMA.commemoration02.text = "On this day we also commemorate the holy Hieromartyr Mokios."

syMA.commemoration03.text = "On this day we also commemorate the holy Martyr Dioskorus of Smyrna."

syMA.commemoration04.text = "The holy Neo-martyr Argyros of Thessaloniki died by the sword."

syMA.commemoration05.text = "We also commemorate our Fathers among the Saints Cyril (869) and Methodius (885) of Thessaloniki, Equals-of-the-Apostles, and illuminators of the Slavs."

syMA.commemoration06.text = ""

syMA.commemoration07.text = ""

syMA.commemoration08.text = ""

syMA.commemoration09.text = ""

syMA.commemoration10.text = ""

syMA.lastline = "By their holy intercessions, O God, have mercy on us. Amen."

