syMA.commemoration03.text = "On this day we also commemorate the holy Martyrs Epimachus and Gordian."
syMA.commemoration03.couplet1.title = "Στίχοι εἰς τὸν Ἅγιον Ἐπίμαχον"
syMA.commemoration03.couplet1.verse1 = ""
syMA.commemoration03.couplet1.verse2 = ""
syMA.commemoration03.couplet2.title = "Εἰς τὸν Ἅγιον Γορδιανόν"
syMA.commemoration03.couplet2.verse1 = ""
syMA.commemoration03.couplet2.verse2 = ""

syMA.commemoration04.text = "On this day the holy Martyrs Callinica and Aquilina, who believed in Christ through Saint Christopher, died: the first was pierced with a long spit from foot to shoulder, the second was hanged by her hair with a heavy stone tied to her feet."
syMA.commemoration04.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration04.couplet1.verse1 = ""
syMA.commemoration04.couplet1.verse2 = ""

syMA.commemoration05.text = "The holy monastic martyr Nicholas the New of Vounene, died by the sword."
syMA.commemoration05.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration05.couplet1.verse1 = ""
syMA.commemoration05.couplet1.verse2 = ""

syMA.lastline = "By their holy intercessions, O God, have mercy on us. Amen."

