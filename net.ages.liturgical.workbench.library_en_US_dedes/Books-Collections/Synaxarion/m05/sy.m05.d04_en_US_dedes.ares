A_Resource_Whose_Name = sy.m05.d04_en_US_dedes

syMA.commemoration01.text = "On May 4 we commemorate the holy Martyr Pelagia of Tarsus."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration01.couplet1.verse1 = ""
syMA.commemoration01.couplet1.verse2 = ""
syMA.commemoration01.single1 = ""
syMA.commemoration01.narration.p01 = ""

syMA.commemoration02.text = "On this day we also commemorate our devout Father Hilarion the Wonderworker."
syMA.commemoration02.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration02.couplet1.verse1 = ""
syMA.commemoration02.couplet1.verse2 = ""
syMA.commemoration02.narration.p01 = ""

syMA.commemoration03.text = "Our devout father Nikephoros, the Abbot of Medicium Monastery, reposed in peace."
syMA.commemoration03.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration03.couplet1.verse1 = ""
syMA.commemoration03.couplet1.verse2 = ""

syMA.commemoration04.text = "Devout Nikephoros, who lived in stillness in the wilderness of Athos, and who authored the wise method of noetic prayer, reposed in peace."
syMA.commemoration04.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration04.couplet1.verse1 = ""
syMA.commemoration04.couplet1.verse2 = ""

syMA.commemoration05.text = "We also commemorate our devout father Athanasios, Bishop of Corinth, who reposed in the year 957, during the reign of Emperors Basil the Bulgar-slayer and his brother Constantine."
syMA.commemoration05.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration05.couplet1.verse1 = ""
syMA.commemoration05.couplet1.verse2 = ""

syMA.commemoration06.text = "On this day is also the commemoration of the transfer of the relics of Saint Lazarus, the friend of Christ, and of Mary Magdalene the Myrrh-bearer, which took place during the reign Leo the Philosopher, in the year 890. The service in their honor is celebrated in the most holy Monastery founded by the same Emperor and named for St. Lazarus."
syMA.commemoration06.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration06.couplet1.verse1 = ""
syMA.commemoration06.couplet1.verse2 = ""

syMA.lastline = "By their holy intercessions, O God, have mercy on us. Amen."

