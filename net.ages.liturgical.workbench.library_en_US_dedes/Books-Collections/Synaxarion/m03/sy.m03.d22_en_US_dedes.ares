syMA.commemoration01.text = "On March 22 we commemorate the holy hieromartyr Basil, Presbyter of the Church of Ancyra."

syMA.commemoration02.text = "On this day we also commemorate the holy women martyrs, Kallinike and Vasilissa, who died by the sword."

syMA.commemoration03.text = "On this day we also commemorate St. Drosis, daughter of the emperor Trajan, and the five consecrated virgins with her."

syMA.commemoration04.text = "The holy new devout martyr Efthymios, who contested in Constantinople in the year 1814, died by the sword."

