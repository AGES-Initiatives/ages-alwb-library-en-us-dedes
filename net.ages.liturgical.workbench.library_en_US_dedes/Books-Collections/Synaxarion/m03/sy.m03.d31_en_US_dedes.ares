syMA.commemoration01.narration.p06 = ""

syMA.commemoration02.text = "On this day we also commemorate our devout father Akakios the Confessor, Bishop of Melitine."
syMA.commemoration02.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration02.couplet1.verse1 = ""
