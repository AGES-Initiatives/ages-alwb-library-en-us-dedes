A_Resource_Whose_Name = sy.m07.d02_en_US_dedes 

syMA.commemoration01.text = "On July 2 we commemorate the Placement of the Precious Garment of the Most Holy Theotokos in her holy shrine at Blachernae, during the reign of Emperor Leo the Great and his wife Berina."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration01.couplet1.verse1 = "Χιτὼν μὲν Υἱοῦ Χριστοφρουροῖς δημίοις."
syMA.commemoration01.couplet1.verse2 = "Ἐσθὴς δὲ Μητρὸς χριστοφρουρήτῳ πόλει."
syMA.commemoration01.single1 = "Δευτερίῃ κατέθεντο σορῷ Ἐσθῆτα Πανάγνου."
syMA.commemoration01.narration.p01 = ""

syMA.commemoration02.text = "On this day we also commemorate the contest of the holy Martyr Quintus. We also commemorate our Father among the Saints Juvenal, Archbishop of Jerusalem."
syMA.commemoration02.couplet1.title = titles_en_US_dedes.ti.Verses
syMA.commemoration02.couplet1.verse1 = ""
syMA.commemoration02.couplet1.verse2 = ""
syMA.commemoration02.couplet2.verse1 = ""
syMA.commemoration02.couplet2.verse2 = ""

syMA.lastline = "By their holy inercessions, O God, have mercy on us. Amen."

