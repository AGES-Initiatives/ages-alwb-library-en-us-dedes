A_Resource_Whose_Name = sy.m07.d14_en_US_dedes 

syMA.commemoration01.text = "On July 14, we commemorate the holy Apostle Aquila."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration01.couplet1.verse1 = "Ἥπλωσε Παῦλος, ὡς σαγήνην, τοὺς λόγους,"
syMA.commemoration01.couplet1.verse2 = "Ἤγρευσε δ' ὡς θήραμα θεῖον Ἀκύλαν."
syMA.commemoration01.single1 = "Ἀκύλαν δεκάτῃ γε τετάρτῃ τύμβος ἔκρυψεν."
syMA.commemoration01.narration.p01 = ""

syMA.commemoration02.text = "On this day we also commemorate the holy Martyr Justus."
syMA.commemoration02.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration02.couplet1.verse1 = "Μὴ θῦμα βαλεῖν εἰς τὸ βωμοῦ πῦρ θέλων,"
syMA.commemoration02.couplet1.verse2 = "Εἰς πῦρ Ἰοῦστος θῦμα βάλλεται ξένον."
syMA.commemoration02.narration.p01 = ""

syMA.commemoration03.text = "On this day we also commemorate our devout Father Onesimus the Wonderworker."
syMA.commemoration03.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration03.couplet1.verse1 = "Ἐλαφρὸν ἦρα φορτίον σὸν ἡδέως,"
syMA.commemoration03.couplet1.verse2 = "Μεθ' οὗ σὸς Ὀνήσιμος ἥκω σοι, Λόγε."

syMA.commemoration04.text = "On this day we also commemorate our Father among the Saints Joseph, Archbishop of Thessalonica."
syMA.commemoration04.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration04.couplet1.verse1 = "Ἀφείς, Ἰωσὴφ καθέδραν τὴν γηΐνην,"
syMA.commemoration04.couplet1.verse2 = "Παρίσταται νῦν τῷ θρόνῳ τοῦ Κυρίου."
syMA.commemoration04.narration.p01 = ""

syMA.commemoration05.text = "The holy Martyrs Aquila and Hilary died by stoning."
syMA.commemoration05.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration05.couplet1.verse1 = ""
syMA.commemoration05.couplet1.verse2 = ""

syMA.commemoration06.text = "The holy Martyr Peter the New died after his feet were cut off."
syMA.commemoration06.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration06.couplet1.verse1 = ""
syMA.commemoration06.couplet1.verse2 = ""

syMA.commemoration07.text = "The holy Martyr Heraclius died after he was beaten with clubs."
syMA.commemoration07.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration07.couplet1.verse1 = ""
syMA.commemoration07.couplet1.verse2 = ""

syMA.commemoration08.text = "On this day we also commemorate our devout Father Nikodemus of the Holy Mountain."
syMA.commemoration08.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration08.couplet1.verse1 = ""
syMA.commemoration08.couplet1.verse2 = ""

syMA.lastline = "By their holy intercessions, O God, have mercy on us. Amen."
