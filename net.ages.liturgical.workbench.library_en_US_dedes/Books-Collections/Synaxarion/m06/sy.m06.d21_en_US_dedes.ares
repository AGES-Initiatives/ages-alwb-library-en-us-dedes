syMA.commemoration01.text = "On the June 21 we commemorate the Holy Martyr Julian of Tarsus in Cilicia."

syMA.commemoration02.text = "On this day we commemorate the holy Martyr Aphrodisios."

syMA.commemoration03.text = "On this day we also commemorate the Holy Hieromartyr Terence, Bishop of Iconium."

syMA.commemoration04.text = "The holy New-Martyr Nikitas from Nisyros, who witnessed on Chios in the year 1732 died by the sword."

syMA.lastline = "By their holy intercessions, Christ God, have mercy on us. Amen."

