A_Resource_Whose_Name = sy.m10.d03_en_US_dedes

syMA.commemoration01.text = "On October 3 we commemorate the holy priest-Martyr Dionysius the Areopagite."

syMA.commemoration02.text = "On this day we also commemorate the holy Martyrs Roustikos and Eleftherios."

syMA.commemoration03.text = "On this day we also commemorate the holy Martyr Theoktistos."

syMA.commemoration04.text = "On this day we also commemorate the holy priest-Martyr Dionysius, Archbishop of Alexandria and the 8 Martyrs with him."

syMA.commemoration05.text = "On this day we also commemorate the holy Martyr Theagenos."

syMA.commemoration06.text = "On this day we also commemorate the holy Martyr Theoteknos."

syMA.commemoration07.text = "On this day we also commemorate the holy Martyr Adafktos."

syMA.commemoration08.text = "On this day we also commmemorate our devout Father St. John of Chozeva, Bishop of Caesarea."

syMA.lastline = "By the intercessions of Your Saints, O God, have mercy on us. Amen."

