syMA.commemoration01.text = "On October 23, we commemorate the holy Hieromartyr and Apostle James, the Brother of the Lord."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration01.couplet1.verse1 = "Κληθεὶς ἀδελφὸς τοῦ κατακρίτου ξύλῳ,"
syMA.commemoration01.couplet1.verse2 = "Θνῇσκεις δι̉ αὐτόν, παμμάκαρ, κρουσθεὶς ξύλῳ."
syMA.commemoration01.single1 = "Ἐσθλὸν Ἀδελφόθεον τριτάτῃ ξύλῳ εἰκάδι πλῆξαν."
syMA.commemoration01.narration.p01 = ""
syMA.commemoration01.narration.p02 = ""

syMA.commemoration02.text = "On this day we also commemorate our devout father Ignatius, Patriarch of Constantinople."
syMA.commemoration02.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration02.couplet1.verse1 = ""
syMA.commemoration02.couplet1.verse2 = ""
syMA.commemoration02.narration.p01 = ""

