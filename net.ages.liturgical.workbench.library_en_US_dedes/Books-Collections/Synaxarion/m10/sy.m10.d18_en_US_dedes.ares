A_Resource_Whose_Name = sy.m10.d18_en_US_dedes

syMA.commemoration01.text = "On October 18 we commemorate the holy Apostle and Evangelist Luke."
syMA.commemoration01.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration01.couplet1.verse1 = "Εἰς Ἐμμαοὺς βλέπειν σε κἂν πρὶν εἰργόμην,"
syMA.commemoration01.couplet1.verse2 = "(Λουκᾶς λέγει), τρανῶς σε νῦν, Χριστέ, βλέπω."
syMA.commemoration01.single1 = "Ὀγδοάτῃ δεκάτῃ πέρατος βίου ἔμμορε Λουκᾶς."
syMA.commemoration01.narration.p01 = ""
syMA.commemoration01.narration.p02 = ""
syMA.commemoration01.narration.p03 = ""

syMA.commemoration02.text = "On this day we also commemorate the holy Martyr Marinos the elder."
syMA.commemoration02.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration02.couplet1.verse1 = ""
syMA.commemoration02.couplet1.verse2 = ""

syMA.commemoration03.text = "On this day we also commemorate our devout father Julian of the Euphrates."
syMA.commemoration03.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration03.couplet1.verse1 = ""
syMA.commemoration03.couplet1.verse2 = ""
syMA.commemoration03.narration.p01 = ""
syMA.commemoration03.narration.p02 = ""
syMA.commemoration03.narration.p03 = ""
syMA.commemoration03.narration.p04 = ""

syMA.commemoration04.text = "On this day St. Mnason, Bishop of Cyprus, and the holy Forty Children died by the sword."
syMA.commemoration04.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration04.couplet1.verse1 = ""
syMA.commemoration04.couplet1.verse2 = ""
syMA.commemoration04.couplet2.verse1 = ""
syMA.commemoration04.couplet2.verse2 = ""

syMA.commemoration05.text = "Today we also commemorate the holy newly appeared Martyrs Gabriel and Kyrmidoles who contested in Egypt in the year 1522."
syMA.commemoration05.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration05.couplet1.verse1 = ""
syMA.commemoration05.couplet1.verse2 = ""

syMA.commemoration06.text = "We also commemorate our devout fathers Symeon and Theodore, and the devout woman Efrosyne, who found the holy icon of the Mother of God and started the Monastery of the Great Cave."
syMA.commemoration06.couplet1.title = titles_en_US_dedes.Stichoi
syMA.commemoration06.couplet1.verse1 = ""
syMA.commemoration06.couplet1.verse2 = ""

syMA.lastline = "By their holy intercessions, O God, have mercy on us. Amen."
